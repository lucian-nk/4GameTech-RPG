CMD:sleep(playerid, params[])
{

    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(HouseEntered[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You are not in your own/rented house.");
	if(HouseEntered[playerid] != PlayerInfo[playerid][pHouseKey] && HouseEntered[playerid] != PlayerInfo[playerid][pRentKey]) return SendClientMessage(playerid, COLOR_ERROR, "You are not in your own/rented house.");
	if(AFK[playerid] == 0)
	{
	    if(PlayerInfo[playerid][pWanted] > 0) return SendClientMessage(playerid, COLOR_ERROR, "You can't sleep while you have wanted.");

		AFK[playerid] = 1;
	    TogglePlayerControllableEx(playerid, 0);
	    SetPlayerColor(playerid, 0xCECECE00);
	    ApplyAnimation(playerid,"PED","KO_skid_front",4.1,0,1,1,1,0);
	    SendClientMessage(playerid, COLOR_SYN2, "You can leave your character AFK and when you come back type again /sleep.");
	    gQuery[0] = (EOS);
	    mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Status`='2' WHERE `Status`='1'");
		mysql_query(handle, gQuery);
	}
	else
	{
	    AFK[playerid] = 0;
    	TogglePlayerControllableEx(playerid, 1);
	    TeamColor(playerid);
	    SendClientMessage(playerid, COLOR_SYN2, "You can sleep again any time.");
	    gQuery[0] = (EOS);
	    mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Status`='1' WHERE `Status`='2'");
		mysql_query(handle, gQuery);
	}
    return 1;
}

CMD:housewithdraw(playerid, params[])
{
	new money;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pHouseKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You don't own a house.");
	new house = PlayerInfo[playerid][pHouseKey];
	if(sscanf(params,"i", money)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /housewithdraw <amount>"), format(gString, sizeof(gString), "You have $%s in house safe.", FormatNumber(HouseInfo[house][hSafe])), SendClientMessage(playerid, COLOR_GRAD3, gString);
	{
		if(HouseEntered[playerid] != house) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in your house.");
		if(money > HouseInfo[house][hSafe] || money < 1) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money in house safe.");
		GivePlayerCash(playerid,money);
		HouseInfo[house][hSafe]=HouseInfo[house][hSafe]-money;
		hUpdate(house, hSafex);
		format(gString, sizeof(gString), "  You have withdrawn $%s from your house safe, Total: $%s ", FormatNumber(money),FormatNumber(HouseInfo[house][hSafe]));
		SendClientMessage(playerid, COLOR_YELLOW, gString);
		PPWithdrawLog(playerid, house, money, 2);
	}
	return 1;
}

CMD:sellhouseto(playerid, params[])
{
	if ( GetPVarInt( playerid, "AccountLocked" ) == 1 )
	{
		if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		{
			SCM( playerid, COLOR_WHITE, "Your account is locked, use /unlockacc to unlock it." );
		}
		if(PlayerInfo[playerid][pLimba] == 2) // Romana
		{
			SCM( playerid, COLOR_WHITE, "Contul tau este blocat, deblocheaza-l folosind comanda /unlockacc." );
		}
	}
	new giveplayerid, splayer[25], price;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pHouseKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You don't own a house.");
	if(sscanf(params,"s[25]i", splayer,price)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /sellhouseto <Player ID/Name> <price>");
	{
		new house = PlayerInfo[playerid][pHouseKey];
		if(!IsPlayerInRangeOfPoint(playerid, 3.0, HouseInfo[house][hEntranceX], HouseInfo[house][hEntranceY], HouseInfo[house][hEntranceZ])) return SendClientMessage(playerid, COLOR_ERROR, "You need to be near your house.");
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(PlayerInfo[giveplayerid][pHouseKey] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Player already own a house.");
		if(MoneyACheck(playerid, price, 1, 1000000000)) return 1;
		if(price > GetPlayerCash(giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player don't have enough money.");
		if(PlayerInfo[giveplayerid][pLevel] < HouseInfo[house][hLevel]) return SendClientMessage(playerid, COLOR_ERROR, "Player level is not high enough.");

		HousePrice[giveplayerid][playerid] = price;
		format(gString, sizeof(gString), "You offered %s to buy your house for $%s.",GetName(giveplayerid),FormatNumber(price));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "House Owner %s ofered you to buy his house for $%s, type (/accept house %d) to accept.",GetName(playerid),FormatNumber(price),playerid);
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
	}
	return 1;
}

CMD:asellhouse(playerid, params[])
{
	new house;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 8) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"i",house)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /asellhouse <houseid>");
	{
	    if(strlen(HouseInfo[house][hOwner]) < 2) return SendClientMessage(playerid, COLOR_ERROR, "Invalid house id.");
		if(GetPlayerID(HouseInfo[house][hOwner]) != INVALID_PLAYER_ID)
		{
			PlayerInfo[GetPlayerID(HouseInfo[house][hOwner])][pHouseKey] = 0;
			Update(GetPlayerID(HouseInfo[house][hOwner]), pHouseKeyx);
		}
		else
		{
			gQuery[0] = (EOS);
			mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `HouseKey`='0' WHERE `HouseKey`=%d", house);
			mysql_query(handle, gQuery);
		}

		format(gString,sizeof(gString),"You sold the house %d, owned by %s.",house,HouseInfo[house][hOwner]);
		format(gString, sizeof(gString), "%s sold house %d, owned by %s",GetName(playerid), house,HouseInfo[house][hOwner]);
		CMDRaport(gString, 5, 0);

		HouseInfo[house][hLock] = 1;
		hUpdate(house, hLockx);
		HouseInfo[house][hOwned] = 0;
		hUpdate(house, hOwnedx);
		HouseInfo[house][hSafe] = 0;
		hUpdate(house, hSafex);
		strmid(HouseInfo[house][hOwner], "The State", 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		hUpdate(house, hOwnerx);
		PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		GameTextForPlayer(playerid, gString, 5000, 3);
		
		for(new i = 1; i <= TotalClans; i++)
		{
		    if(ClanInfo[i][cHouseHQ] == house)
		    {
	   			ClanInfo[i][cHQType] = 0;
			    cUpdate(i, cHQTypex);
			    ClanInfo[i][cHouseHQ] = 0;
			    cUpdate(i, cHouseHQx);
		    }
		}
		HouseUpdate(house);
	}
	return 1;
}

