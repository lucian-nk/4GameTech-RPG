			SellGunWeapon[playerid][giveplayerid] = 0;
			SellGunAmmo[playerid][giveplayerid] = 0;
			SellGunMats[playerid][giveplayerid] = 0;
		}
		else return SendClientMessage(playerid, COLOR_ERROR, "Unknown name.");
	}
	return 1;
}

