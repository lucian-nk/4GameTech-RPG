CMD:materials(playerid, params[])
{
	new act[24], amount;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pJob] != 9) return SendClientMessage(playerid, COLOR_ERROR, "You don't have arms dealer job.");
	if(sscanf(params,"s[24]I(0)", act, amount)) { SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /materials <name>"); SendClientMessage(playerid, -1, "Available names: Get Deliver"); return 1; }
	{
		if(strmatch(act,"get"))
		{
		    if(amount == 0) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /materials get <amount>");
			if(!IsPlayerInRangeOfPoint(playerid,2.0,2770.5027,-1628.2980,12.1775)) return SendClientMessage(playerid, COLOR_ERROR, "You are not at the Materials Package Building in Los Santos. (/locations)");
            if(AmountCheck(playerid, amount, 1, 10)) return 1;
			if(GetPlayerCash(playerid) < 500*amount) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money.");
			if(MatsHolding[playerid] >= 10) return SendClientMessage(playerid, COLOR_ERROR, "You already have 10 packets of materials.");
			format(gString, sizeof(gString), "You took %d packets of materials for $%s. Go to the checkpoint and process it.", amount, FormatNumber(500*amount));
			SendClientMessage(playerid, COLOR_SYN2, gString);
            UseFind[playerid] = 0;
            IsTowing[playerid] = 0;
			SetPlayerCheckpoint(playerid, 811.6922, -564.3546, 16.3359, 2.0);
			CP[playerid] = 1995;
			GivePlayerCash(playerid, -500*amount);
			MatsHolding[playerid] += amount;
		}
		else if(strmatch(act,"deliver"))
		{
			if(!IsPlayerInRangeOfPoint(playerid,2.0,811.6922,-564.3546,16.3359)) return SendClientMessage(playerid, COLOR_ERROR, "You are not at the Materials Process Building in Los Santos.");
			if(MatsHolding[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You don't have any packet of materials.");
			format(gString, sizeof(gString), "You have got %s Materials for the %d packages.", FormatNumber(100*MatsHolding[playerid]), MatsHolding[playerid]);
			SendClientMessage(playerid, COLOR_SYN2, gString);
			PlayerInfo[playerid][pMats] += 100*MatsHolding[playerid];
			Update(playerid, pMatsx);
			MatsHolding[playerid] = 0;
		}
	}
	return 1;
}

