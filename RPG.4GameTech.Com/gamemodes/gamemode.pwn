            GivePlayerWeaponEx(i, pgunp, 9999);
	        TogglePlayerControllableEx(i, 1);
	        SendClientMessage(i, COLOR_YELLOW, "Match started, 4 minutes left.");
	    }
	}
	PaintballRound = 1;
	StartingPaintballRound = 0;
	PaintballWinner = 999;
	PaintballWinnerScore = 0;
	PaintballPlayers = 0;
	PaintBallTimer = 240;
 	painttimer = SetTimer("PaintballEnded", PaintBallTimer*1000, false);
	return 1;
}

function PaintballEnded()
{
    KillTimer(painttimer);
    foreach(Player,i)
	{
        if(PlayerPaintballing[i] != 0)
        {
            if(!IsPlayerConnected(PaintballWinner))
            {
                PaintballWinner = 999;
				PaintballWinnerScore = 0;
                foreach(Player,j)
				{
			        if(PlayerPaintballing[j] != 0)
			        {
						if(PlayerPaintballScore[j] > PaintballWinnerScore) PaintballWinner = j;
			        }
				}
            }
            
			if(IsPlayerConnected(PaintballWinner))
			{
				format(gString,sizeof(gString), "** %s won the Paintball Match with %d kills **",GetName(PaintballWinner),PaintballWinnerScore);
				SendClientMessage(i, COLOR_WHITE, gString);
				format(gString,sizeof(gString),"** Your score is %d and you have %d kills and %d deaths **",(KillP[i]-DeathP[i]),KillP[i],DeathP[i]);
				SendClientMessage(i, COLOR_WHITE, gString);
				
				if(i == PaintballWinner)
				{
					new chance = random(10);
					if(chance == 5)
					{
						SendClientMessage(PaintballWinner, COLOR_WHITE, "You win 2 respect points at paintball match.");
						PlayerInfo[PaintballWinner][pRespect] += 2;
						Update(PaintballWinner, pRespectx);
					}
					else
					{
						SendClientMessage(PaintballWinner, COLOR_WHITE, "You win $20,000 at paintball match.");
					    GivePlayerCash(PaintballWinner, 20000);
					}
					GiveAchievement(PaintballWinner, 23, "Win a paintball match", 100);
				}
			}
            ResetPlayerWeapons(i);
            SetPlayerArmourEx(i, 0);
			SetPlayerHealthEx(i, 100);
            PlayerPaintballing[i] = 0;
            PlayerPaintballScore[i] = 0;
            SetPlayerPos(i, BizzInfo[19][bEntranceX],BizzInfo[19][bEntranceY],BizzInfo[19][bEntranceZ]);
            SetPlayerInterior(i,0);
			SetPlayerVirtualWorld(i,0);
			KillP[i] = 0;
			DeathP[i] = 0;
			for(new m = 0; m < 5; m++) SendDeathMessageToPlayer(i, 1001, 1001, 200);
	    }
	}
    PaintballRound = 0;
	PaintballWinner = 999;
	PaintballWinnerScore = 0;
	StartingPaintballRound = 0;
	return 1;
}

