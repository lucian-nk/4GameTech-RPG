CMD:service(playerid, params[])
{
	new service[64];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(sscanf(params,"s[64]", service)) { SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /service <name>"); SendClientMessage(playerid, -1, "Available names: Taxi"); return 1; }
	{
		if(strmatch(service, "taxi"))
		{
			if(TaxiDrivers < 1)
			{
				SendClientMessage(playerid, COLOR_ERROR, "There are no Taxi Drivers On Duty at the moment, try again later.");
				return 1;
			}
			if(TaxiDuty[playerid] == 1)
			{
				SendClientMessage(playerid, COLOR_ERROR, "You can't call for a Taxi now.");
				return 1;
			}
			if(NeedTaxi[playerid] == 1)
			{
				SendClientMessage(playerid, COLOR_ERROR, "You already called for a Taxi.");
				return 1;
			}
      		foreach(new i : Player)
			{
				if(TaxiAccepted[i] == playerid)
				{
					format(gString, sizeof(gString), "%s already accepted your Taxi Call.", GetName(i), playerid);
					SendClientMessage(playerid, COLOR_ERROR, gString);
					return 1;
				}
			}
			format(gString, sizeof(gString), "%s is in need of a Taxi Driver, type (/accept taxi %d) to accept.", GetName(playerid), playerid);
			SendFactionMessage(5, COLOR_SYN2, gString);
			SendClientMessage(playerid, COLOR_SYN2, "You have called for a Taxi Driver, wait for a reply.");
			NeedTaxi[playerid] = 1;
		}
  		else if(strmatch(service, "mechanic"))
		{
		    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_ERROR, "You need to be driver of a vehicle.");
			if(Mechanics < 1)
			{
				SendClientMessage(playerid, COLOR_ERROR, "There are no Mechanics On Duty at the moment, try again later.");
				return 1;
			}
			if(MechanicDuty[playerid] == 1)
			{
				SendClientMessage(playerid, COLOR_ERROR, "You can't call for a Mechanic now.");
				return 1;
			}
			if(NeedMechanic[playerid] == 1)
			{
				SendClientMessage(playerid, COLOR_ERROR, "You already called for a Mechanic.");
				return 1;
			}
      		foreach(new i : Player)
			{
				if(MechanicAccepted[i] == playerid)
				{
					format(gString, sizeof(gString), "%s already accepted your Mechanic Call.", GetName(i), playerid);
					SendClientMessage(playerid, COLOR_ERROR, gString);
					return 1;
				}
			}
			format(gString, sizeof(gString), "%s is in need of a Mechanic, type (/accept mechanic %d) to accept.", GetName(playerid), playerid);
			foreach(new i : Player) if(MechanicDuty[i] == 1) SendClientMessage(i, COLOR_SYN2, gString);
			SendClientMessage(playerid, COLOR_SYN2, "You have called for a Mechanic, wait for a reply.");
			NeedMechanic[playerid] = 1;
		}
	}
	return 1;
}

