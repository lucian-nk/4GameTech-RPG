CMD:sellvehto(playerid, params[])
{
	new giveplayerid, splayer[25], price;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(sscanf(params,"s[25]i", splayer,price)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /sellvehto <Player ID/Name> <price>");
	{
		if(!IsPlayerInAnyVehicle(playerid) || !strmatch(vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vOwner], GetName(playerid)) || OwnedVeh(GetPlayerVehicleID(playerid)) == 0 || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in your vehicle.");
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(MoneyACheck(playerid, price, 1, 1000000000)) return 1;
		if(price > GetPlayerCash(giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player don't have enough money.");
		if(PlayerInfo[giveplayerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_ERROR, "Player don't have level 3.");
		if(vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vType] == 1) return SendClientMessage(playerid, COLOR_ERROR, "You can't sell this vehicle.");

		new veh = GetPlayerVehicleID(playerid);
		new model = GetVehicleModel(veh);
		if(GetVehNumber(giveplayerid) >= PlayerInfo[giveplayerid][pVehicleSlots]) return SendClientMessage(playerid, COLOR_ERROR, "Player has vehicle slots full.");
  		if(GetModelNumber(giveplayerid, model) > 0 && PlayerInfo[giveplayerid][pPremiumAccount] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Player already has a model of this vehicle.");
		else if(GetModelNumber(giveplayerid, model) > 1 && PlayerInfo[giveplayerid][pPremiumAccount] == 1)  return SendClientMessage(playerid, COLOR_ERROR, "Player already has two models of this vehicle.");
		VehiclePrice[giveplayerid][playerid] = price;
		format(gString, sizeof(gString), "You offered %s to buy your vehicle(%s) for $%s.",GetName(giveplayerid),VehicleNames[GetVehicleModel(GetPlayerVehicleID(playerid)) - 400],FormatNumber(price));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Vehicle Owner %s ofered you to buy his vehicle(%s) for $%s, type (/accept vehicle %d) to accept.",GetName(playerid),VehicleNames[GetVehicleModel(GetPlayerVehicleID(playerid)) - 400],FormatNumber(price),playerid);
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
	}
	return 1;
}

