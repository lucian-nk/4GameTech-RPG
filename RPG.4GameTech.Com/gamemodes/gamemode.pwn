function DestroyWarPickups(war)
{
	for(new i = 0; i <= CountDynamicPickups(); i++)
	{
		if(IsValidDynamicPickup(i) && war == 1)
		{
		    if(PickupHealth1[i] || PickupWeapon1[i])
		    {
			    DestroyDynamicPickup(i);
			    PickupHealth1[i] = 0;
				PickupWeapon1[i] = 0;
			}
		}
		if(IsValidDynamicPickup(i) && war == 2)
		{
		    if(PickupHealth2[i] || PickupWeapon2[i])
		    {
			    DestroyDynamicPickup(i);
			    PickupHealth2[i] = 0;
				PickupWeapon2[i] = 0;
			}
		}
	}
}

function FindFreeReport()
{
	for(new i = 0; i < sizeof(ReportInfo); i++) if(ReportInfo[i][rPlayer] == INVALID_PLAYER_ID) return i;
	return -1;
}

function HaveReport(playerid)
{
	for(new i = 0; i < sizeof(ReportInfo); i++) if(ReportInfo[i][rPlayer] == playerid) return 1;
	return 0;
}

function DeleteReport(playerid)
{
    for(new i = 0; i < sizeof(ReportInfo); i++) if(ReportInfo[i][rPlayer] == playerid) ResetReport(i);
}

function ReportID(playerid)
{
    for(new i = 0; i < sizeof(ReportInfo); i++) if(ReportInfo[i][rPlayer] == playerid) return i;
    return -1;
}

function ResetReport(reportid)
{
	strmid(ReportInfo[reportid][rReason], " ", 0, 128, 128);
	ReportInfo[reportid][rPlayer] = INVALID_PLAYER_ID;
	ReportInfo[reportid][rTime] = 0;
}

function FindFreeHelp()
{
	for(new i = 0; i < sizeof(HelpInfo); i++) if(HelpInfo[i][hPlayer] == INVALID_PLAYER_ID) return i;
	return -1;
}

function HaveHelp(playerid)
{
	for(new i = 0; i < sizeof(HelpInfo); i++) if(HelpInfo[i][hPlayer] == playerid) return 1;
	return 0;
}

function DeleteHelp(playerid)
{
    for(new i = 0; i < sizeof(HelpInfo); i++) if(HelpInfo[i][hPlayer] == playerid) ResetHelp(i);
}

function HelpID(playerid)
{
    for(new i = 0; i < sizeof(HelpInfo); i++) if(HelpInfo[i][hPlayer] == playerid) return i;
    return -1;
}

function ResetHelp(helpid)
{
	strmid(HelpInfo[helpid][hReason], " ", 0, 128, 128);
	HelpInfo[helpid][hPlayer] = INVALID_PLAYER_ID;
	HelpInfo[helpid][hTime] = 0;
}

function TogglePlayerControllableEx(playerid, stat)
{
    if(stat == 0) PlayerFreezed[playerid] = true;
    else PlayerFreezed[playerid] = false;
    TogglePlayerControllable(playerid, stat);
    return 1;
}

function PrepareClanWar()
{
	if(ClanWarP1 < 2 || ClanWarP2 < 2)
	{
		foreach(new i : Player) if(InClanWar[i])
		{
			SendClientMessage(i, COLOR_YELLOW, "There are not enough members to start the war.");
			InClanWar[i] = 0;
			KillCW[i] = 0;
			DeathCW[i] = 0;
            SetPlayerInterior(i, 0);
			SetPlayerPos(i, 994.6793, -1209.2073, 16.9375);
			TogglePlayerControllableEx(i, 1);
		}
		ClanWarT1 = 0;
		ClanWarT2 = 0;
		
		ClanWarS1 = 0;
		ClanWarS2 = 0;
		
		ClanWarP1 = 0;
		ClanWarP2 = 0;
	}
	else
	{
	    foreach(new i : Player) if(InClanWar[i]) SendClientMessage(i, COLOR_YELLOW, "The war will start in 10 seconds.");
	    SetTimer("StartClanWar", 10000, 0);
	}
}

function StartClanWar()
{
	ClanWarStarted = 1;
	format(gString, sizeof(gString), "NR Bot: War between clan %s and %s have been started in Clan War Arena.",ClanInfo[ClanWarT1][cName], ClanInfo[ClanWarT2][cName]);
	SendClientMessageToAll(COLOR_NEWS, gString);
    foreach(new i : Player) if(InClanWar[i])
    {
        format(gString,sizeof(gString),"Clan NEWS: War between clan %s and %s have been started.", ClanInfo[ClanWarT1][cName], ClanInfo[ClanWarT2][cName]);
		SendClientMessage(i, COLOR_CC, gString);
		
        TogglePlayerControllableEx(i, 1);
        ResetPlayerWeapons(i);
		SetPlayerArmourEx(i, 0);
		SetPlayerHealthEx(i, 100);
        GivePlayerWeaponEx(i, 24, 99999);
		GivePlayerWeaponEx(i, 31, 99999);
		GivePlayerWeaponEx(i, 32, 99999);
		GivePlayerWeaponEx(i, 33, 99999);
		ClanWarTimer = 600;
    }
}

function StopClanWar()
{
	ClanWarStarted = 0;
    foreach(new i : Player) if(InClanWar[i])
    {
        if(ClanWarS1 > ClanWarS2) format(gString,sizeof(gString),"Clan NEWS: Clan %s won the war against clan %s. Score: %d - %d", ClanInfo[ClanWarT1][cName], ClanInfo[ClanWarT2][cName], ClanWarS1, ClanWarS2, ClanInfo[ClanWarT1][cWWars]++, cUpdate(ClanWarT1, cWWarsx));
		else if(ClanWarS1 < ClanWarS2) format(gString,sizeof(gString),"Clan NEWS: Clan %s won the war against clan %s. Score: %d - %d", ClanInfo[ClanWarT2][cName], ClanInfo[ClanWarT1][cName], ClanWarS2, ClanWarS1, ClanInfo[ClanWarT2][cWWars]++, cUpdate(ClanWarT2, cWWarsx));
		else if(ClanWarS1 == ClanWarS2) format(gString,sizeof(gString),"Clan NEWS: War between clan %s and %s finished at deuce.", ClanInfo[ClanWarT1][cName], ClanInfo[ClanWarT2][cName]);
		SendClientMessage(i, COLOR_CC, gString);
		format(gString,sizeof(gString),"Your score is %d and you have %d kills and %d deaths.",(KillCW[i]-DeathCW[i]),KillCW[i],DeathCW[i]);
		SendClientMessage(i, COLOR_CC, gString);
		
		new clan = PlayerInfo[i][pCLeader] + PlayerInfo[i][pCMember];
		if(ClanWarS1 > ClanWarS2 && clan == ClanWarT1)
		{
			GivePlayerCash(i, 5000);
			PlayerInfo[i][pDrugs] += 2;
			Update(i, pDrugsx);
			PlayerInfo[i][pMats] += 200;
			Update(i, pMatsx);
		}
		if(ClanWarS1 < ClanWarS2 && clan == ClanWarT2)
		{
			GivePlayerCash(i, 5000);
			PlayerInfo[i][pDrugs] += 2;
			Update(i, pDrugsx);
			PlayerInfo[i][pMats] += 200;
			Update(i, pMatsx);
		}
        ResetPlayerWeapons(i);
		SetPlayerArmourEx(i, 0);
		SetPlayerHealthEx(i, 100);
		
        InClanWar[i] = 0;
		KillCW[i] = 0;
		DeathCW[i] = 0;
        SetPlayerInterior(i, 0);
		SetPlayerPos(i, 994.6793, -1209.2073, 16.9375);
		TogglePlayerControllableEx(i, 1);
		
		for(new m = 0; m < 5; m++) SendDeathMessageToPlayer(i, 1001, 1001, 200);
    }
    ClanWarPause[ClanWarT1] = gettime() + 120;
    ClanWarPause[ClanWarT2] = gettime() + 120;
    ClanWarT1 = 0;
	ClanWarT2 = 0;

	ClanWarS1 = 0;
	ClanWarS2 = 0;

	ClanWarP1 = 0;
	ClanWarP2 = 0;
}

