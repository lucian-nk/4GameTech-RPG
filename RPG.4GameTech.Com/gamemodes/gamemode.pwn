	new found = -1;
 	foreach(new i : Player)
	{
		if(PlayerInfo[i][pPhoneNumber] == tnum && tnum != 0)
		{
			found = i;
		}
	}
	if(found >= 0)
	{
		if(OnOff[found] == 0 || SpectatorID[found] != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_ERROR, "Player's cellphone is turned off.");
		if(GetPlayerCash(playerid) < 10) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money.");
		
		format(gString, sizeof(gString), "SMS: %s, From: %s (%s)", smstext,GetName(playerid),PhoneFormat(sender));
