CMD:findbiz(playerid, params[])
{
	if(!gLogged[playerid]) 
		return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
		
	new id;
	
	if(sscanf(params, "i", id))
		return SendClientMessage(playerid, COLOR_SYN,"Synthax:{FFFFFF} /findbiz <business id>");
		
	if(id < 1 || id > businesses_count)
		return SendClientMessage(playerid, COLOR_ERROR, "Invalid business id.");
		
	SetPlayerCheckpoint(playerid, BizzInfo[id][bEntranceX], BizzInfo[id][bEntranceY], BizzInfo[id][bEntranceZ], 5.0);
	CP[playerid] = 1;
	
	SendClientMessage(playerid, COLOR_SYN2, "A checkpoint has been set on your map.");
	
	return 1;
}

CMD:findhouse(playerid, params[])
{
	if(!gLogged[playerid]) 
		return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
		
	new id;
	
	if(sscanf(params, "i", id))
		return SendClientMessage(playerid, COLOR_SYN,"Synthax:{FFFFFF} /findhouse <house id>");
		
	if(id < 1 || id > sizeof(HouseInfo) - 1)
		return SendClientMessage(playerid, COLOR_ERROR, "Invalid house id.");
		
	SetPlayerCheckpoint(playerid, HouseInfo[id][hEntranceX], HouseInfo[id][hEntranceY], HouseInfo[id][hEntranceZ], 5.0);
	CP[playerid] = 1;
	
	SendClientMessage(playerid, COLOR_SYN2, "A checkpoint has been set on your map.");
	
	return 1;
}

