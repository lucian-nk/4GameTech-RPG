CMD:changenick(playerid, params[])
{
	new giveplayerid, splayer[25], name[25], nwt[25];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 10) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[25]s[25]", splayer, name)) return SendClientMessage(playerid, COLOR_SYN,"Synthax:{FFFFFF} /changenick <Player ID/Name> <name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		strmid(nwt, name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		new clan = PlayerInfo[giveplayerid][pCMember] + PlayerInfo[giveplayerid][pCLeader];
		if(clan != 0)
		{
			if(ClanInfo[clan][cTagType] == 0) strins(name, ClanInfo[clan][cTag], 0);
			else strins(name, ClanInfo[clan][cTag], strlen(name));
			if(strlen(name) > 20) return SendClientMessage(playerid, COLOR_ERROR, "Name is to long with tag.");
		}
		if(strlen(name) > 20) return SendClientMessage(playerid, COLOR_ERROR, "Name is to long.");
		if(ExistName(name)) return SendClientMessage(playerid, COLOR_ERROR, "Name already exist.");
		format(gString, sizeof(gString), "%s changed %s's name to %s.",GetName(playerid), GetName(giveplayerid), name);
		CMDRaport(gString, 6, 0);
		
		if(PlayerInfo[giveplayerid][pHouseKey] > 0)
		{
   			strmid(HouseInfo[PlayerInfo[giveplayerid][pHouseKey]][hOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			hUpdate(PlayerInfo[giveplayerid][pHouseKey], hOwnerx);
			HouseUpdate(PlayerInfo[giveplayerid][pHouseKey]);
		}
		
		if(PlayerInfo[giveplayerid][pBizzKey] > 0)
		{
			strmid(BizzInfo[PlayerInfo[giveplayerid][pBizzKey]][bOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			bUpdate(PlayerInfo[giveplayerid][pBizzKey], bOwnerx);
			BizzUpdate(PlayerInfo[giveplayerid][pBizzKey]);
		}
		
		for(new i = 1; i <= Total_Veh_Created; i++)
		{
			if(strmatch(vInfo[i][vOwner], GetName(giveplayerid)))
			{
				strmid(vInfo[i][vOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
				vUpdate(i, vOwnerx);
			}
		}
		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Name`='%e' WHERE `ID`=%d", name, PlayerInfo[giveplayerid][pSQLID]);
		mysql_query(handle, gQuery);
		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `AName`='%e' WHERE `ID`=%d", nwt, PlayerInfo[giveplayerid][pSQLID]);
		mysql_query(handle, gQuery);
		SetPlayerName(giveplayerid, name);
		TextDrawSetString(NameText[giveplayerid], GetName(giveplayerid));
	}
	return 1;
}

