		}
		return 1;
	}
	if(sscanf(params,"i",price)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /fare <price>");
	{
		new vid = GetPlayerVehicleID(playerid);
		if(MoneyACheck(playerid, price, 1, 200)) return 1;
