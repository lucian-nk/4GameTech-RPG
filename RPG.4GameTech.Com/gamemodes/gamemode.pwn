function ClockUpdate()
{
	new hour, minutes, seconds, day, month, year;
	gettime(hour, minutes, seconds);
	getdate(year, month, day);
	format(gString,128,"%02d:%02d", hour, minutes);
	TextDrawSetString(ClockTime, gString);
	format(gString,128,"%02d/%02d/%d", day, month, year);
	TextDrawSetString(ClockDate, gString);
	if(minutes == 0 && LastPayDayH != hour)
	{
	    if(hour == 22 && strmatch(GetWeekDay(day, month, year), "Friday"))
	    {
			new aux, aux2, Clan[100], bool: cont = true, Wars[100];
			for(new i = 1; i <= TotalClans; i++) Wars[i] = ClanInfo[i][cWWars], Clan[i] = ClanInfo[i][cID];
		 	while (cont)
		    {
		    	cont = false;
				for(new i = 1; i <= TotalClans; i++)
				{
				    for(new j = i; j <= TotalClans; j++) if(Wars[i] < Wars[j]) aux = Wars[i], Wars[i] = Wars[j] , Wars[j] = aux, aux2 = Clan[i], Clan[i] = Clan[j], Clan[j] = aux2, cont = true;
			 	}
		 	}
    		ClanInfo[Clan[1]][cBest] = 1;
    		cUpdate(Clan[1], cBestx);
			format(gString, sizeof(gString), "NR Bot: Clan %s is the winner of the Clan turf this week with %d wars won. Congratulations!",ClanInfo[Clan[1]][cName], ClanInfo[Clan[1]][cWWars]);
			SendClientMessageToAll(COLOR_NEWS, gString);
	    }
	    if(hour == 24 && HappyDay == 1)
	    {
	        HappyDay = 0;
	    	switch(random(5))
	    	{
		        case 0: HappyDayHour = 18;
		        case 1: HappyDayHour = 19;
		        case 2: HappyDayHour = 20;
		        case 3: HappyDayHour = 21;
		        case 4:	HappyDayHour = 22;
	        }
	    }
	    ActualizareVreme(); // la pornirea gamemodeului
		PayDay();
		LastPayDayH = hour;
	}
}

