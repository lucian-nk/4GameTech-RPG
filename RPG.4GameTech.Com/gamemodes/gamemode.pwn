	{
		for(new i = 1; i < sizeof(HouseInfo); i++)
		{
			if (IsPlayerInRangeOfPoint(playerid, 1.5, HouseInfo[i][hEntranceX], HouseInfo[i][hEntranceY], HouseInfo[i][hEntranceZ]))
			{
				if(strmatch(edits,"level"))
				{
					HouseInfo[i][hLevel] = amount;
					hUpdate(i, hLevelx);
					HouseUpdate(i);
					format(gString,sizeof(gString),"Level of house %d changed to %d.",i,amount);
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"price"))
				{
					HouseInfo[i][hValue] = amount;
					hUpdate(i, hValuex);
					HouseUpdate(i);
					format(gString,sizeof(gString),"Price of house %d changed to $%s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"rentable"))
				{
					HouseInfo[i][hRentable] = amount;
					hUpdate(i, hRentablex);
					HouseUpdate(i);
					format(gString,sizeof(gString),"Rentable status of house %d changed to %s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
    			else if(strmatch(edits,"rent"))
				{
					HouseInfo[i][hRent] = amount;
					hUpdate(i, hRentx);
					HouseUpdate(i);
					format(gString,sizeof(gString),"Rent price of house %d changed to $%s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
    			else if(strmatch(edits,"owned"))
				{
					HouseInfo[i][hOwned] = amount;
					hUpdate(i, hOwnedx);
					HouseUpdate(i);
					format(gString,sizeof(gString),"Owned status of house %d changed to %s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"lock"))
				{
				    HouseInfo[i][hLock] = amount;
					hUpdate(i, hLockx);
					HouseUpdate(i);
					format(gString,sizeof(gString),"Lock of house %d changed to %s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else return 1;
				format(gString, sizeof(gString), "%s edited house %d, stat: %s, amount: %s.",GetName(playerid), i, edits, FormatNumber(amount));
				CMDRaport(gString, 6, 0);
			}
		}
	}
	return 1;
}

