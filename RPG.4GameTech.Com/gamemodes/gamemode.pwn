function StartPaintball()
{
	format(gString, sizeof(gString), "NR Bot: At the moment %d players are fighting inside the Paintball arena.", PaintballPlayers);
	SendClientMessageToAll(COLOR_NEWS,gString);
	new maxim,weapon;
	for(new i = 0; i < sizeof(pgun); i++) if(pgun[i] > maxim) { maxim = pgun[i]; weapon = i;}
	if(weapon == 0) pgunp = 31;
	if(weapon == 1) pgunp = 24;
	if(weapon == 2) pgunp = 32;
	if(weapon == 3) pgunp = 27;
