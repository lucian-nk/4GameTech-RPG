	    if(PlayerInfo[playerid][pAdmin] == 8) format(gString, sizeof(gString), "** Co-Owner %s[%d] : %s **", GetName(playerid), playerid, msg);
	    if(PlayerInfo[playerid][pAdmin] == 7) format(gString, sizeof(gString), "** Manager Factiuni %s[%d] : %s **", GetName(playerid), playerid, msg);
	    if(PlayerInfo[playerid][pAdmin] == 6) format(gString, sizeof(gString), "** Admin Exper %s[%d] : %s **", GetName(playerid), playerid, msg);
	    if(PlayerInfo[playerid][pAdmin] == 5) format(gString, sizeof(gString), "** Administrator %s[%d] : %s **", GetName(playerid), playerid, msg);
	    if(PlayerInfo[playerid][pAdmin] == 4) format(gString, sizeof(gString), "** Admin de Elita %s[%d] : %s **", GetName(playerid), playerid, msg);
		    else if(PlayerInfo[playerid][pAdmin] == 3) format(gString, sizeof(gString), "** Admin Avansat %s[%d] : %s **", GetName(playerid), playerid, msg);
			else format(gString, sizeof(gString), "** Admin Level %d %s[%d] : %s **", PlayerInfo[playerid][pAdmin], GetName(playerid), playerid, msg);
		}
		else format(gString, sizeof(gString), "** Lider %s %s[%d] : %s **", FactionName[PlayerInfo[playerid][pLeader]], GetName(playerid), playerid, msg);
		new string2[128];
		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
			strdel(gString, 110, 256);

			format(gString,128,"%s ...",gString);
			LBroadCast(COLOR_LC, gString);

			format(string2,128,"... %s",string2);
			LBroadCast(COLOR_LC, string2);
		}
		else
		{
			LBroadCast(COLOR_LC, gString);
		}
	}
	return 1;
}
