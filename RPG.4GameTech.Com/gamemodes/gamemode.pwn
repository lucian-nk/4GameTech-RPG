	if(PlayerInfo[playerid][pAdmin] < 1 && PlayerInfo[playerid][pHelper] < 3) return SendClientMessage(playerid, COLOR_ERROR, "Your admin/helper level is not high enough to use this command.");
	if(sscanf(params,"s[25]s[128]", splayer, msg)) return SendClientMessage(playerid, COLOR_SYN,"Synthax:{FFFFFF} /pm <Player ID/Name> <message>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		new string2[128];
		
		if(PlayerInfo[playerid][pAdmin]) format(gString, sizeof(gString), "< Admin PM from %s : %s >", GetName(playerid), msg);
		else format(gString, sizeof(gString), "< Helper PM from %s : %s >", GetName(playerid), msg);
		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
			strdel(gString, 110, 256);

			format(gString,128,"%s ...",gString);
			SendClientMessage(giveplayerid, COLOR_YELLOW, gString);

			format(string2,128,"... %s",string2);
			SendClientMessage(giveplayerid, COLOR_YELLOW, string2);
		}
		else
		{
			SendClientMessage(giveplayerid, COLOR_YELLOW, gString);
		}
		
		format(gString, sizeof(gString), "< PM sent to %s[%d] : %s >", GetName(giveplayerid),giveplayerid, msg);
  		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
			strdel(gString, 110, 256);

			format(gString,128,"%s ...",gString);
			SendClientMessage(playerid,  COLOR_YELLOW, gString);

			format(string2,128,"... %s",string2);
			SendClientMessage(playerid,  COLOR_YELLOW, string2);
		}
		else
		{
			SendClientMessage(playerid,  COLOR_YELLOW, gString);
		}

		//
		format(gString, sizeof(gString), "[ PM from %s[%d] to %s[%d]: %s ]", GetName(playerid), playerid, GetName(giveplayerid), giveplayerid, msg);
		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
			strdel(gString, 110, 256);
			format(gString,128,"%s ...",gString);
			format(string2,128,"... %s",string2);
		}
		
  		foreach(new i : Player)
		{
		    if(ShowPM[i])
		    {
				if(strlen(gString) > 120)
				{
					SendClientMessage(i, COLOR_YELLOW, gString);
					SendClientMessage(i, COLOR_YELLOW, string2);
				}
				else SendClientMessage(i, COLOR_YELLOW, gString);
			}
		}
		//
	}
	return 1;
}

