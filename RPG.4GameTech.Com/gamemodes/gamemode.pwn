CMD:bizwithdraw(playerid, params[])
{
	new money;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pBizzKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You don't own a business.");
	new bizz = PlayerInfo[playerid][pBizzKey];
	if(sscanf(params,"i", money)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /bizwithdraw <amount>"), format(gString, sizeof(gString), "You have $%s in business safe.", FormatNumber(BizzInfo[bizz][bSafe])), SendClientMessage(playerid, COLOR_GRAD3, gString);
	{
		if(BizzEntered[playerid] != bizz && BizzInfo[bizz][bSBiz] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in your business.");
		if(!IsPlayerInRangeOfPoint(playerid, 3, BizzInfo[bizz][bEntranceX], BizzInfo[bizz][bEntranceY], BizzInfo[bizz][bEntranceZ]) && BizzInfo[bizz][bSBiz] == 1) return SendClientMessage(playerid, COLOR_ERROR, "You need to be near your business.");
		if(money > BizzInfo[bizz][bSafe] || money < 1) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money in bizz safe.");
		GivePlayerCash(playerid, money);
		BizzInfo[bizz][bSafe] -= money;
		bUpdate(bizz, bSafex);
		format(gString, sizeof(gString), "  You have withdrawn $%s from your bizz safe, Total: $%s ", FormatNumber(money),FormatNumber(BizzInfo[bizz][bSafe]));
		SendClientMessage(playerid, COLOR_YELLOW, gString);
		PPWithdrawLog(playerid, bizz, money, 1);
	}
	return 1;
}

CMD:sellbizto(playerid, params[])
{
	new giveplayerid, splayer[25], price;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pBizzKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You don't own a business.");
	if(sscanf(params,"s[25]i", splayer,price)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /sellbizto <Player ID/Name> <price>");
	{
		new bizz = PlayerInfo[playerid][pBizzKey];
		if(!IsPlayerInRangeOfPoint(playerid, 3.0, BizzInfo[bizz][bEntranceX], BizzInfo[bizz][bEntranceY], BizzInfo[bizz][bEntranceZ])) return SendClientMessage(playerid, COLOR_ERROR, "You need to be near your house.");
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(PlayerInfo[giveplayerid][pBizzKey] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Player already own a business.");
		if(MoneyACheck(playerid, price, 1, 1000000000)) return 1;
		if(price > GetPlayerCash(giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player don't have enough money.");
		if(PlayerInfo[giveplayerid][pLevel] < BizzInfo[bizz][bLevel]) return SendClientMessage(playerid, COLOR_ERROR, "Player level is not high enough.");

		BizzPrice[giveplayerid][playerid] = price;
		format(gString, sizeof(gString), "You offered %s to buy your business for $%s.",GetName(giveplayerid),FormatNumber(price));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Buiness Owner %s ofered you to buy his business for $%s, type (/accept business %d) to accept.",GetName(playerid),FormatNumber(price),playerid);
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
	}
	return 1;
}

CMD:asellbiz(playerid, params[])
{
	new biz;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"i",biz)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /asellbiz <bizid>");
	{
	    if(strlen(BizzInfo[biz][bOwner]) <= 0) return SendClientMessage(playerid, COLOR_ERROR, "Invalid business id.");
		if(GetPlayerID(BizzInfo[biz][bOwner]) != INVALID_PLAYER_ID)
		{
			PlayerInfo[GetPlayerID(BizzInfo[biz][bOwner])][pBizzKey] = 0;
			Update(GetPlayerID(BizzInfo[biz][bOwner]), pBizzKeyx);
		}
		else
		{
		    gQuery[0] = (EOS);
			mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `BizzKey`='0' WHERE `BizzKey`=%d", biz);
			mysql_query(handle, gQuery);
		}

		format(gString,sizeof(gString),"You sold the business %d, owned by %s.",biz,BizzInfo[biz][bOwner]);
  		format(gString, sizeof(gString), "%s sold business %d, owned by %s",GetName(playerid), biz, BizzInfo[biz][bOwner]);
		CMDRaport(gString, 5, 0);

		BizzInfo[biz][bLocked] = 1;
		bUpdate(biz, bLockedx);
		BizzInfo[biz][bOwned] = 0;
		bUpdate(biz, bOwnedx);
		BizzInfo[biz][bSafe] = 0;
		bUpdate(biz, bSafex);
		BizzInfo[biz][bEntranceCost] = 1;
		bUpdate(biz, bEntranceCostx);
		if(BizzInfo[biz][bItemPrice] > 0)
		{
		    BizzInfo[biz][bItemPrice] = 1;
		    bUpdate(biz, bItemPricex);
		}
		strmid(BizzInfo[biz][bOwner], "The State", 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		bUpdate(biz, bOwnerx);
		PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		GameTextForPlayer(playerid, gString, 5000, 3);
		BizzUpdate(biz);
	}
	return 1;
}

