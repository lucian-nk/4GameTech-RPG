CMD:fvr(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pRank] < 5) return SendClientMessage(playerid, COLOR_ERROR, "You don't have leader/co-leader.");
	new faction = PlayerInfo[playerid][pLeader] + PlayerInfo[playerid][pMember];

	new bool:occupied[MAX_VEHICLES];
  	foreach(new i : Player)
	{
		if(IsPlayerInAnyVehicle(i)) { occupied[GetPlayerVehicleID(i)] = true; }
	}
	
	for(new i = 1; i < MAX_VEHICLES; i++)
	{
	    if(i == INVALID_VEHICLE_ID) continue;
	    if(occupied[i] == true) continue;
	    if(faction == 1) if(IsAPoliceCar(i)) SetVehicleToRespawn(i);
	    if(faction == 2) if(IsAFBICar(i)) SetVehicleToRespawn(i);
	    if(faction == 3) if(IsAGovCar(i)) SetVehicleToRespawn(i);
	    if(faction == 4) if(IsANRCar(i)) SetVehicleToRespawn(i);
	    if(faction == 5) if(IsATaxiCar(i)) SetVehicleToRespawn(i);
     	if(faction == 6) if(IsATTCCar(i)) SetVehicleToRespawn(i);
	    if(faction == 7) if(IsAHitmenCar(i)) SetVehicleToRespawn(i);
	    if(faction == 8) if(IsABarziniCar(i)) SetVehicleToRespawn(i);
	    if(faction == 9) if(IsAPaternoCar(i)) SetVehicleToRespawn(i);
	    if(faction == 10) if(IsATattagliaCar(i)) SetVehicleToRespawn(i);
	    if(faction == 11) if(IsACorleoneCar(i)) SetVehicleToRespawn(i);
	}
	SendClientMessage(playerid, COLOR_SYN2, "All unocupied faction vehicles was respawned.");
	return 1;
}

CMD:fnews(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You are not in a faction.");
	if(PlayerInfo[playerid][pRank] < 6) return SendClientMessage(playerid, COLOR_ERROR, "You don't have rank 5.");
	new id = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader], msg[128];
	if(sscanf(params,"s[128]",msg)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /fnews <text>");
	if(strmatch(msg, "0")) strmid(SafeInfo[id][sNews], msg, 0, strlen(msg), 128), sUpdate(id,sNewsx);
	else
	{
		strmid(SafeInfo[id][sNews], msg, 0, strlen(msg), 128);
		sUpdate(id,sNewsx);
		format(gString,sizeof(gString),"** Faction News: %s",msg);
		SendFactionMessage(id, COLOR_YELLOW, gString);
	}
	return 1;
}

CMD:members(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You are not member of a faction.");
	new fString[564],string[124],faction = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader],members=0;
	foreach(Player,i)
	{
		if(PlayerInfo[i][pMember] == faction)
		{
		    string[0] = 0;
		    if(AFK[i] == 1)
		    {
				if(PlayerInfo[i][pMember] == faction) format(string, sizeof(string), "%s(%d) - Rank %d - %d/3 -(AFK)\n",GetName(i),i,PlayerInfo[i][pRank],PlayerInfo[i][pFWarns]);
			}
			else if(AFK[i] == 0)
			{
			    if(PlayerInfo[i][pMember] == faction) format(string, sizeof(string), "%s(%d) - Rank %d - %d/3\n",GetName(i),i,PlayerInfo[i][pRank],PlayerInfo[i][pFWarns]);
			}
			members++;
			strcat(fString,string);
		}
	}
	if(members == 0) return SendClientMessage(playerid, COLOR_ERROR, "No members online now.");
	format(string,sizeof(string),"Members online: %d",members);
	ShowPlayerDialog(playerid, 9999, DIALOG_STYLE_LIST, string, fString , "Close", "");
	return 1;
}

