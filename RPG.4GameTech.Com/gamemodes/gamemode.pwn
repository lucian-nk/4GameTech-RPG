stock CalcDate(&year, &month, &day, extradays)
{
	getdate(year, month, day);
	day += extradays;

	switch(month)
	{
		case 1, 3, 5, 7, 8, 10, 12:
		{
			if(day > 31 )
			{
				if(month == 12)
				{
					month = 1;
					year++;
				}
				else
				{
					month++;
				}
				day -= 31;
			}
		}
		case 4, 6, 9, 11:
		{
			if(day > 30 )
			{
				month++;
				day -= 30;
			}
		}
		case 2:
		{
			if(day > 28 && year / 4 != 0)
			{
				month++;
				day -= 28;
			}
			if(day > 29 && year / 4 == 0)
			{
				month++;
				day -= 29;
			}
		}
	}
}

