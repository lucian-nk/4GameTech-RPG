	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		switch (stat)
		{
			case 1:
			{
				PlayerInfo[giveplayerid][pLevel] = amount;
				Update(giveplayerid,pLevelx);
				format(gString, sizeof(gString), "You have set %s's level to %d.",GetName(giveplayerid), amount);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				SetPlayerScore(giveplayerid, PlayerInfo[giveplayerid][pLevel]);
				format(gString, sizeof(gString), "%s set %s's level to %d.",GetName(playerid), GetName(giveplayerid), amount);
				CMDRaport(gString, 6, 0);
			}
			case 2:
			{
				PlayerInfo[giveplayerid][pAccount] = amount;
				Update(giveplayerid,pAccountx);
				format(gString, sizeof(gString), "You have set %s's bank money to %d.",GetName(giveplayerid), amount);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				format(gString, sizeof(gString), "%s set %s's bank money to %d.",GetName(playerid), GetName(giveplayerid), amount);
				CMDRaport(gString, 6, 0);
			}
			case 3:
			{
				PlayerInfo[giveplayerid][pPhoneNumber] = amount;
				Update(giveplayerid, pPhoneNumberx);
				format(gString, sizeof(gString), "You have set %s's phone number to %s.",GetName(giveplayerid), PhoneFormat(amount));
				SendClientMessage(playerid, COLOR_SYN2, gString);
				format(gString, sizeof(gString), "%s set %s's phone number to %s.",GetName(playerid), GetName(giveplayerid), PhoneFormat(amount));
				CMDRaport(gString, 6, 0);
			}
			case 4:
			{
				PlayerInfo[giveplayerid][pRespect] = amount;
				Update(giveplayerid,pRespectx);
				format(gString, sizeof(gString), "You have set %s's respect points to %d.",GetName(giveplayerid), amount);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				format(gString, sizeof(gString), "%s set %s's respect points to %d.",GetName(playerid), GetName(giveplayerid), amount);
				CMDRaport(gString, 6, 0);
			}
			case 5:
			{
				PlayerInfo[giveplayerid][pHoursPlayed] = amount;
				Update(giveplayerid,pHoursPlayedx);
				format(gString, sizeof(gString), "You have set %s's hours played to %d.",GetName(giveplayerid), amount);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				format(gString, sizeof(gString), "%s set %s's hours played to %d.",GetName(playerid), GetName(giveplayerid), amount);
				CMDRaport(gString, 6, 0);
			}
			case 6:
			{
                if(amount < 0 && amount > 1) return 1;
				if(PlayerInfo[giveplayerid][pPremiumAccount] == 1 && amount == 1) return SendClientMessage(playerid, COLOR_ERROR, "Player already has Premium Account.");
				PlayerInfo[giveplayerid][pPremiumAccount] = amount;
				Update(giveplayerid,pPremiumAccountx);
				if(amount == 1) PlayerInfo[giveplayerid][pVehicleSlots] += 1;
				else PlayerInfo[giveplayerid][pVehicleSlots] -= 1;
