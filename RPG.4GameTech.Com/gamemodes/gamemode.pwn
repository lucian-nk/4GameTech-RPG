function RemovePickup(playerid, pickup)
{
	new pickupv[75];
	sscanf(PlayerInfo[playerid][pPlayerPickups], "p<|>e<iiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiii>", pickupv);
	for(new i = 0; i < sizeof(pickupv); i++) if(pickupv[i] == pickup) pickupv[i] = 0;

	new strn[16];
	format(PlayerInfo[playerid][pPlayerPickups], 16, "%d", pickupv[0]);
	for(new i = 1; i < sizeof(pickupv); i++)
	{
	    format(strn, sizeof(strn), "%d|", pickupv[i]);
		strins(PlayerInfo[playerid][pPlayerPickups], strn, 0);
	}
	Update(playerid, pPlayerPickupsx);
	
	new remainpickups = 0;
	for(new i = 0; i < sizeof(pickupv); i++) if(pickupv[i] != 0) remainpickups++;
	if(remainpickups)
	{
		format(gString, sizeof(gString), "You need to find others %d stars.", remainpickups);
		SendClientMessage(playerid, COLOR_SYN2, gString);
	}
	else
	{
	    format(gString, sizeof(gString), "%s found all 75 stars and finished 'The lucky one' event. Reward: $1,000,000, 250 Gold, 10 Rob Points and 1 Level UP.", GetName(playerid));
	    SendClientMessageToAll(COLOR_YELLOW2, gString);
	    GivePlayerCash(playerid, 1000000);
		PlayerInfo[playerid][pCoins] += 250;
		Update(playerid, pCoinsx);
		PlayerInfo[playerid][pRob] += 10;
		Update(playerid, pRobx);
		PlayerInfo[playerid][pLevel] += 1;
		Update(playerid, pLevelx);
		SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
	}
	return 1;
}


function GeneratePickups(playerid)
{
    new numbers[75];
    new unumbers[101];
	for(new i = 0; i < sizeof(numbers); i++)
	{
	    new rando = random(100)+1;
	    if(unumbers[rando] == 0) { numbers[i] = rando; unumbers[rando] = 1; }
		else
		{
		    new ok = 0;
		    while(ok == 0)
		    {
		        new rando2 = random(100)+1;
		        if(unumbers[rando2] == 0) { numbers[i] = rando2; unumbers[rando2] = 1; ok = 1; }
		    }
		}
	}
	
	new strn[16];
	format(PlayerInfo[playerid][pPlayerPickups], 16, "%d", numbers[0]);
	for(new i = 1; i < sizeof(numbers); i++)
	{
	    format(strn, sizeof(strn), "%d|", numbers[i]);
		strins(PlayerInfo[playerid][pPlayerPickups], strn, 0);
	}
	Update(playerid, pPlayerPickupsx);
}

