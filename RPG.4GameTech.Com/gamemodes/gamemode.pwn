		{
			SCM( playerid, COLOR_WHITE, "Your account is locked, use /unlockacc to unlock it." );
		}
		if(PlayerInfo[playerid][pLimba] == 2) // Romana
		{
			SCM( playerid, COLOR_WHITE, "Contul tau este blocat, deblocheaza-l folosind comanda /unlockacc." );
		}
	}
	if(sscanf(params,"s[21]s[21]", oldpass, newpass)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /changepass <old-password> <new-password>");
	{
		if(!strmatch(oldpass, PlayerInfo[playerid][pPassword])) return SendClientMessage(playerid, COLOR_ERROR, "Incorect password.");

		format(PlayerInfo[playerid][pPassword], 64, newpass);

		gQuery[0] = (EOS);
