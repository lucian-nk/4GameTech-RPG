CMD:rac(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	new bool:occupied[MAX_VEHICLES];
 	foreach(new i : Player)
	{
		if(IsPlayerInAnyVehicle(i))
		{
		    if(IsTrailerAttachedToVehicle(GetPlayerVehicleID(i))) occupied[GetVehicleTrailer(GetPlayerVehicleID(i))] = true;
			occupied[GetPlayerVehicleID(i)] = true;
		}
	}
	for(new i = 1; i < MAX_VEHICLES; i++)
	{
	    if(i == INVALID_VEHICLE_ID) continue;
	    if(occupied[i] == true) continue;
     	SetVehicleToRespawn(i);
	}
	SendClientMessage(playerid, COLOR_SYN2, "All unocupied vehicles were respawned.");
	format(gString, sizeof(gString), "Admin %s respawned all vehicles.", GetName(playerid));
	SendClientMessageToAll(COLOR_LIGHTRED, gString);
 	format(gString, sizeof(gString), "%s respawned all vehicles.",GetName(playerid));
	CMDRaport(gString, 3, 0);
	return 1;
}

