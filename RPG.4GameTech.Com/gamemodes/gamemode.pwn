CMD:megaphone(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(IsPlayerConnected(playerid))
	{
		new vid = GetPlayerVehicleID(playerid),id,name[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME],string[200];
		GetPlayerName(playerid, sendername, sizeof(sendername));
		if(PlayerInfo[playerid][pMuted] == 1)
	    {
			format(string, sizeof(string), "Nu poti tasta, ai primit mute pentru %d secunde!",PlayerInfo[playerid][pMuteTime]);
			SendClientMessage(playerid, COLOR_LIGHTRED, string);
			return 1;
		}
		if(sscanf(params, "u", id)) return SendClientMessage(playerid, 0xFFFFFFFF, "Tasteaza: /m <ID/Nume jucator>");
		if(IsACop(playerid))
		{
		    GetPlayerName(id, name, sizeof(name));
			if(!IsACopCar(vid))
			{
				SendClientMessage(playerid, COLOR_ERROR, "You are not in a Police Vehicle.");
				return 1;
			}
			if(PlayerInfo[playerid][pMember] == 1||PlayerInfo[playerid][pLeader] == 1)
			{
				format(string, sizeof(string), "[Officer %s: STOP! %s, trage pe partea dreapta a carosabilului!]", sendername, name);
				ProxDetector(60.0, playerid, string,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW);
			}
			else if(PlayerInfo[playerid][pMember] == 2||PlayerInfo[playerid][pLeader] == 2)
			{
				format(string, sizeof(string), "[Agent %s: STOP! %s, trage pe partea dreapta a carosabilului!]", sendername, name);
				ProxDetector(60.0, playerid, string,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW);
			}
			else if(PlayerInfo[playerid][pMember] == 3||PlayerInfo[playerid][pLeader] == 3)
			{
				format(string, sizeof(string), "[Soldier %s: STOP! %s, trage pe partea dreapta a carosabilului!]", sendername, name);
				ProxDetector(60.0, playerid, string,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW);
			}
			printf("%s", string);
		}
		else
		{
			SendClientMessage(playerid, 0xFFB870FF, "Nu esti membru PD.");
			return 1;
		}
	}
	return 1;
}

