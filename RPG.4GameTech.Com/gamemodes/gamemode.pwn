CMD:veh(playerid, params[])
{
	new modelv[32], col1, col2, vsplate[15];
	new Float:X,Float:Y,Float:Z,Float:Angle;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[32]ii",modelv,col1,col2)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /veh <model> <color1> <color2>");
	{
		new model = VehicleM(modelv);
		if (model < 400 || model > 611) return SendClientMessage(playerid, COLOR_ERROR, "Model must be between 400 and 611.");
        format(gString, sizeof(gString), "%s spawned a %s.",GetName(playerid), VehicleNames[model - 400]);
		CMDRaport(gString, 5, 0);

		GetPlayerPos(playerid, X,Y,Z);
		GetPlayerFacingAngle(playerid, Angle);
		VehToDestroy[counter] = CreateVehicle(model, X, Y, Z, Angle, col1, col2, 1800);
		SetVehicleParamsEx(VehToDestroy[counter],VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
        Fuel[VehToDestroy[counter]] = 100;
		
		LinkVehicleToInterior(VehToDestroy[counter], GetPlayerInterior(playerid));
		SetVehicleVirtualWorld(VehToDestroy[counter], GetPlayerVirtualWorld(playerid));

		if(counter < 9)
		{
			format(vsplate, sizeof(vsplate), "ASV 00%d", counter+1);
		}
		else
		{
			format(vsplate, sizeof(vsplate), "ASV 0%d", counter);
		}

		SetVehicleNumberPlate(VehToDestroy[counter], vsplate);
		PutPlayerInVehicle(playerid, VehToDestroy[counter], 0);
		format(gString, sizeof(gString), "Vehicle spawned. [ID: %d] [CID: %d]", counter+1, VehToDestroy[counter]);
		SendClientMessage(playerid, COLOR_SYN2, gString);
		counter ++;
	}
	return 1;
}

CMD:destroyvehicles(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(counter == 0) return SendClientMessage(playerid, COLOR_ERROR, "No vehicle to destroy.");
	format(gString, sizeof(gString), "%s destroyed all admin spawned vehicles.",GetName(playerid));
	CMDRaport(gString, 5, 0);
	for(new i=0;i<sizeof(VehToDestroy);i++)
	{
		if(VehToDestroy[i] == INVALID_VEHICLE_ID) continue;
		DestroyVehicle(VehToDestroy[i]);
		VehToDestroy[i] = 0;
		counter = 0;
	}
	return 1;
}

CMD:destroythisvehicle(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
    if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in a vehicle.");
	new found = 0;
	for(new i = 0; i < sizeof(VehToDestroy) && found == 0; i++)
	{
		if(VehToDestroy[i] == GetPlayerVehicleID(playerid))
		{
			DestroyVehicle(VehToDestroy[i]);
			VehToDestroy[i] = 0;
			found = 1;
		}
	}
 	if(found == 0)SendClientMessage(playerid, COLOR_ERROR, "You can't destroy this vehicle.");
	return 1;
}

CMD:dtv(playerid, params[]) return cmd_destroythisvehicle(playerid, params);

CMD:apark(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
 	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	new ivID = GetPlayerVehicleID(playerid), Float:Pos[4];
	if(OwnedVeh(ivID) == 0) return SendClientMessage(playerid, COLOR_ERROR, "You can't park this vehicle.");
	GetVehiclePos(ivID, Pos[0], Pos[1], Pos[2]);
	GetVehicleZAngle(ivID, Pos[3]);

	vInfo[OwnedVeh(ivID)][vPosX] = Pos[0];
	vInfo[OwnedVeh(ivID)][vPosY] = Pos[1];
	vInfo[OwnedVeh(ivID)][vPosZ] = Pos[2];
	vInfo[OwnedVeh(ivID)][vPosA] = Pos[3];
	vUpdate(OwnedVeh(ivID), vPosXx);
	vUpdate(OwnedVeh(ivID), vPosYx);
	vUpdate(OwnedVeh(ivID), vPosZx);
	vUpdate(OwnedVeh(ivID), vPosAx);

	new Float:health;
	GetVehicleHealth(ivID, health);

	new
	vCreated = OwnedVeh(ivID);
	OwnedVeh(ivID) = 0;
    if(IsValidObject(neon1[ivID])) DestroyObject(neon1[ivID]);
	if(IsValidObject(neon2[ivID])) DestroyObject(neon2[ivID]);
	DestroyVehicle(ivID);

	new
	NewVeh = CreateVehicle(vInfo[vCreated][vModel], vInfo[vCreated][vPosX], vInfo[vCreated][vPosY], vInfo[vCreated][vPosZ], vInfo[vCreated][vPosA], vInfo[vCreated][vColor1], vInfo[vCreated][vColor2], 500000);
	SetVehicleNumberPlate(NewVeh, vInfo[vCreated][vPlate]);
	ChangeVehiclePaintjob(NewVeh, vInfo[vCreated][vPaintJ]);
	SetVehicleParamsEx(NewVeh,VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
	OwnedVeh(NewVeh) = vCreated;

	ModVehicle(NewVeh);

	PutPlayerInVehicle(playerid,NewVeh,0);
	SetVehicleHealth(NewVeh,health);

	SendClientMessage(playerid, COLOR_SYN2, "Vehicle was parked here.");
	return 1;
}

