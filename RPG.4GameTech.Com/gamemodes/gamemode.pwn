CMD:givekey(playerid, params[])
{
	if(!gLogged[playerid])
		return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	
	if(!GetVehNumber(playerid))
		return SendClientMessage(playerid,COLOR_ERROR,"You do not currently own a vehicle.");
		
	new 
  	giveplayerid, splayer[25];
	
	if(sscanf(params, "s[25]", splayer))
		return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /givekey <Player ID/Name>");
		
	if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
	else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
	else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
	else giveplayerid = GetPlayers(splayer);
		
	if(!IsPlayerInAnyVehicle(playerid) || !strmatch(vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vOwner], GetName(playerid)) || OwnedVeh(GetPlayerVehicleID(playerid)) == 0 || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) 
		return SendClientMessage(playerid, COLOR_ERROR, "You need to be in your vehicle.");
	
	if(!ProxDetectorS(5.0, playerid, giveplayerid))
		return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		
	format(gString, sizeof (gString), "You are now sharing your car keys with %s.", GetName(giveplayerid));
	SendClientMessage(playerid, COLOR_SYN2, gString);
	
	format(gString, sizeof (gString), "%s is now sharing his car keys with you. Use /switchkey to switch to this vehicle.", GetName(playerid));
	SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		
	SwitchKey[giveplayerid] = OwnedVeh(GetPlayerVehicleID(playerid));
	ActiveKey[giveplayerid] = 1;
	
	return 1;
}

