CMD:createclan(playerid, params[])
{
	new name[64], tag[24], tagtype, slots;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 9) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[24]iis[64]",tag,tagtype,slots,name)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /createclan <tag> <tagtype (0 before / 1 after)> <slots> <name>");
	{
	    if(ExistClan(tag)) return SendClientMessage(playerid, COLOR_ERROR, "Clan already exist.");
	    if(strlen(tag) > 10) return SendClientMessage(playerid, COLOR_ERROR, "Clan tag is to long.");
	    if(strlen(name) > 32) return SendClientMessage(playerid, COLOR_ERROR, "Clan name is to long.");
	    
		TotalClans ++;
		
		strmid(ClanInfo[TotalClans][cName], name, 0, 32, 32);
		strmid(ClanInfo[TotalClans][cTag], tag, 0, 10, 10);
		ClanInfo[TotalClans][cTagType] = tagtype;
		ClanInfo[TotalClans][cSlots] = slots;

		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery) ,"INSERT INTO `clans` (`Name`,`Tag`,`TagType`,`Slots`) VALUES ('%e','%e',%d,%d)",
		ClanInfo[TotalClans][cName],
		ClanInfo[TotalClans][cTag],
		ClanInfo[TotalClans][cTagType],
		ClanInfo[TotalClans][cSlots]);
		mysql_query(handle ,gQuery);
		
