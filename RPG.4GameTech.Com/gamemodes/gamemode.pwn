CMD:betsix(playerid, params[]) 
{
	#pragma unused params

	if (!IsPlayerInRangeOfPoint(playerid, 2.0, -2159.0476, 640.3580, 1052.3817))
		return SendClientMessage(playerid, COLOR_ERROR, "You are not near the Betting place.");

	new
		string[144];
		
	if (Betsix_Started)
	{
		format(string, sizeof(string), "You must wait for this round to end. Remaining balls to be extracted: %d.", 35 - Betsix_Draw);
		return SendClientMessage(playerid, COLOR_ERROR, string);
	}
	if (BetsixNumbers[playerid][0] > 0)
	{
		format(string, sizeof(string), "You have already a ticket (%d, %d, %d, %d, %d, %d).", BetsixNumbers[playerid][0], BetsixNumbers[playerid][1], BetsixNumbers[playerid][2], BetsixNumbers[playerid][3], BetsixNumbers[playerid][4], BetsixNumbers[playerid][5]);
		return SendClientMessage(playerid, COLOR_ERROR, string);
	}
	
	new 
		numbers[6], bet, i, j;

	if (sscanf(params, "a<i>[6]i", numbers, bet))
		return SendClientMessage(playerid, COLOR_SYN, "Synthax: {FFFFFF}/betsix [number 1] [number 2] [number 3] [number 4] [number 5] [number 6] [$ bet]");

	if (GetPlayerCash(playerid) < bet)
		return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money.");
		
	if (bet > 5000 || bet < 1)
		return SendClientMessage(playerid, COLOR_ERROR, "You can't bet more than $5,000 or less than $1.");

	for (i = 0; i < 6; i++) {
		if (numbers[i] > 49 || numbers[i] < 1)
			return SendClientMessage(playerid, COLOR_ERROR, "The numbers can be only between 1 and 49.");
	}
	
	for (i = 0; i < 5; i++) {
		for (j = i + 1; j < 6; j++) {
			if (numbers[i] == numbers[j])
				return SendClientMessage(playerid, COLOR_ERROR, "You can't bet the same number twice.");
		}
	}

	BetsixBet[playerid] = bet;
	GivePlayerCash(playerid, - bet);

	for (i = 0; i < 6; i++)
		BetsixNumbers[playerid][i] = numbers[i];

	format(string, sizeof(string), "[BetSix]: You bet $%s on these numbers: %d, %d, %d, %d, %d, %d. The next round starts in %d minutes and %d seconds.", FormatNumber(bet), BetsixNumbers[playerid][0], BetsixNumbers[playerid][1], BetsixNumbers[playerid][2], BetsixNumbers[playerid][3], BetsixNumbers[playerid][4], BetsixNumbers[playerid][5], Betsix_Time / 60, Betsix_Time % 60);
	SendClientMessage(playerid, 0xCDDB72FF, string);

	return 1;
}

CMD:fish(playerid, params[]) 
{
	if (IsPlayerConnected(playerid)) 
	{
