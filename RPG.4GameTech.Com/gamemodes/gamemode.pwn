{
	for(new i = 1; i < sizeof(ZoneInfo); i++)
	{
		GangZoneShowForPlayer(playerid, Zone[i], GetZoneColor(ZoneInfo[i][zTeam]));
		if(TakeON[i] == 1) GangZoneFlashForPlayer(playerid,Zone[i],GetZoneColor(Attackers));
		else if(TakeON[i] == 2) GangZoneFlashForPlayer(playerid,Zone[i],GetZoneColor(Attackers2));

