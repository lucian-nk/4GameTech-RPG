CMD:attack(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(!IsAMafiaMember(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "You are not a mafia member.");
 	if(!CanStartWar) return SendClientMessage(playerid, COLOR_ERROR, "Wars are disabled now.");
	if(PlayerInfo[playerid][pRank] < 5) return SendClientMessage(playerid, COLOR_ERROR, "You need Rank 5 or higher to attack a zone.");
    new whour , wminute, wsecond;
   	gettime(whour, wminute, wsecond);
//  new day, month, year;
//	getdate(year, month, day);
//	if(strmatch(GetWeekDay(day, month, year), "Sunday") || strmatch(GetWeekDay(day, month, year), "Saturday")) return SendClientMessage(playerid, COLOR_ERROR, "You can't start a war in weekend.");
 	{
	 	if(whour < 19 || whour > 21) return SendClientMessage(playerid, COLOR_ERROR, "You can start war only between hours 19:00 - 21:00 | [7:00 - 9:00 (Seara)].");
	}
	new zone = GetPlayerZone(playerid);
	new member = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader];
	if(zone == -1) return SendClientMessage(playerid, COLOR_ERROR, "You are not on a zone.");
	if(ZoneInfo[zone][zTeam] < 1) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in a zone to attack.");
	if(WarPause[member] > gettime())
	{
	    format(gString, sizeof(gString), "You need to wait %d seconds.", WarPause[member]-gettime());
		SendClientMessage(playerid, COLOR_ERROR, gString);
		return 1;
	}
	if(ZoneInfo[zone][zHours] > 0) return SendClientMessage(playerid, COLOR_ERROR, "You cannot attack this zone now.");
	if(ZoneInfo[zone][zTeam] == member) return SendClientMessage(playerid, COLOR_ERROR, "You cannot attack your zone.");
	if(TakeON[zone]) return SendClientMessage(playerid, COLOR_ERROR, "This zone is already attacked.");
	if(InWar[ZoneInfo[zone][zTeam]] == 1) return SendClientMessage(playerid, COLOR_ERROR, "This mafia has already a war.");
	if(InWar[member] == 1) return SendClientMessage(playerid, COLOR_ERROR, "Your mafia has already a war.");
	if(War == 1 && War2 == 1) return SendClientMessage(playerid, COLOR_ERROR, "There is already a war in progress.");

	if(War == 0)
	{
		Attackers = member;
		Defenders = ZoneInfo[zone][zTeam];

		format(gString, sizeof(gString), "%s (%s) attacked your turf %d",FactionName[Attackers],GetName(playerid),zone);
		SendFactionMessage(Defenders, COLOR_WAR, gString);

		format(gString, sizeof(gString), "Your mafia (%s) attacked turf %d owned by %s",GetName(playerid),zone,FactionName[Defenders]);
		SendFactionMessage(Attackers, COLOR_WAR, gString);

		ScoreAtt = 0.0;
		ScoreDef = 0.0;

		War = 1;

		TakeON[zone] = 1;

		GangZoneFlashForAllEx(Zone[zone], GetZoneColor(Attackers));

		new sec = 1000;
		WarTime[0] = 900;
		SetTimerEx("StopWar", sec*WarTime[0], 0, "ii", zone, 1);

		format(gString, sizeof(gString), "Turf %d, Time remaining: 15:00",zone);
		SendFactionMessage(Defenders, COLOR_WAR, gString);
		SendFactionMessage(Attackers, COLOR_WAR, gString);
		InWar[Attackers] = 1;
		InWar[Defenders] = 1;
		OnPlayerSpawn(playerid);
		for(new i = barzini[0]; i <= corleone[7]; i++) if(GetVehicleVirtualWorld(i) == 0) SetVehicleToRespawn(i);
		return 1;
	}
 	if(War2 == 0)
	{
		Attackers2 = member;
		Defenders2 = ZoneInfo[zone][zTeam];

		format(gString, sizeof(gString), "%s (%s) attacked your turf %d",FactionName[Attackers2],GetName(playerid),zone);
		SendFactionMessage(Defenders2, COLOR_WAR, gString);

		format(gString, sizeof(gString), "Your mafia (%s) attacked turf %d owned by %s",GetName(playerid),zone,FactionName[Defenders2]);
		SendFactionMessage(Attackers2, COLOR_WAR, gString);

		ScoreAtt2 = 0.0;
		ScoreDef2 = 0.0;

		War2 = 1;

		TakeON[zone] = 2;

		GangZoneFlashForAllEx(Zone[zone], GetZoneColor(Attackers2));

		new sec = 1000;
		WarTime[1] = 900;
		SetTimerEx("StopWar", sec*WarTime[1], 0, "ii", zone, 2);

		format(gString, sizeof(gString), "Turf %d, Time remaining: 15:00",zone);
		SendFactionMessage(Defenders2, COLOR_WAR, gString);
		SendFactionMessage(Attackers2, COLOR_WAR, gString);
		InWar[Attackers2] = 1;
		InWar[Defenders2] = 1;
		OnPlayerSpawn(playerid);
		for(new i = barzini[0]; i <= corleone[7]; i++) if(GetVehicleVirtualWorld(i) == 0) SetVehicleToRespawn(i);
		return 1;
	}
	return 1;
}

