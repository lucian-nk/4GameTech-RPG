public OnPlayerPickUpDynamicPickup(playerid, pickupid)
{
	if(pickupid == PICKUP_FISHPLACE)
		GameTextForPlayer(playerid, "~r~Bait Shop~n~~n~~w~Type~n~~g~/buyfishpermit~n~~g~/sellfish", 1500, 3);

	else if(pickupid == PICKUP_FISH[0] || pickupid == PICKUP_FISH[1])
		GameTextForPlayer(playerid, "~y~Fishing Place~n~~w~Type ~y~~h~/fish~n~~n~~y~You will need a fish permit.", 1500, 3);
		
	else if (pickupid == PICKUP_BETSIX)
		GameTextForPlayer(playerid, "~w~~h~Bet~r~~h~Six~n~~w~Type ~r~~h~/betsix~w~ to place your bet", 1500, 3);
		
	if(VerifyPickup(pickupid))
	{
	    if(ValidPickup(playerid, VerifyPickup(pickupid)))
		{
		    if(PickupFind[playerid] > 0)
		    {
		        format(gString, sizeof(gString), "You need to wait %d seconds.", PickupFind[playerid]);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				return 1;
		    }
			SendClientMessage(playerid, COLOR_SYN2, "Congratulations, you found a lucky star.");
			RemovePickup(playerid, VerifyPickup(pickupid));
			PickupFind[playerid] = 120;
		}
		else SendClientMessage(playerid, COLOR_SYN2, "Sorry, this star is a unlucky one.");
	}
	if(IsInWar1(playerid))
	{
		if(PickupHealth1[pickupid])
		{
		    new Float:health;
			GetPlayerHealth(playerid,health);
			if(health <= 89) SetPlayerHealthEx(playerid,health+10);
			else SetPlayerHealthEx(playerid,100);
			PickupHealth1[pickupid] = 0;
			PickupWeapon1[pickupid] = 0;
			DestroyDynamicPickup(pickupid);
			SendClientMessage(playerid, COLOR_SYN2, "You picked up 10 health from the ground.");
		}
	    if(PickupWeapon1[pickupid])
	    {
	        GivePlayerWeaponEx(playerid, PickupWeapon1[pickupid], 30);
	        PickupHealth1[pickupid] = 0;
			PickupWeapon1[pickupid] = 0;
			DestroyDynamicPickup(pickupid);
			SendClientMessage(playerid, COLOR_SYN2, "You picked up a gun with 30 ammo from the ground.");
	    }
    }
    
    if(IsInWar2(playerid))
	{
		if(PickupHealth2[pickupid])
		{
		    new Float:health;
			GetPlayerHealth(playerid,health);
			if(health <= 89) SetPlayerHealthEx(playerid,health+10);
			else SetPlayerHealthEx(playerid,100);
			PickupHealth2[pickupid] = 0;
			PickupWeapon2[pickupid] = 0;
			DestroyDynamicPickup(pickupid);
			SendClientMessage(playerid, COLOR_SYN2, "You picked up 10 health from the ground.");
		}
	    if(PickupWeapon2[pickupid])
	    {
	        GivePlayerWeaponEx(playerid, PickupWeapon2[pickupid], 30);
	        PickupHealth2[pickupid] = 0;
			PickupWeapon2[pickupid] = 0;
			DestroyDynamicPickup(pickupid);
			SendClientMessage(playerid, COLOR_SYN2, "You picked up a gun with 30 ammo from the ground.");
	    }
    }
    return 1;
}

public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
    new string[512];
	if(gLogged[playerid] == 0) return 1;
	if((IsACBUGWeapon(playerid) && RELEASED(KEY_FIRE)) && newkeys != KEY_FIRE && newkeys & KEY_CROUCH)
	{
	   ApplyAnimation ( playerid , "PED" , "getup" , 4.1 , 0 , 0 , 0 , 0 , 0 ) ;
	   SetPlayerArmedWeapon(playerid, 0);

	}
	if(newkeys & KEY_NO)
	{
		new ivID = FindClosestVehicle(playerid);

		if(OwnedVeh(ivID) != 0)
		{
			if(strmatch(vInfo[OwnedVeh(ivID)][vOwner], GetName(playerid)) || (ActiveKey[playerid] == 2 && SwitchKey[playerid] == OwnedVeh(ivID)))
			{
				if(vInfo[OwnedVeh(ivID)][vLocked] == 0)
				{
					vInfo[OwnedVeh(ivID)][vLocked] = 1;

					vUpdate(OwnedVeh(ivID), vLockedx);

					new model = GetVehicleModel(ivID);
					format(gString,sizeof(gString),"~w~%s ~r~locked.", VehicleNames[model - 400]);
					GameTextForPlayer(playerid, gString, 5000, 6);
				}
				else if(vInfo[OwnedVeh(ivID)][vLocked] == 1)
				{
					vInfo[OwnedVeh(ivID)][vLocked] = 0;

					vUpdate(OwnedVeh(ivID), vLockedx);

					new model = GetVehicleModel(ivID);
					format(gString,sizeof(gString),"~w~%s ~g~unlocked.", VehicleNames[model - 400]);
					GameTextForPlayer(playerid, gString, 5000, 6);
				}
			}
		}
	}
	if((newkeys & KEY_ACTION) && (GetPlayerState(playerid)==PLAYER_STATE_DRIVER))
	{
		new vid = GetPlayerVehicleID(playerid);
		GetVehicleParamsEx(vid,engine,lights,alarm,doors,bonnet,boot,objective);
		if(lights == VEHICLE_PARAMS_OFF)
		{
			SetVehicleParamsEx(vid,engine,VEHICLE_PARAMS_ON,alarm,doors,bonnet,boot,objective);
		}
		else
		{
			SetVehicleParamsEx(vid,engine,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
		}
	}
	if(newkeys & KEY_CROUCH)
	{
		if(IsPlayerInRangeOfPoint(playerid, 15.0, 1589.053344, -1638.123168, 14.122960) && IsACop(playerid))
		{
			MoveDynamicObject(pdgate1, 1598.33362, -1638.26563, 13.81120, 2.0);
		}
		else if(IsPlayerInRangeOfPoint(playerid, 15.0, 1544.700317, -1630.735717, 13.096980) && IsACop(playerid))
		{
			if(justrotated == 0)
			{
				SetObjectRot(pdgate2, 0, 0, 90);
				SetTimer("RotateBack", 2500, 0);
				justrotated = 1;
			}
