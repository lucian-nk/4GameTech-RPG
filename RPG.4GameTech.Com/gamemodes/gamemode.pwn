				Update(playerid, pDrugsx);
				format(gString, sizeof(gString), "New Balance: %s", FormatNumber(SafeInfo[id][sDrugs]));
				SendClientMessage(playerid, COLOR_YELLOW, gString);
				SafeDrugsLog(playerid, "deposited", amount, id, PlayerInfo[playerid][pLeader]);
