function Float:GetDistanceBetweenPlayers(p1,p2)
{
        new Float:x1,Float:y1,Float:z1,Float:x2,Float:y2,Float:z2;
        if(!IsPlayerConnected(p1) || !IsPlayerConnected(p2))
        {
                return -1.00;
        }
        GetPlayerPos(p1,x1,y1,z1);
        GetPlayerPos(p2,x2,y2,z2);
        return floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,
z1)),2));
}

function StartRace() // iFordawin
{
	new
		str[4],
		soundid = 1056,
		p[7],
		veh;
		
	RaceCountdown --;
		
	if(RaceCountdown)
	{
		str[0] = RaceCountdown + 48;
		str[1] = EOS;
	}
	else
	{
		strcat((str[0] = EOS, str), "GO!"); // strcpy - mult mai rapid decat strmid/format.
		soundid = 1057;
		RaceStarted = true;
	}
	
 	foreach(new i : Player)
	{
		if(!InRace[i])
			continue;
			
		veh = GetPlayerVehicleID(i);
				
		GameTextForPlayer(i, str, 1500, 3);
		PlayerPlaySound(i, soundid, 0.0, 0.0, 0.0);
		
		RaceCP[i] = 1;
		
		if(!RaceCountdown)
		{
			if(veh != INVALID_VEHICLE_ID)
			{
				GetVehicleParamsEx(veh, p[0], p[1], p[2], p[3], p[4], p[5], p[6]);
				SetVehicleParamsEx(veh, 1, p[1], p[2], p[3], p[4], p[5], p[6]);
			}
			TogglePlayerControllableEx(i, 1);
			SetPlayerRaceCheckpoint(i, 0, RaceCheckpoints[RaceCP[i] - 1][0], RaceCheckpoints[RaceCP[i] - 1][1], RaceCheckpoints[RaceCP[i] - 1][2], RaceCheckpoints[RaceCP[i]][0], RaceCheckpoints[RaceCP[i]][1], RaceCheckpoints[RaceCP[i]][2], 3.5);
		}
	}
	
	if(RaceCountdown)
		SetTimer("StartRace", 1000, false);
	
	return 1;
}

function PrepareRace()
{
	if(RacePlayers >= 2)
	{
		format(gString, sizeof(gString), "NR Bot: There are %d racers in the arena at the moment.", RacePlayers);
		SendClientMessageToAll(COLOR_NEWS, gString);

  		foreach(new i : Player)
		{
			if(!InRace[i])
				continue;
				
			SendClientMessage(i, COLOR_YELLOW, "Get ready, the race will start soon!");
		}
		RaceCountdown = 4;
		SetTimer("StartRace", 1000, false);
	}
	else // < 2 players => end race.
	{
  		foreach(new i : Player)
		{
			if(!InRace[i]) continue;
			SendClientMessage(i, COLOR_YELLOW, "The race can not start because there are not enough players. (Minimum 2 players)");
			SetPlayerPos(i, BizzInfo[25][bEntranceX], BizzInfo[25][bEntranceY], BizzInfo[25][bEntranceZ]);
			SetPlayerInterior(i,0);
			SetPlayerVirtualWorld(i,0);
			ResetPlayerWeapons(i);
			InRace[i] = false;
			RaceCP[i] = 0;
			RaceVehicle[i] = 0;
			TogglePlayerControllableEx(i, 1);
			DisablePlayerRaceCheckpoint(i);
		}
		for(new j = 0; j < sizeof (race); j++)
		{
			SetVehicleToRespawn(race[j]);
		
			SetVehicleVirtualWorld(race[j], 1337); // Hide vehicles.
			Fuel[race[j]] = 100;
		}
		
		RaceStarted = false;
		RacePlayers = 0;
	}
	return 1;
}

