function Find(playerid, findid)
{
	if(UseFind[playerid] == 1)
	{
		new Float:X,Float:Y,Float:Z;

		FindPoints[playerid] --;
		format(gString, sizeof(gString), "%d", FindPoints[playerid]);
		if(FindPoints[playerid]%2 == 0) GameTextForPlayer(playerid, gString, 1000, 6);

		if(!IsPlayerConnected(findid))
		{
			CP[playerid] = 0;
			DisablePlayerCheckpoint(playerid);
			KillTimer(FindTimer[playerid]);
			UseFind[playerid] = 0;
			return 1;
		}

		if(FindPoints[playerid] <= 0)
		{
			CP[playerid] = 0;
			DisablePlayerCheckpoint(playerid);
			KillTimer(FindTimer[playerid]);
			UseFind[playerid] = 0;
			return 1;
		}
		if(BizzEntered[findid] != 0)
		{
			new i = BizzEntered[findid];
			DisablePlayerCheckpoint(playerid);
			SetPlayerCheckpoint(playerid,BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ], 3.0);
		}
		else if(HouseEntered[findid] != 0)
		{
			new i = HouseEntered[findid];
			DisablePlayerCheckpoint(playerid);
			SetPlayerCheckpoint(playerid,HouseInfo[i][hEntranceX], HouseInfo[i][hEntranceY], HouseInfo[i][hEntranceZ], 3.0);
		}
		else if(IsInHQ(findid))
		{
		    GetHQ(findid, X,Y,Z);
		    DisablePlayerCheckpoint(playerid);
			SetPlayerCheckpoint(playerid,X,Y,Z, 3.0);
		}
		else
		{
			GetPlayerPos(findid, X,Y,Z);
			DisablePlayerCheckpoint(playerid);
			SetPlayerCheckpoint(playerid,X,Y,Z, 3.0);
		}
		CP[playerid] = 1995;
	}
	else
	{
		CP[playerid] = 0;
		DisablePlayerCheckpoint(playerid);
		KillTimer(FindTimer[playerid]);
		return 1;
	}
	return 1;
}

function PayDay()
{
	new interest, hour, minutes, seconds;
	gettime(hour, minutes, seconds);
 	foreach(new i : Player)
	{
		if(gLogged[i] == 1)
		{
			if(PlayerInfo[i][pPayDay] < 20 && PlayerInfo[i][pPremiumAccount] == 0)
			{
				format(gString,sizeof(gString),"You haven't played long enough to obtain a PayDay.(%d/20)",PlayerInfo[i][pPayDay]);
				SendClientMessage(i, COLOR_LIGHTRED, gString);
				continue;
			}
			else if(PlayerInfo[i][pPayDay] < 10 && PlayerInfo[i][pPremiumAccount] == 1)
			{
				format(gString,sizeof(gString),"You haven't played long enough to obtain a PayDay.(%d/10)",PlayerInfo[i][pPayDay]);
				SendClientMessage(i, COLOR_LIGHTRED, gString);
				continue;
			}

			if(PlayerInfo[i][pPremiumAccount] == 1) { interest = 2; }
			else { interest = 1; }

   			if(AFK[i] == 1) PlayerInfo[i][pPayCheck] /= 4;

			if(PlayerInfo[i][pPremiumAccount] == 1) PlayerInfo[i][pPayCheck] = PlayerInfo[i][pPayCheck] + PlayerInfo[i][pPayCheck]/2;

			new house = PlayerInfo[i][pRentKey];
			new rent;
			if(house != 0)
			{
				rent = HouseInfo[house][hRent];
				HouseInfo[house][hSafe] += rent;
				hUpdate(house, hSafex);
			}
			
			new paycheck = PlayerInfo[i][pPayCheck];
			new bankacc = PlayerInfo[i][pAccount];
			new interestm = bankacc*interest/100;
			if(interestm > 1500) interestm = 1500;
			if(AFK[i] == 1) interestm /= 4;
			new stateimposit;
			if(PlayerInfo[i][pBizzKey]) stateimposit += BizzInfo[PlayerInfo[i][pBizzKey]][bLevel] * 1000;
			if(PlayerInfo[i][pHouseKey]) stateimposit += HouseInfo[PlayerInfo[i][pHouseKey]][hLevel] * 1000;
			for(new c = 1; c <= Total_Veh_Created; c++) if(strmatch(vInfo[c][vOwner], GetName(i))) stateimposit += vInfo[c][vPrice]/1000;
			stateimposit += interestm/100;
			
			PlayerInfo[i][pAccount] = bankacc + paycheck + interestm + stateimposit+ rent + Tax;
			Update(i,pAccountx);
			
			format(gString, sizeof(gString), "|________________ %s PayCheck ________________|", GetName(i));
			SendClientMessage(i, -1, gString);
			format(gString, sizeof(gString), "{BABABA}Old Bank Balance: $%s", FormatNumber(bankacc));
			SendClientMessage(i, -1, gString);
			format(gString, sizeof(gString), "{BABABA}Paycheck: $%s    Money from interest: $%s (0.%d%s)", FormatNumber(paycheck), FormatNumber(interestm), interest, "%%");
			SendClientMessage(i, -1, gString);
			format(gString, sizeof(gString), "{BABABA}Mayor Tax: -$%s    State due: $%s", FormatNumber(paycheck), FormatNumber(stateimposit));
			SendClientMessage(i, -1, gString);
			format(gString, sizeof(gString), "{BABABA}New Bank Balance: $%s", FormatNumber(PlayerInfo[i][pAccount]));
			SendClientMessage(i, -1, gString);
			if(house != 0)
			{
			    format(gString, sizeof(gString), "{BABABA}Rent Price: $%s", FormatNumber(rent));
				SendClientMessage(i, -1, gString);
			}
			if(PlayerInfo[i][pCLeader] + PlayerInfo[i][pCMember] != 0 && ClanInfo[PlayerInfo[i][pCLeader] + PlayerInfo[i][pCMember]][cBest] == 1) GivePlayerCash(i,5000), SendClientMessage(i, -1, "{BABABA}Clan turf: $5,000");
			if(AFK[i] == 1) SendClientMessage(i, -1, "{BABABA}You received only 25 percent from interest and 25 percend from paycheck because you were sleeping.");
			if(PlayerInfo[i][pRPBoost] > 0)
			{
			    PlayerInfo[i][pRPBoost] -= 1;
				Update(i,pRPBoostx);
			    PlayerInfo[i][pRespect] += 1;
				Update(i,pRespectx);
				SendClientMessage(i, -1, "{BABABA}Respect boost used. (+1 RP)");
			}
		    if(HappyDayHour == hour && HappyDay == 0)
			{
				PlayerInfo[i][pRespect] += 1;
				Update(i,pRespectx);
				SendClientMessage(i,COLOR_LIGHTBLUE,"You got a respect point because is your happy day.");
				HappyDay = 1;
			}
			
			CharityMoney += Tax;
			PlayerInfo[i][pGPoints] += 1;
			Update(i, pGPointsx);
			PlayerInfo[i][pPayDay] = 0;
			Update(i, pPayDayx);
			PlayerInfo[i][pPayCheck] = 0;
			Update(i, pPayCheckx);

			PlayerInfo[i][pRespect] += 1;
			Update(i,pRespectx);

	        if(AFK[i] == 0)
			{
				PlayerInfo[i][pHoursPlayed] += 1;
				Update(i,pHoursPlayedx);
				
				if(PlayerInfo[i][pHoursPlayed] % 25 == 0)
				{
				    format(gString, sizeof(gString), "Get %d hours played", PlayerInfo[i][pHoursPlayed]);
					GiveAchievement(i, 25, gString, 100);
				}
				else UpdateAchievementStatus(i, 25, 0);
			}

			if(PlayerInfo[i][pFPunish] > 0) PlayerInfo[i][pFPunish] -= 1;
			Update(i, pFPunishx);
			
			if(PlayerInfo[i][pPremiumAccount] == 1)
			{
				PlayerInfo[i][pPayDayHad] += 1;
				Update(i,pPayDayHadx);
				if(PlayerInfo[i][pPayDayHad] >= 3)
				{
					PlayerInfo[i][pRespect] += 1;
					Update(i,pRespectx);
					PlayerInfo[i][pPayDayHad] = 0;
					Update(i,pPayDayHadx);
					if(PlayerInfo[i][pFPunish] > 0) PlayerInfo[i][pFPunish] -= 1;
					Update(i, pFPunishx);
				}
			}
			BuyLevelMsg(i);

			if(PlayerInfo[i][pMember] + PlayerInfo[i][pLeader] == 6)
			{
			    new points;
			    if(PlayerInfo[i][pRank] == 1) points = 5;
			    else if(PlayerInfo[i][pRank] == 2) points = 6;
			    else if(PlayerInfo[i][pRank] == 3) points = 7;
			    else if(PlayerInfo[i][pRank] == 4) points = 8;
			    else if(PlayerInfo[i][pRank] == 5) points = 9;
			    else if(PlayerInfo[i][pRank] == 6) points = 10;
			    else if(PlayerInfo[i][pRank] == 7) points = 10;

			    new maxpct = PlayerInfo[i][pRank] * 5 + points;
				if(PlayerInfo[i][pTowPoints] < maxpct) PlayerInfo[i][pTowPoints] += points;
				Update(i, pTowPointsx);
			}
			
			if(PlayerInfo[i][pRob] < 10)PlayerInfo[i][pRob] += 1;
			Update(i, pRobx);
			if(PlayerInfo[i][pFreePoints] < 3 && IsACop(i))PlayerInfo[i][pFreePoints] += 1;
			Update(i, pFreePointsx);
			if(PlayerInfo[i][pDrivingLic] > 0) { PlayerInfo[i][pDrivingLic] -= 1; if(PlayerInfo[i][pDrivingLic] == 0) SendClientMessage(i, 0x359AFFFF, "Your driving license has expired."); }
			else if(PlayerInfo[i][pDrivingLic] < 0) PlayerInfo[i][pDrivingLic] += 1;
			Update(i, pDrivingLicx);
			if(PlayerInfo[i][pFlyingLic] > 0) { PlayerInfo[i][pFlyingLic] -= 1; if(PlayerInfo[i][pFlyingLic] == 0) SendClientMessage(i, 0x359AFFFF, "Your flying license has expired."); }
			else if(PlayerInfo[i][pFlyingLic] < 0) PlayerInfo[i][pFlyingLic] += 1;
			Update(i, pFlyingLicx);
			if(PlayerInfo[i][pSailLic] > 0) { PlayerInfo[i][pSailLic] -= 1; if(PlayerInfo[i][pSailLic] == 0) SendClientMessage(i, 0x359AFFFF, "Your sailing license has expired."); }
			else if(PlayerInfo[i][pSailLic] < 0) PlayerInfo[i][pSailLic] += 1;
			Update(i, pSailLicx);
			
			if(PlayerInfo[i][pGunLic] > 0)
			{
			    if(PlayerInfo[i][pGunLic]-1 <= 0) ResetPlayerWeapons(i);
				PlayerInfo[i][pGunLic] -= 1;
				if(PlayerInfo[i][pGunLic] == 0) SendClientMessage(i, 0x359AFFFF, "Your weapon license has expired.");
			}
			else if(PlayerInfo[i][pGunLic] < 0) PlayerInfo[i][pGunLic] += 1;
			Update(i, pGunLicx);
			
			if(PlayerInfo[i][pGift] > 0) PlayerInfo[i][pGift]--; 
			Update(i, pGiftx);
		}
	}
	
	for(new i = 1; i < sizeof(ZoneInfo); i++)
	{
	    if(ZoneInfo[i][zHours] > 0)
	    {
		    ZoneInfo[i][zHours] -= 1;
			zUpdate(i, zHoursx);
		}
	}
	
	if(NightOn == 0) SetWorldTime(hour);
	else SetWorldTime(0);
 	ActualizareVreme(); // la pornirea gamemodeului
	gQuery[0] = (EOS);
	mysql_format(handle, gQuery, sizeof(gQuery),"UPDATE `others` SET `Charity`=%d WHERE `ID`='1'", CharityMoney);
	mysql_query(handle, gQuery);
	gQuery[0] = (EOS);
	mysql_format(handle, gQuery, sizeof(gQuery), "SELECT * FROM `bans` WHERE `Days` > '0'");
	mysql_tquery(handle, gQuery, "BanCheck", "");
	return 1;
}

function PayDayCheck()
{
  	foreach(new i : Player)
	{
		if(gLogged[i] == 0) continue;
		new level = PlayerInfo[i][pLevel];
		if(level >= 1 && level <= 5) { PlayerInfo[i][pPayCheck] += 200; }
		else if(level >= 6 && level <= 10) { PlayerInfo[i][pPayCheck] += 300; }
		else if(level >= 11 && level <= 15) { PlayerInfo[i][pPayCheck] += 400; }
		else if(level >= 16 && level <= 20) { PlayerInfo[i][pPayCheck] += 500; }
		else if(level >= 21 && level <= 25) { PlayerInfo[i][pPayCheck] += 600; }
		else if(level >= 26 && level <= 30) { PlayerInfo[i][pPayCheck] += 700; }
		else if(level >= 31) { PlayerInfo[i][pPayCheck] += 800; }
		Update(i,pPayCheckx);

		PlayerInfo[i][pPayDay] += 1;
		Update(i, pPayDayx);

		if(PlayerInfo[i][pContractTime] > 0)
		{
			PlayerInfo[i][pContractTime] --;
			Update(i, pContractTimex);
		}
		if(PlayerInfo[i][pMoneyBoost] > 0)
		{
			PlayerInfo[i][pMoneyBoost] --;
			Update(i, pMoneyBoostx);
		}

		new Float: health;
		GetPlayerHealthEx(i, health);
		if(AFK[i] == 0) SetPlayerHealthEx(i, health - 1);
	}
	return 1;
}

function IdleKick()
{
    new nr = 0;
	foreach(new i : Player) nr ++;

 	foreach(new i : Player)
	{
		if(IsAfk[i] == 1 && AFK[i] != 1 && PlayerInfo[i][pLevel] > 1 && PlayerInfo[i][pJailed] == 0 && PlayerInfo[i][pAdmin] < 6 && IsBot[i] == 0)
		{
			AFKMins[i]++;
			if( AFKMins[i] >= AFKTime )
   			{
				format(gString, sizeof(gString), "%s was kicked by AdmBot, reason: AFK without sleep longer than %d minutes.", GetName(i), AFKTime);
				SendClientMessageToAll(COLOR_LIGHTRED, gString);
				Kick(i);
			}
		}
	}
	return 1;
}

function DateDiff(y1,m1,d1,y2,m2,d2)
{
	new ok = 1;
	new countd = 0;
	while(ok)
	{
		new nyear, nmonth, nday;
		CalcDate2(y1, m1, d1, nyear, nmonth, nday, countd);
		if(nyear == y2 && nmonth == m2 && nday == d2) ok = 0;
		countd ++;
	}
	return countd-1;
}

