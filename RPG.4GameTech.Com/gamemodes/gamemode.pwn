	format(gString, sizeof(gString), "* %s takes out a cellphone.", GetName(playerid));
	ProxDetector(20.0, playerid, gString,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);

	new sender = PlayerInfo[playerid][pPhoneNumber];

	new found = -1;
 	foreach(new i : Player)
	{
		if(PlayerInfo[i][pPhoneNumber] == tnum && tnum != 0)
		{
			found = i;
		}
	}
	if(found >= 0)
	{
		if(OnOff[found] == 0 || SpectatorID[found] != INVALID_PLAYER_ID)
		{
			SendClientMessage(playerid, COLOR_ERROR, "Player's cellphone is turned off");
			return 1;
		}
		if(OnCall[found] == 1) return SendClientMessage(playerid, COLOR_ERROR, "You get a busy tone((Player on a call already))");
