function BanCheck()
{
	new rows, fields;
	cache_get_data(rows, fields, handle);

	if(rows)
	{
		new tmp[128];
		new ID, UnbanY, UnbanM, UnbanD, UnbanH;
		new y, m, d, h,mi,s;
		for(new i = 0; i < rows; i++)
		{
			cache_get_field_content(i, "ID", tmp), ID = strval(tmp);
			cache_get_field_content(i, "UnbanY", tmp), UnbanY = strval(tmp);
			cache_get_field_content(i, "UnbanM", tmp), UnbanM = strval(tmp);
			cache_get_field_content(i, "UnbanD", tmp), UnbanD = strval(tmp);
			cache_get_field_content(i, "UnbanH", tmp), UnbanH = strval(tmp);

			getdate(y,m,d);
			gettime(h,mi,s);
			if(y >= UnbanY && m >= UnbanM && d >= UnbanD && h >= UnbanH)
			{
			    gQuery[0] = (EOS);
				mysql_format(handle, gQuery, sizeof(gQuery),"DELETE FROM `bans` WHERE `ID` = %d", ID);
				mysql_query(handle ,gQuery);
			}
		}
	}
	return 1;
}

