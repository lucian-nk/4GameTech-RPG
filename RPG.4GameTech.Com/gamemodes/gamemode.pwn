public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	if(GunTest[playerid] == true && GunO == 1)
	{
		for(new i = 0; i < sizeof(gobj); i++)
		{
			if(!IsValidDynamicObject(gobj[i])) continue;
			new Float:x, Float:y, Float:z;
			GetDynamicObjectPos(gobj[i], x, y, z);
			if ( IsPlayerAimingAt ( playerid, x, y, z, 0.25 ) )
			{
				DestroyDynamicObject(gobj[i]);
				GunScore[playerid] += 1;
				PlayerPlaySound(playerid,17802,0.0,0.0,0.0);

				if(GunScore[playerid] == 5)
				{
					ResetPlayerWeapons(playerid);
					GivePlayerWeapon(playerid,29,99999);
				}
				if(GunScore[playerid] == 10)
				{
					ResetPlayerWeapons(playerid);
					GivePlayerWeapon(playerid,31,99999);
				}
				if(GunScore[playerid] == 15)
				{
					ResetPlayerWeapons(playerid);
					GivePlayerWeapon(playerid,34,99999);
				}
				if(GunScore[playerid] == 20)
				{
					ResetPlayerWeapons(playerid);
					SetPlayerPos(playerid, 2457.6025, -1901.6090, 13.5469);
					GivePlayerCash(playerid, -20000);
					SendClientMessage(playerid, COLOR_SYN2, "Congratulations! Now you have a weapon license.");
					PlayerInfo[playerid][pGunLic] = 100;
					Update(playerid, pGunLicx);
					GunTest[playerid] = false;
					GunScore[playerid] = 0;
					GunO = 0;
					GiveAchievement(playerid, 22, "Get weapon license", 100);
				}
			}
		}
	}
	if(hittype == 2) return 1;
	return 0;
}
