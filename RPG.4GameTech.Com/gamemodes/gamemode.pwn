CMD:live(playerid, params[])
{
	new giveplayerid, splayer[25], price;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader] != 4) return SendClientMessage(playerid, COLOR_ERROR, "You're not part of News Reporters.");
	if(PlayerInfo[playerid][pRank] < 3) return SendClientMessage(playerid, COLOR_ERROR, "You need rank 3 to use this command.");
	if(!IsANRCar(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in a News Reporters vehicle.");
	if(InLive[playerid] == 1)
	{
		SendClientMessage(playerid, COLOR_SYN2, "Live Conversation ended.");
		InLive[playerid] = 0;

    	foreach(new i : Player)
		{
			if(InLiveWith[i] == playerid)
			{
				SendClientMessage(i, COLOR_SYN2, "Live Conversation ended.");
				TogglePlayerControllableEx(i, 1);
				InLiveWith[i] = -1;
				InLive[i] = 0;
			}
		}
		return 1;
	}
	if(sscanf(params,"s[25]i", splayer,price)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /live <Player ID/Name> <price>");
	{
	    if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "You cannot Talk Live with yourself.");
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(MoneyACheck(playerid, price, 1, 500000)) return 1;
		LivePrice[giveplayerid][playerid] = price;
		format(gString, sizeof(gString), "You offered to have a Live Conversation with %s for $%s.",GetName(giveplayerid),FormatNumber(price));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Reporter %s offered you to have a Live Conversation for $%s, type (/accept live %d) to accept.",GetName(playerid),FormatNumber(price),playerid);
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
	}
	return 1;
}

