			SendClientMessage(playerid, COLOR_SYN2, gString);
			format(gString, sizeof(gString), "Leader %s kicked you from %s with 20 faction punish.",GetName(playerid),FactionName[PlayerInfo[playerid][pLeader]]);
			SendClientMessage(giveplayerid, COLOR_SYN2, gString);
			FactionLog(PlayerInfo[playerid][pLeader], GetSQLID(playerid), GetSQLID(giveplayerid), "Was kicked out (with FP)");
		}
        PlayerInfo[giveplayerid][pSpawnChange] = 1;
	    Update(giveplayerid, pSpawnChangex);
		OnPlayerSpawn(giveplayerid);
	}
	return 1;
}

