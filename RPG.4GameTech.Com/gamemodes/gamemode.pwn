CMD:editclan(playerid, params[])
{
	new clanid, edits[8], amount[32];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 9) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"is[8]s[32]",clanid,edits,amount)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /editclan <Clan ID> <name/tag/slots/tagtype(0 before / 1 after)> <amount>");
	{
	    if(strmatch(edits,"name"))
		{
		    if(ClanTMembers(clanid)) return SendClientMessage(playerid, COLOR_ERROR, "Clan is not empty.");
	    	if(strlen(amount) > 32) return SendClientMessage(playerid, COLOR_ERROR, "Clan name is to long.");
		    strmid(ClanInfo[clanid][cName], amount, 0, 32, 32);
		    cUpdate(clanid, cNamex);
		    
		    format(gString, sizeof(gString),"Edited clan %d name: %s", clanid, amount);
			SendClientMessage(playerid, COLOR_SYN2, gString);
		}
		else if(strmatch(edits,"tag"))
		{
		    if(ClanTMembers(clanid)) return SendClientMessage(playerid, COLOR_ERROR, "Clan is not empty.");
		    if(ExistClan(amount)) return SendClientMessage(playerid, COLOR_ERROR, "Clan already exist.");
	    	if(strlen(amount) > 10) return SendClientMessage(playerid, COLOR_ERROR, "Clan tag is to long.");
		    strmid(ClanInfo[clanid][cTag], amount, 0, 10, 10);
		    cUpdate(clanid, cTagx);
		    
		    format(gString, sizeof(gString),"Edited clan %d tag: %s", clanid, amount);
			SendClientMessage(playerid, COLOR_SYN2, gString);
		}
		else if(strmatch(edits,"slots"))
		{
		    ClanInfo[clanid][cSlots] = strval(amount);
		    cUpdate(clanid, cSlotsx);
		    
		    format(gString, sizeof(gString),"Edited clan %d slots: %d", clanid, strval(amount));
			SendClientMessage(playerid, COLOR_SYN2, gString);
		}
  		else if(strmatch(edits,"tagtype"))
		{
		    if(ClanTMembers(clanid)) return SendClientMessage(playerid, COLOR_ERROR, "Clan is not empty.");
		    ClanInfo[clanid][cTagType] = strval(amount);
		    cUpdate(clanid, cTagTypex);
		    
		    format(gString, sizeof(gString),"Edited clan %d tagtype: %d", clanid, strval(amount));
			SendClientMessage(playerid, COLOR_SYN2, gString);
		}
	}
	return 1;
}

CMD:makeclanleader(playerid, params[])
{
	new giveplayerid, splayer[25], leader;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 9) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[25]i",splayer,leader)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /makeclanleader <Player ID/Name> <Clan ID>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[giveplayerid][pCMember] != 0 || PlayerInfo[giveplayerid][pCLeader] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Player is already in a clan.");
		if(strlen(ClanInfo[leader][cName]) < 2) return SendClientMessage(playerid, COLOR_ERROR, "Invalid clan id.");
		
		new name[25];
		GetPlayerName(giveplayerid, name, sizeof(name));
		if(ClanInfo[leader][cTagType] == 0) strins(name, ClanInfo[leader][cTag], 0);
		else strins(name, ClanInfo[leader][cTag], strlen(name));
		if(strlen(name) > 20) return SendClientMessage(playerid, COLOR_ERROR, "Name is to long with tag.");
		if(ExistName(name)) return SendClientMessage(playerid, COLOR_ERROR, "Name already exist.");
		
		format(gString, sizeof(gString), "You have been promoted as leader of clan %s by %s.", ClanInfo[leader][cName], GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN3, gString);
		format(gString, sizeof(gString), "You have promoted %s as leader of clan %s.", GetName(giveplayerid), ClanInfo[leader][cName]);
		SendClientMessage(playerid, COLOR_SYN3, gString);
		
  		if(PlayerInfo[giveplayerid][pHouseKey] > 0)
		{
			strmid(HouseInfo[PlayerInfo[giveplayerid][pHouseKey]][hOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			hUpdate(PlayerInfo[giveplayerid][pHouseKey], hOwnerx);
			HouseUpdate(PlayerInfo[giveplayerid][pHouseKey]);
		}

		if(PlayerInfo[giveplayerid][pBizzKey] > 0)
		{
			strmid(BizzInfo[PlayerInfo[giveplayerid][pBizzKey]][bOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			bUpdate(PlayerInfo[giveplayerid][pBizzKey], bOwnerx);
			BizzUpdate(PlayerInfo[giveplayerid][pBizzKey]);
		}

		for(new i = 1; i <= Total_Veh_Created; i++)
		{
			if(strmatch(vInfo[i][vOwner], GetName(giveplayerid)))
			{
				strmid(vInfo[i][vOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
				vUpdate(i, vOwnerx);
			}
		}
		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Name`='%e' WHERE `ID`=%d", name, PlayerInfo[giveplayerid][pSQLID]);
		mysql_query(handle, gQuery);
		
		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `AName`='%e' WHERE `ID`=%d", GetName(giveplayerid), PlayerInfo[giveplayerid][pSQLID]);
		mysql_query(handle, gQuery);
		
		SetPlayerName(giveplayerid, name);
		TextDrawSetString(NameText[giveplayerid], GetName(giveplayerid));

		PlayerInfo[giveplayerid][pCLeader] = leader;
		Update(giveplayerid, pCLeaderx);
		
		format(gString, sizeof(gString), "%s promoted %s to leader of clan %s.",GetName(playerid), GetName(giveplayerid), ClanInfo[leader][cName]);
		CMDRaport(gString, 5, 0);
	}
	return 1;
}

