public OnPlayerDisconnect(playerid, reason)
{
//==============================================================================
	if(IsTWD == 1)
	{
	    if(HumanClass[playerid] > 0)
	    {
	        HumansInTWD--;
	        HumanClass[playerid] = 0;
	        JoinedTWD[playerid] = 0;
	        SetPlayerTeam(playerid, 255);
	    }
	    if(ZombieClass[playerid] > 0)
	    {
	        ZombieInTWD--;
	        ZombieClass[playerid] = 0;
	        JoinedTWD[playerid] = 0;
	        SetPlayerTeam(playerid, 255);
	    }
	    new string[32];
	    format(string, sizeof(string), "Zombie: ~r~%d", ZombieInTWD);
	    TextDrawSetString(RedScore,string);
	    format(string, sizeof(string), "Humans: ~b~%d", HumansInTWD);
	    TextDrawSetString(BlueScore,string);
	    TextDrawShowForAll(BlueScore);
	    TextDrawShowForAll(RedScore);

	    if(HumansInTWD == 0)
     	{
	        SendClientMessageToAll(COLOR_WHITE, "{66FF33}[The Walking Dead]: {FFFFFF}Zombie au reusit sa infesteze tot orasul!");
	        SendClientMessageToAll(COLOR_WHITE, "{66FF33}[The Walking Dead]: {FFFFFF}Toti zombie au primit 200k + 1 RP + 10 TWD Credits");
	        TextDrawHideForAll(BlueScore);
	        TextDrawHideForAll(RedScore);
	        IsTWD = 0;
	        SetWeather(1);
	        foreach(new i : Player)
			{
				if(IsPlayerConnected(i))
				{
					if(ZombieClass[i] > 0)
					{
	                    GivePlayerMoney(i, 200000);
						PlayerInfo[i][pRespect] += 1;
						ResetPlayerWeapons(i);
						SpawnPlayer(i);
						JoinedTWD[i] = 0;
						ZombieClass[i] = 0;
						GhostZombieAbility[i] = 0;
		                GhostCooldown[i] = 0;
						SetPlayerArmour(i,0);
						PlayerInfo[playerid][pTWDCred] += 10;
						Update(playerid, pTWDCredx);
					}
					if(HumanClass[i] > 0)
					{
						ResetPlayerWeapons(i);
						SpawnPlayer(i);
						JoinedTWD[i] = 0;
						HumanClass[i] = 0;
						SetPlayerArmour(i,0);
					}
				}
	        }
    	}
     	if(ZombieInTWD == 0)
      	{
	        SendClientMessageToAll(COLOR_WHITE, "{66FF33}[The Walking Dead]: {FFFFFF}Oamenii au reusit sa supravietuiasca invaziei de zombie.");
	        SendClientMessageToAll(COLOR_WHITE, "{66FF33}[The Walking Dead]: {FFFFFF}Toti oamenii au primit 200k + 1 RP + 10 TWD Credits!");
	        TextDrawHideForAll(BlueScore);
	        TextDrawHideForAll(RedScore);
	        IsTWD = 0;
	        SetWeather(1);
	        foreach(new i : Player)
			{
				if(IsPlayerConnected(i))
				{
					if(HumanClass[i] > 0)
					{
	                    GivePlayerMoney(i, 200000);
						PlayerInfo[i][pRespect] += 1;
						ResetPlayerWeapons(i);
						SpawnPlayer(i);
						JoinedTWD[i] = 0;
						HumanClass[i] = 0;
						SetPlayerArmour(i,0);
						PlayerInfo[playerid][pTWDCred] += 10;
						Update(playerid, pTWDCredx);
						SetPlayerTeam(i, 255);
					}
					if(ZombieClass[i] > 0)
					{
					    ResetPlayerWeapons(i);
						SpawnPlayer(i);
						JoinedTWD[i] = 0;
						ZombieClass[i] = 0;
						GhostZombieAbility[i] = 0;
		                GhostCooldown[i] = 0;
						SetPlayerArmour(i,0);
						SetPlayerTeam(i, 255);
					}
				}
	       	}
		}
	}
//==============================================================================
    gLogged[playerid] = 0;
    mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Status` = '0' WHERE `ID` = %d", PlayerInfo[playerid][pSQLID]);
	mysql_query(handle, gQuery);
    ConnectionLog(playerid, 2);
    new vehicle = OwnedVeh(GetSpawnedVeh(playerid));
    DespawnVeh(playerid);
    vInfo[vehicle][vSpawned] = 1;
	vUpdate(vehicle, vSpawnedx);
	
	if(SwitchKey[playerid] != -1) SwitchKey[playerid] = -1;
	
	if(eventorganizer[playerid] && activeevent) 
	{
		SendClientMessageToAll(0x7094DBFF, "Event over.");
		activeevent = 0;
	}
	if(JoinedDeathMatch[playerid] > 0)
	{
		if(JoinedDeathMatch[playerid] ==  2)
		{
			if(Echipa2 > 0)
			{
				Echipa2 -= 1;
			}
		}
		else if(JoinedDeathMatch[playerid] ==  1)
		{
			if(Echipa1 > 0)
			{
				Echipa1 -= 1;
			}
		}
	}
	
	if(InRace[playerid])
	{
		if(IsPlayerInAnyVehicle(playerid))
			SetVehicleVirtualWorld(GetPlayerVehicleID(playerid), 1337);
	
		InRace[playerid] = false;
		RacePlayers --;
		
		if(RacePlayers < 4)
		{
   			foreach(new i : Player)
			{
				if(!InRace[i])
					continue;
						
				SendClientMessage(i, COLOR_YELLOW, "The race ended because there are not enough players. (Minimum 2 players)");
				SetPlayerPos(i, BizzInfo[25][bEntranceX], BizzInfo[25][bEntranceY], BizzInfo[25][bEntranceZ]);
				SetPlayerInterior(i,0);
				SetPlayerVirtualWorld(i,0);
				ResetPlayerWeapons(i);
				InRace[i] = false;
				RaceCP[i] = 0;
				RaceVehicle[i] = 0;
				TogglePlayerControllableEx(i, 1);
				DisablePlayerRaceCheckpoint(i);
				break;
			}
			
			for(new j = 0; j < sizeof (race); j++)
			{
				SetVehicleToRespawn(race[j]);
			
				SetVehicleVirtualWorld(race[j], 1337); // Hide vehicles.
				Fuel[race[j]] = 100;
			}
			RaceStarted = false;
			RacePlayers = 0;
		}
	}
	
    requestingevent[playerid] = 0;
	eventorganizer[playerid] = 0;
	
    KillTimer(LoginTimer[playerid]);
    Delete3DTextLabel(label[playerid]);
    
    DeleteReport(playerid);
    if(ReportChat[playerid] != -1)
    {
	    if(ReportChat[ReportChat[playerid]] != -1) ReportChat[ReportChat[playerid]] = -1;
	    ReportChat[playerid] = -1;
    }
    
    DeleteHelp(playerid);
    if(HelpChat[playerid] != -1)
    {
	    if(HelpChat[HelpChat[playerid]] != -1) HelpChat[HelpChat[playerid]] = -1;
	    HelpChat[playerid] = -1;
    }
    
    if(PlayerPaintballing[playerid] != 0) { PaintballPlayers --; if(PaintballPlayers <= 1) PaintballEnded(); }
    
    if(InClanWar[playerid])
    {
        new clan = PlayerInfo[playerid][pCLeader] + PlayerInfo[playerid][pCMember];
        if(clan == ClanWarT1) { ClanWarP1 --; if(ClanWarP1 <= 0) StopClanWar(); }
        if(clan == ClanWarT2) { ClanWarP2 --; if(ClanWarP2 <= 0) StopClanWar(); }
    }
  	foreach(new i : Player)
	{
		if(strmatch(PlayerInfo[i][pContract], GetName(playerid)))
		{
			SendClientMessage(i, COLOR_YELLOW, "Your Hit has left the server, you can wait till he login back or you can use /leavehit.");
		}
	}
	if(PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader] == 7)
	{
	    if(!strmatch(PlayerInfo[playerid][pContract], "No-One"))
		{
		    new id = GetPlayerID(PlayerInfo[playerid][pContract]);
			if(id != INVALID_PLAYER_ID)
			{
			    PlayerInfo[id][pContracted] = 0;
			    Update(id, pContractedx);
			}
			else
			{
			    gQuery[0] = (EOS);
			    mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Contracted`='0' WHERE `Name`='%e'", PlayerInfo[playerid][pContract]);
				mysql_query(handle, gQuery);
			}
			format(PlayerInfo[playerid][pContract], 25, "No-One");
			Update(playerid, pContractx);
		}
	}
	if(GunTest[playerid] == true)
	{
		for(new i = 0; i < sizeof(gobj); i++)
		{
			if(!IsValidDynamicObject(gobj[i])) continue;
			DestroyDynamicObject(gobj[i]);
		}
		GunTest[playerid] = false;
		GunScore[playerid] = 0;
		GunO = 0;
	}
	
	if(TruckV[0][playerid] || TruckV[1][playerid])
	{
	    if(TruckV[0][playerid]) DestroyVehicle(TruckV[0][playerid]);
	    if(TruckV[1][playerid]) DestroyVehicle(TruckV[1][playerid]);
		RadioCH[TruckV[0][playerid]] = 0;
		truckveh[TruckV[0][playerid]] = 0;
		DisablePlayerCheckpoint(playerid);
        CP[playerid] = 0;
        AntiTP[playerid] = 0;
        TruckV[0][playerid] = 0;
        TruckV[1][playerid] = 0;
	}
	
	if(DrivingTest[playerid] == 1)
	{
	    if(DMVVeh[playerid]) DestroyVehicle(DMVVeh[playerid]);
	    RadioCH[DMVVeh[playerid]] = 0;
	    DMVCar[DMVVeh[playerid]] = 0;
	    DMVVeh[playerid] = 0;
		DCheck[playerid] = 0;
		DrivingTest[playerid] = 0;
		DisablePlayerRaceCheckpoint(playerid);
		TextDrawHideForPlayer(playerid, DMVText[playerid]);
	}

	TextDrawHideForPlayer(playerid, WantedTime[playerid]);
	
  	foreach(new i : Player)
	{
	    if(SpectatorID[i] == playerid)
		{
