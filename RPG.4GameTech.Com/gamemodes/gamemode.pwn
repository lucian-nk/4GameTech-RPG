CMD:park(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(!IsPlayerInAnyVehicle(playerid) || !strmatch(vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vOwner], GetName(playerid)) || OwnedVeh(GetPlayerVehicleID(playerid)) == 0 || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in your vehicle.");
	new
	ivID = GetPlayerVehicleID(playerid),
	Float:Pos[4];

	if(strmatch(vInfo[OwnedVeh(ivID)][vOwner], GetName(playerid)))
	{
		GetVehiclePos(ivID, Pos[0], Pos[1], Pos[2]);
		GetVehicleZAngle(ivID, Pos[3]);

		vInfo[OwnedVeh(ivID)][vPosX] = Pos[0];
		vInfo[OwnedVeh(ivID)][vPosY] = Pos[1];
		vInfo[OwnedVeh(ivID)][vPosZ] = Pos[2];
		vInfo[OwnedVeh(ivID)][vPosA] = Pos[3];
		vUpdate(OwnedVeh(ivID), vPosXx);
		vUpdate(OwnedVeh(ivID), vPosYx);
		vUpdate(OwnedVeh(ivID), vPosZx);
		vUpdate(OwnedVeh(ivID), vPosAx);

		new Float:health;
		GetVehicleHealth(ivID, health);

		new
		vCreated = OwnedVeh(ivID);
		OwnedVeh(ivID) = 0;
        if(IsValidObject(neon1[ivID])) DestroyObject(neon1[ivID]);
		if(IsValidObject(neon2[ivID])) DestroyObject(neon2[ivID]);
		DestroyVehicle(ivID);

		new
		NewVeh = CreateVehicle(vInfo[vCreated][vModel], vInfo[vCreated][vPosX], vInfo[vCreated][vPosY], vInfo[vCreated][vPosZ], vInfo[vCreated][vPosA], vInfo[vCreated][vColor1], vInfo[vCreated][vColor2], 500000);
		SetVehicleNumberPlate(NewVeh, vInfo[vCreated][vPlate]);
		ChangeVehiclePaintjob(NewVeh, vInfo[vCreated][vPaintJ]);
		SetVehicleParamsEx(NewVeh,VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
		OwnedVeh(NewVeh) = vCreated;

		ModVehicle(NewVeh);

		PutPlayerInVehicle(playerid,NewVeh,0);
		SetVehicleHealth(NewVeh,health);

		SendClientMessage(playerid, COLOR_SYN2, "Vehicle was parked here.");
	}
	return 1;
}

CMD:vcolor(playerid, params[])
{
	new col1, col2;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(sscanf(params,"ii",col1,col2)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /vcolor <color1> <color2>");
	{
		if(!IsPlayerInAnyVehicle(playerid) || !strmatch(vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vOwner], GetName(playerid)) || OwnedVeh(GetPlayerVehicleID(playerid)) == 0 || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in your vehicle.");
		if((col1 < 0 || col1 > 255) || (col2 < 0 || col2 > 255)) return SendClientMessage(playerid, COLOR_ERROR, "Unknown color.");
		if((col1 > 127 || col2 > 127) && PlayerInfo[playerid][pPremiumAccount] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You need Premium Account for hidden colors.");
		new cost;
		if(col1 > 127 && col2 < 128 || col1 < 128 && col2 > 127) cost = 7500;
		else if(col1 > 127 && col2 > 127) cost = 10000;
		else cost = 5000;

		if(GetPlayerCash(playerid) < cost) return format(gString,126,"You don't have enough money ($%s).", FormatNumber(cost)), SendClientMessage(playerid, COLOR_ERROR, gString);

		if(OwnedVeh(GetPlayerVehicleID(playerid)) != 0)
		{
			if(strmatch(vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vOwner], GetName(playerid)))
			{
				vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vColor1] = col1;
				vInfo[OwnedVeh(GetPlayerVehicleID(playerid))][vColor2] = col2;
				vUpdate(OwnedVeh(GetPlayerVehicleID(playerid)), vColor1x);
				vUpdate(OwnedVeh(GetPlayerVehicleID(playerid)), vColor2x);
				GivePlayerCash(playerid, -cost);
				format(gString,126,"~r~-$%s", FormatNumber(cost));
				GameTextForPlayer(playerid, gString, 5000, 1);
				ChangeVehicleColor(GetPlayerVehicleID(playerid), col1, col2);
				BizzInfo[22][bSafe] += cost;
				bUpdate(22,bSafex);
				BizzPayLog(playerid, 22, cost);
				return 1;
			}
		}
	}
	return 1;
}

