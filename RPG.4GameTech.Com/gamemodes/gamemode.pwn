function SetDestination(playerid, Float:x, Float:y, Float:z)
{
	DestSet[playerid][0] = x;
	DestSet[playerid][1] = y;
	DestSet[playerid][2] = z;
	DestinationSet[playerid] = 1;
}
function RemoveDestination(playerid)
{
	DestSet[playerid][0] = 0.0;
	DestSet[playerid][1] = 0.0;
	DestSet[playerid][2] = 0.0;
	DestinationSet[playerid] = 0;
}

function CheckCar()
{
 	foreach(new i : Player)
	{
	    if(GetPlayerState(i) == PLAYER_STATE_DRIVER && SpeedLimiter[i] > 0 && GetPlayerSpeed(i, 0) > SpeedLimiter[i])
		{
			new newspeed = GetPlayerSpeed(i, 0) - SpeedLimiter[i];
			ModifyVehicleSpeed(GetPlayerVehicleID(i), - newspeed);
		}
		
		if(IsPlayerInAnyVehicle(i))
		{
			new car = GetPlayerVehicleID(i);
			new Speed, zone[30];
			if(GetVehicleType(car) == 5 || GetVehicleType(car) == 9) Speed = GetPlayerSpeed(i,1);
			else Speed = GetPlayerSpeed(i,0);
		    new oldspeed = Speed;
			new newspeed = Speed;
			new acceleration = abs(newspeed - oldspeed);
			new Consume = (((acceleration / 10) + 1) * (newspeed / 20));
			//new Float:Consume2 = float(Consume);
			
			if(Speed <= 110)
			{
				format(gString,sizeof(gString),"Speed: ~y~%d ~w~KM/H",Speed);
				TextDrawSetString(Speedo[1][i], gString);
			}
			if(Speed > 110 && Speed <= 160)
			{
				format(gString,sizeof(gString),"Speed: ~b~%d ~w~KM/H",Speed);
				TextDrawSetString(Speedo[1][i], gString);
			}
			if(Speed > 160)
			{
				format(gString,sizeof(gString),"Speed: ~r~%d ~w~KM/H",Speed);
				TextDrawSetString(Speedo[1][i], gString);
			}

			/*format(gString, sizeof(gString), "Speed: %d Km/h", Speed);
			TextDrawSetString(Speedo[1][i], gString);
			
			format(gString, sizeof(gString), "Fuel: %d%", Fuel[car]);
			TextDrawSetString(Speedo[0][i], gString);*/
			

			format(gString,sizeof(gString),"Fuel: %d % (C: %d %%)", Fuel[car], Consume); //
			TextDrawSetString(Speedo[0][i], gString);
			if(DestinationSet[i] == 0)
			{
				format(gString,sizeof(gString),"Destination: None"); //
				TextDrawSetString(Destination[i], gString);
			}
			else
			{
				GetPlayer3DZone2(DestSet[i][0],DestSet[i][1],DestSet[i][2],zone,30);
				format(gString,sizeof(gString),"Destination: ~y~%s",zone); //
				TextDrawSetString(Destination[i], gString);

				new Float:distance = GetPlayerDistanceFromPoint(i, DestSet[i][0],DestSet[i][1],DestSet[i][2]);
				format(gString,sizeof(gString),"              ~y~%.2f meters",distance); //
				TextDrawSetString(DestinationKM[i], gString);
				TextDrawShowForPlayer(i,DestinationKM[i]);
			}

			if(RadioSet[i] == 0)
			{
				format(gString,sizeof(gString),"Radio: ~r~OFF"); //
				TextDrawSetString(Radio[i], gString);
			}
			else
			{
				format(gString,sizeof(gString),"Radio: ~y~%s",RadioName(RadioSet[i])); //
				TextDrawSetString(Radio[i], gString);
			}
			if(OwnedVeh(car) != 0 && OdoVehicle(GetVehicleModel(car)) == 1)
			{
				format(gString, sizeof(gString), "Odometer: %0.1f Km", vInfo[OwnedVeh(car)][vKm]);
				TextDrawSetString(Speedo[2][i], gString);
			}
			if(Speed > 235 && GetVehicleType(car) != 5 && GetVehicleType(car) != 9 && GetPlayerState(i) == PLAYER_STATE_DRIVER)
			{
				if(SHB[i] < gettime())
				{
				    format(gString, sizeof(gString), "AdmWarning:{FFFFFF} %s caught %d km/h with a %s.",GetName(i), Speed, VehicleNames[GetVehicleModel(car) - 400]);
					ABroadCast(COLOR_LIGHTRED, gString, 1);
					format(gString, sizeof(gString), "speed hack (%d km/h with %s)", Speed, VehicleNames[GetVehicleModel(car) - 400]);
					HackLog(i, gString);
				}
				SHB[i] = gettime()+5;
			}
		}
    	if(SpectatorID[i] != INVALID_PLAYER_ID)
		{
			if(GetPlayerInterior(i) != GetPlayerInterior(SpectatorID[i])) SetPlayerInterior(i, GetPlayerInterior(SpectatorID[i]));
			if(GetPlayerVirtualWorld(i) != GetPlayerVirtualWorld(SpectatorID[i])) SetPlayerVirtualWorld(i, GetPlayerVirtualWorld(SpectatorID[i]));

			if((GetPlayerState(SpectatorID[i]) == PLAYER_STATE_DRIVER || GetPlayerState(SpectatorID[i]) == PLAYER_STATE_PASSENGER) && OldState[SpectatorID[i]] != GetPlayerState(SpectatorID[i])) PlayerSpectateVehicle(i, GetPlayerVehicleID(SpectatorID[i]));
			else if(GetPlayerState(SpectatorID[i]) == PLAYER_STATE_ONFOOT && OldState[SpectatorID[i]] != GetPlayerState(SpectatorID[i])) PlayerSpectatePlayer(i, SpectatorID[i]);
			OldState[SpectatorID[i]] = GetPlayerState(SpectatorID[i]);

			new specstring[210], Float:shp, Float:sap, Float:vhp, temp2[70], temp3[60];
			GetPlayerHealthEx(SpectatorID[i], shp);
			GetPlayerArmourEx(SpectatorID[i], sap);
			format(specstring, sizeof(specstring), "~r~%s(%d)~w~ (%.1fHP/%.1fAP)", GetName(SpectatorID[i]), SpectatorID[i], shp, sap);
			if(GetPlayerWeapon(SpectatorID[i]) > 0 && GetPlayerAmmo(SpectatorID[i]) > 0) { format(temp2, sizeof(temp2), "~n~Weapon: ~r~%d ~w~Ammo: ~r~%d", GetPlayerWeapon(SpectatorID[i]), GetPlayerAmmo(SpectatorID[i])); strcat(specstring, temp2); }
