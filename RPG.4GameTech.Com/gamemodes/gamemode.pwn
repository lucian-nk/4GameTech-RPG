CMD:id(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(sscanf(params,"s[25]", splayer)) return SendClientMessage(playerid, COLOR_SYN,"Synthax:{FFFFFF} /id <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);

		new status[12];
		if(AFK[giveplayerid] == 1) format(status, 12, "Sleep");
		else if(AFK[giveplayerid] == 0 && IsAfk[giveplayerid] == 1 && AFKMins[giveplayerid] > 2) format(status, 12, "AFK");
		else format(status, 12, "Active");
		
		new factionname[64];
		format(factionname, sizeof(factionname), "%s", FactionName[PlayerInfo[giveplayerid][pLeader] + PlayerInfo[giveplayerid][pMember]]);
		
		new rankname[8];
		if(PlayerInfo[giveplayerid][pLeader] != 0) format(rankname, sizeof(rankname), "Leader");
		else format(rankname, sizeof(rankname), "%d", PlayerInfo[giveplayerid][pRank]);
		
		if(Undercover[giveplayerid] == 1)
		{
		    format(factionname, sizeof(factionname), "None");
		    format(rankname, sizeof(rankname), "0");
		}
	    format(gString, sizeof(gString), "ID: %d | Nick: %s | Level: %d | Faction: %s(%s) | Status: %s",giveplayerid,GetName(giveplayerid),PlayerInfo[giveplayerid][pLevel],factionname,rankname,status);
		SendClientMessage(playerid, COLOR_YELLOW2, gString);
	}
	return 1;
}

CMD:eject(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(sscanf(params,"s[25]", splayer)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /eject <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		
		new vid = GetPlayerVehicleID(playerid);
		if(GetPlayerState(playerid) != 2) return SendClientMessage(playerid, COLOR_ERROR, "You are not driver.");
        if(!IsPlayerInVehicle(giveplayerid, vid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not in your vehicle.");
        
        format(gString, sizeof(gString), "You have ejected player %s from your vehicle.", GetName(giveplayerid));
