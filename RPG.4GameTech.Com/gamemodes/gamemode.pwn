stock GetNearestVehicle(playerid, Float: range) {
	new
		vehicle = INVALID_VEHICLE_ID,
		Float: dist,
		Float: max_dist = FLOAT_INFINITY,
		Float: x, Float: y, Float: z;
		
	for(new i = 1; i < MAX_VEHICLES; i++)
	{
		if(!GetVehicleModel(i))
			continue;
	
		GetVehiclePos(i, x, y, z);
		
		dist = GetPlayerDistanceFromPoint(playerid, x, y, z);
		
		if (dist <= range && dist < max_dist) {
			vehicle = i;
			max_dist = dist;
			continue;
		}
	}
	return vehicle;
}

stock GetClosestVehicle(playerid, Float:range, deny)
{
	new     Float:p_X;
	new     Float:p_Y;
	new     Float:p_Z;

	new     Float:Distance;
	new     Float:PretendentDistance = range +1;
	new     Pretendent = 0;

	GetPlayerPos(playerid, p_X, p_Y, p_Z);

	for(new vehicleid = 1; vehicleid < MAX_VEHICLES; vehicleid++)
	{
		if(GetVehicleModel(vehicleid) && vehicleid != deny)
		{
			Distance = GetVehicleDistanceFromPoint(vehicleid, p_X, p_Y, p_Z);

			if(Distance <= range && Distance <= PretendentDistance)
			{
				Pretendent = vehicleid;
				PretendentDistance = Distance;
			}
		}
	}

	return Pretendent;
}

