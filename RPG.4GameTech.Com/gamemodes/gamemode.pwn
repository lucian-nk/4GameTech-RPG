	return 1;
}

function ReferralCheck(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		new rows, fields;
		new tmp[128];
		cache_get_data(rows, fields, handle);

		if(rows != 0)
		{
		    new name[25], id, registred;
		    cache_get_field_content(0, "ID", tmp),  id = strval(tmp);
		    cache_get_field_content(0, "Registred", tmp),  registred = strval(tmp);
		    cache_get_field_content(0, "Name", tmp),  format(name, 64, tmp);
		    if(strmatch(name, GetName(playerid)))
		    {
   				if(PlayerInfo[playerid][pLimba] == 1) // Engleza
			    {
					format(gString, sizeof(gString), "You can't be your referral, choose another referral name:");
					ShowPlayerDialog(playerid, DIALOG_REFERRAL, DIALOG_STYLE_INPUT, "Referral", gString, "Enter", "Skip");
				}
			    if(PlayerInfo[playerid][pLimba] == 2) // Romana
			    {
			    	format(gString, sizeof(gString), "Tu nu poti fii referral-ul tau! Te rugam introdu alt nume:");
					ShowPlayerDialog(playerid, DIALOG_REFERRAL, DIALOG_STYLE_INPUT, "Referral", gString, "Enter", "Skip");
				}
				return 1;
		    }
		    if(registred == 0)
		    {
  				if(PlayerInfo[playerid][pLimba] == 1) // Engleza
			    {
			        format(gString, sizeof(gString), "This player is not registered, choose another referral name:");
					ShowPlayerDialog(playerid, DIALOG_REFERRAL, DIALOG_STYLE_INPUT, "Referral", gString, "Enter", "Skip");
				}
			    if(PlayerInfo[playerid][pLimba] == 2) // Romana
			    {
			        format(gString, sizeof(gString), "Acest jucator nu exista, alege alt referral:");
					ShowPlayerDialog(playerid, DIALOG_REFERRAL, DIALOG_STYLE_INPUT, "Referral", gString, "Enter", "Skip");
				}
				return 1;
		    }
   			if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		    {
			    format(gString, sizeof(gString), "You have been brought here by %s ?", name);
				ShowPlayerDialog(playerid, DIALOG_REFERRAL_CHECK, DIALOG_STYLE_MSGBOX,"Referral", gString, "Yes", "No");
			}
		    if(PlayerInfo[playerid][pLimba] == 2) // Romana
		    {
			    format(gString, sizeof(gString), "Esti sigur ca ai fost adus pe server de %s ?", name);
				ShowPlayerDialog(playerid, DIALOG_REFERRAL_CHECK, DIALOG_STYLE_MSGBOX,"Referral", gString, "Da", "Nu");
			}
			ReferralID[playerid] = id;
			format(ReferralName[playerid], 64, name);
		}
		else
		{
   			if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		    {
			    format(gString, sizeof(gString), "Retype name more carefully:");
				ShowPlayerDialog(playerid, DIALOG_REFERRAL, DIALOG_STYLE_INPUT, "Referral", gString, "Enter", "Skip");
			}
		    if(PlayerInfo[playerid][pLimba] == 2) // Romana
		    {
			    format(gString, sizeof(gString), "Rescrie numele cu mai multa atentie!:");
				ShowPlayerDialog(playerid, DIALOG_REFERRAL, DIALOG_STYLE_INPUT, "Referral", gString, "Enter", "Skip");
			}
		}
	}
	return 1;
}

function SetPlayerSpawn(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		SetPlayerTeam(playerid, 4);
		SetPlayerArmourEx(playerid, 0);
		SetPlayerHealthEx(playerid, 100);
		ResetPlayerWeapons(playerid);
		SetPlayerSkin(playerid, PlayerInfo[playerid][pSkin]);
		TeamColor(playerid);
		
		if(SpectatorID[playerid] != INVALID_PLAYER_ID) SpectatorID[playerid] = INVALID_PLAYER_ID, PlayerTextDrawHide(playerid, SpecText[playerid]);
		
		if(BeforeSpectate[playerid][pSpectating]) // iFordawin
		{
			SetPlayerVirtualWorld(playerid, BeforeSpectate[playerid][pWorld]);
			SetPlayerInterior(playerid, BeforeSpectate[playerid][pInt]);

			if(BeforeSpectate[playerid][pState] == PLAYER_STATE_DRIVER)
				PutPlayerInVehicle(playerid, BeforeSpectate[playerid][pInVehicle], 0);
			
			else if(BeforeSpectate[playerid][pState] == PLAYER_STATE_PASSENGER)
				PutPlayerInVehicle(playerid, BeforeSpectate[playerid][pInVehicle], 1 + random(3));
				
			else
				SetPlayerPos(playerid, BeforeSpectate[playerid][pOldPos][0], BeforeSpectate[playerid][pOldPos][1], BeforeSpectate[playerid][pOldPos][2]);
			
			BeforeSpectate[playerid][pSpectating] = false;
			return 1;
		}
       if(JoinedDeathMatch[playerid] > 0)
		{
			JoinedDeathMatch[playerid] = 0;
			SetPlayerSkin(playerid, PlayerInfo[playerid][pSkin]);
			ResetPlayerWeapons(playerid);
		}
		if(PlayerInfo[playerid][pSpawnChange] == 1 || IsInWar1(playerid) || IsInWar2(playerid)) FactionSpawn(playerid);
		else if(PlayerInfo[playerid][pSpawnChange] == 2)
		{
			if(PlayerInfo[playerid][pHouseKey] != 0 || PlayerInfo[playerid][pRentKey] != 0)
			{
				new h = PlayerInfo[playerid][pHouseKey] + PlayerInfo[playerid][pRentKey];
				SetPlayerInterior(playerid,HouseInfo[h][hInt]);
				SetPlayerVirtualWorld(playerid,HouseInfo[h][hWorld]);
				SetPlayerPos(playerid,HouseInfo[h][hExitX],HouseInfo[h][hExitY],HouseInfo[h][hExitZ]);
    			HouseEntered[playerid] = h;
			}
			else
			{
			    PlayerInfo[playerid][pSpawnChange] = 1;
				Update(playerid, pSpawnChangex);
				SetPlayerSpawn(playerid);
			}
		}
		else if(PlayerInfo[playerid][pSpawnChange] == 3)
		{
		    new clan = PlayerInfo[playerid][pCMember] + PlayerInfo[playerid][pCLeader];
			if(ClanInfo[clan][cHQType] == 1)
			{
			    new h = ClanInfo[clan][cHouseHQ];
			    SetPlayerInterior(playerid,HouseInfo[h][hInt]);
				SetPlayerVirtualWorld(playerid,HouseInfo[h][hWorld]);
				SetPlayerPos(playerid,HouseInfo[h][hExitX],HouseInfo[h][hExitY],HouseInfo[h][hExitZ]);
    			HouseEntered[playerid] = h;
			}
			else
			{
			    if(PlayerInfo[playerid][pHouseKey] != 0 || PlayerInfo[playerid][pRentKey] != 0) PlayerInfo[playerid][pSpawnChange] = 2;
			    else PlayerInfo[playerid][pSpawnChange] = 1;
				Update(playerid, pSpawnChangex);
				SetPlayerSpawn(playerid);
			}
		}
		if(PlayerInfo[playerid][pDrivingLic] > 0 && PlayerInfo[playerid][pTutVeh] == 0)
		{
		    if(PlayerInfo[playerid][pLevel] < 3) TutVehCreate(playerid, 462);
		    else if(PlayerInfo[playerid][pLevel] >= 3 && PlayerInfo[playerid][pLevel] < 5) TutVehCreate(playerid, 401);
		    else if(PlayerInfo[playerid][pLevel] >= 5) TutVehCreate(playerid, 445);
		}

		if(TurfsON[playerid]) ShowZones(playerid);

		if(HoldWood[playerid] == 1)
		{
			DisablePlayerCheckpoint(playerid);
			CP[playerid] = 0;
			if(IsPlayerAttachedObjectSlotUsed(playerid,1)) RemovePlayerAttachedObject(playerid,1);
			ClearAnimations(playerid);
			HoldWood[playerid] = 0;
		}
		
		if(HoldPizza[playerid] == 1 || TakenPizza[playerid] == 1)
		{
			HoldPizza[playerid] = 0;
			TakenPizza[playerid] = 0;
			if(IsPlayerAttachedObjectSlotUsed(playerid, 0)) RemovePlayerAttachedObject(playerid, 0);
		    if(IsPlayerAttachedObjectSlotUsed(playerid, 1)) RemovePlayerAttachedObject(playerid, 1);
		    if(IsPlayerAttachedObjectSlotUsed(playerid, 2)) RemovePlayerAttachedObject(playerid, 2);
		    if(IsPlayerAttachedObjectSlotUsed(playerid, 3)) RemovePlayerAttachedObject(playerid, 3);
		    if(IsPlayerAttachedObjectSlotUsed(playerid, 4)) RemovePlayerAttachedObject(playerid, 4);
			ClearAnimations(playerid);
		}
		
		if(PlayerPaintballing[playerid] != 0)
		{
		    ResetPlayerWeapons(playerid);
			SetPlayerArmourEx(playerid,0);
			SetPlayerHealthEx(playerid,100);
            GivePlayerWeaponEx(playerid, pgunp, 9999);
		    new randoo = random(sizeof(PaintballSpawns));
			SetPlayerPos(playerid, PaintballSpawns[randoo][0], PaintballSpawns[randoo][1], PaintballSpawns[randoo][2]);
			SetPlayerHealthEx(playerid, 10000);
			SetTimerEx("paintinv", 3000, false, "i", playerid);
			TogglePlayerControllableEx(playerid, 0);
			SetTimerEx("paintfix", 1500, false, "i", playerid);
   			SetPlayerInterior(playerid,1);
			SetPlayerVirtualWorld(playerid,1);
		}
		
  		if(InClanWar[playerid])
		{
		    ResetPlayerWeapons(playerid);
			SetPlayerArmourEx(playerid,0);
			SetPlayerHealthEx(playerid,100);
			
	   		new clan = PlayerInfo[playerid][pCLeader] + PlayerInfo[playerid][pCMember];
		    if(clan == ClanWarT1)
		    {
		        new rando = random(3);
		        SetPlayerInterior(playerid, 10);
				SetPlayerPos(playerid, ClanWarSpawn[rando][0], ClanWarSpawn[rando][1], ClanWarSpawn[rando][2]);
				TogglePlayerControllableEx(playerid, 1);
		    }
		    else if(clan == ClanWarT2)
		    {
		        new rando = random(3) + 3;
		        SetPlayerInterior(playerid, 10);
				SetPlayerPos(playerid, ClanWarSpawn[rando][0], ClanWarSpawn[rando][1], ClanWarSpawn[rando][2]);
				TogglePlayerControllableEx(playerid, 1);
			}
			GivePlayerWeaponEx(playerid, 24, 99999);
			GivePlayerWeaponEx(playerid, 31, 99999);
			GivePlayerWeaponEx(playerid, 32, 99999);
			GivePlayerWeaponEx(playerid, 33, 99999);
			SetPlayerVirtualWorld(playerid,0);
		}

		if(PlayerInfo[playerid][pJailed] == 1)
		{
			SetPlayerInterior(playerid, 1);
			new randp = random(sizeof(JailSpawn));
			SetPlayerPos(playerid, JailSpawn[randp][0], JailSpawn[randp][1], JailSpawn[randp][2]);
			TogglePlayerControllableEx(playerid, 0);
			SetTimerEx("unfr", 1500, false, "i", playerid);
			SendClientMessage(playerid, COLOR_LIGHTRED, "Incomplete Jail Sentence, back to jail.");
			TextDrawShowForPlayer(playerid, JailTime[playerid]);
			SetPlayerVirtualWorld(playerid, 0);
			ResetPlayerWeapons(playerid);
		}
		else
		{
			TextDrawHideForPlayer(playerid, JailTime[playerid]);
		}
		
		if(PlayerInfo[playerid][pAJailed] == 1)
		{
			SetPlayerInterior(playerid, 10);
			SetPlayerPos(playerid, 215.3533, 110.3420, 999.0156);
			TextDrawShowForPlayer(playerid, AJailTime[playerid]);
			SetPlayerVirtualWorld(playerid, 0);
			ResetPlayerWeapons(playerid);
		}
		else
		{
			TextDrawHideForPlayer(playerid, AJailTime[playerid]);
		}
	}
	return 1;
}

function PTree(t, playerid)
{
	new Float:x, Float:y, Float:z;
	GetDynamicObjectPos(Tree[t], x, y, z);
	DestroyDynamicObject(Tree[t]);
	Tree[t] = CreateDynamicObject(838, x, y, z,   356.85840, 0.00000, -0.87929);

	SetTimerEx("RTree", 30000, 0, "i", t);

	if(!IsPlayerConnected(playerid)) return 1;
	ResetPlayerWeapons(playerid);
	SetPlayerAttachedObject(playerid, 1, 1463, 1,0.20,0.36,0.0,0.0,90.0, 0.0, 0.4, 0.3, 0.6);
	ApplyAnimation(playerid,"CARRY","crry_prtial",4.0,1,0,0,1,1,1);

    UseFind[playerid] = 0;
    IsTowing[playerid] = 0;
	SetPlayerCheckpoint(playerid,2352.1277,-643.2382,128.3634,1.0);
	HoldWood[playerid] = 1;
	CutWood[playerid] = 0;
	CP[playerid] = 110;
	return 1;
}

function RTree(tree)
