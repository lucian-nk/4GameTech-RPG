				GivePlayerCash(playerid, cash);
				HideTruckerTD(playerid);
			}
			else
	    	{
	    	    HideTruckerTD(playerid);
	    	    SendClientMessage(playerid, COLOR_SYN2, "No trailer attached.");
		    	DisablePlayerCheckpoint(playerid);
		    	CP[playerid] = 0;
		    	if(TruckV[1][playerid]) DestroyVehicle(TruckV[1][playerid]);
		    	TruckV[1][playerid] = 0;

	    	}
		}
	}
	return 1;
}

public OnPlayerEnterRaceCheckpoint(playerid)
{
	if(RaceCP[playerid] && InRace[playerid] && IsPlayerInVehicle(playerid, RaceVehicle[playerid])) // Race System
	{
		if(RaceCP[playerid] < sizeof (RaceCheckpoints) - 2)
			SetPlayerRaceCheckpoint(playerid, 0, RaceCheckpoints[RaceCP[playerid] - 1][0], RaceCheckpoints[RaceCP[playerid] - 1][1], RaceCheckpoints[RaceCP[playerid] - 1][2], RaceCheckpoints[RaceCP[playerid]][0], RaceCheckpoints[RaceCP[playerid]][1], RaceCheckpoints[RaceCP[playerid]][2], 3.5);
			
		else if(RaceCP[playerid] == sizeof (RaceCheckpoints) - 2)
			SetPlayerRaceCheckpoint(playerid, 1, RaceCheckpoints[RaceCP[playerid] - 1][0], RaceCheckpoints[RaceCP[playerid] - 1][1], RaceCheckpoints[RaceCP[playerid] - 1][2], 0.0, 0.0, 0.0, 3.5);
			
		else if(RaceCP[playerid] == sizeof (RaceCheckpoints) - 1) // Finish
		{
			new
				prize = random(10);
				
			switch(prize)
			{
				case 0: 
				{
					format(gString, sizeof (gString), "The race has been finished. %s is our winner. He won 1 respect point.", GetName(playerid));
				
					PlayerInfo[playerid][pRespect] ++;
					Update(playerid, pRespectx);
				}
				case 1 .. 9:
				{
					format(gString, sizeof (gString), "The race has been finished. %s is our winner. He won $10.000.", GetName(playerid));
					GivePlayerCash(playerid, 25000);
				}
			}
		
			foreach (Player, i)
			{
			    if(InRace[i] == true)
			    {
					SendClientMessage(i, COLOR_YELLOW, gString);
					SetPlayerPos(i, BizzInfo[25][bEntranceX], BizzInfo[25][bEntranceY], BizzInfo[25][bEntranceZ]);
					SetPlayerInterior(i,0);
					SetPlayerVirtualWorld(i,0);
					ResetPlayerWeapons(i);
					InRace[i] = false;
					DisableRemoteVehicleCollisions(playerid, 0);
					RaceCP[i] = 0;
					RaceVehicle[i] = 0;
					TogglePlayerControllableEx(i, 1);
					DisablePlayerRaceCheckpoint(i);
				}
			}
			
			for(new j = 0; j < sizeof (race); j++) 
			{
				SetVehicleToRespawn(race[j]);
			
				Fuel[race[j]] = 100;
				SetVehicleVirtualWorld(race[j], 1337); // Hide vehicles.
			}
			
			RaceStarted = false;
			RacePlayers = 0;
			RaceCountdown = 0;
		}
			
		RaceCP[playerid] ++;
	}
	new sendernamen[MAX_PLAYER_NAME];
	GetPlayerName(playerid, sendernamen, sizeof(sendernamen));
	if(CP[playerid] == 764)
	{
	    new string[182];
	    DisablePlayerRaceCheckpoint(playerid);
	    if(MotoCrossPremiu == 0)
	    {
	    	MotoCrossPremiu++;
	    	InscrisiMotoCross--;
	    	format(string, sizeof(string), "Event Race: %s a terminat circuitul pe primul loc! A castigat $65.000", sendernamen);
			SendClientMessageToAll(COLOR_WHITE,string);
			GivePlayerCash(playerid, 65000);
			Update(playerid, pCashx);
			DisablePlayerRaceCheckpoint(playerid);
			if(InscrisiMotoCross == 0)
			{
			    foreach(Player, i)
			    {
			        JoinedMotoCross[i] = 0;
			    }
			    for(new i =1; i<=MAX_PLAYERS;i++)
			    {
			    	SetVehicleToRespawn(MotoCros[i]);
			    }
			}
		}
		else if(MotoCrossPremiu == 1)
		{
		    MotoCrossPremiu++;
		    format(string, sizeof(string), "Event Race: %s a terminat circuitul pe locul II! A castigat $50.000", sendernamen);
			SendClientMessageToAll(COLOR_WHITE,string);
			GivePlayerCash(playerid, 50000);
			Update(playerid, pCashx);
			InscrisiMotoCross--;
			DisablePlayerRaceCheckpoint(playerid);
			if(InscrisiMotoCross == 0)
			{
			    foreach(Player, i)
			    {
