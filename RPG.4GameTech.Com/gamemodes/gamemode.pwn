	new count = 0;
	foreach(new i : Player)
	{
		if(PlayerInfo[i][pJailed] > 0)
		{
			format(gString, sizeof(gString), "%s(%d) - Jail time life: [%d].",GetName(i),i,PlayerInfo[i][pJailTime]);
			SendClientMessage(playerid, COLOR_WHITE, gString);
			count++;
		}
	}
	if(count == 0) return SendClientMessage(playerid, COLOR_LIGHTGREEN, "There are no players jailed.");
	else if(count == 1) return SendClientMessage(playerid, COLOR_LIGHTGREEN, "Is only one player in jail.");
	else if(count > 1) format(gString, sizeof(gString), "There are %d players in jail.",count), SendClientMessage(playerid, COLOR_LIGHTGREEN,gString);
	return 1;
}

