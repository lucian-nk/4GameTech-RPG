			PlayerInfo[giveplayerid][pMats] -= SellMatsAmount[playerid][giveplayerid];
			Update(giveplayerid, pMatsx);
			
			PlayerInfo[playerid][pMats] += SellMatsAmount[playerid][giveplayerid];
			Update(playerid, pMatsx);
			
			GivePlayerCash(playerid, - SellMatsPrice[playerid][giveplayerid]);
			GivePlayerCash(giveplayerid, SellMatsPrice[playerid][giveplayerid]);
			
