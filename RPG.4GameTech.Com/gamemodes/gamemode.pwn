function CreateVeh(playerid, dealer, id, type)
{
	if(PlayerInfo[playerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_ERROR, "You need level 3.");
	if(GetVehNumber(playerid) >= PlayerInfo[playerid][pVehicleSlots]) return SendClientMessage(playerid, COLOR_ERROR, "You have vehicle slots full.");

	new price, model, stok;
	if(dealer == 1)
	{
	    if(PlayerInfo[playerid][pDrivingLic] <= 0) return SendClientMessage(playerid, COLOR_ERROR, "You don't have driving license.");
		price = D1Info[id][d1Price];
		model = D1Info[id][d1Model];
