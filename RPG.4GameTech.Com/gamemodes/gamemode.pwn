CMD:rentroom(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pHouseKey] != 0) return SendClientMessage(playerid, COLOR_ERROR, "You already own a house, type /sellhouse if you want to rent this one.");
	for(new h = 1; h < sizeof(HouseInfo); h++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 2.0, HouseInfo[h][hEntranceX], HouseInfo[h][hEntranceY], HouseInfo[h][hEntranceZ]) && HouseInfo[h][hOwned] == 1 &&  HouseInfo[h][hRentable] == 1)
		{
			if(GetPlayerCash(playerid) < HouseInfo[h][hRent]) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money.");
			PlayerInfo[playerid][pRentKey] = h;
			Update(playerid,pRentKeyx);
			GivePlayerCash(playerid,-HouseInfo[h][hRent]);

			HouseInfo[h][hSafe] = HouseInfo[h][hSafe]+HouseInfo[h][hRent];
			hUpdate(h, hSafex);
			
			RentroomLog(playerid, h, HouseInfo[h][hRent]);

			SetPlayerInterior(playerid,HouseInfo[h][hInt]);
			SetPlayerVirtualWorld(playerid,HouseInfo[h][hWorld]);
			SetPlayerPos(playerid,HouseInfo[h][hExitX],HouseInfo[h][hExitY],HouseInfo[h][hExitZ]);
			
			PlayerInfo[playerid][pSpawnChange] = 2;
		    Update(playerid, pSpawnChangex);
		    
		    HouseEntered[playerid] = h;

			GameTextForPlayer(playerid, "~w~Welcome Home~n~You can exit at any time by moving to this door and typing /exit", 5000, 3);

			SendClientMessage(playerid, COLOR_SYN2, "Congratulations, You can enter and exit here anytime.");
			SendClientMessage(playerid, COLOR_SYN2, "Type /renthelp to review the new property help section.");
			GiveAchievement(playerid, 6, "Rent a room", 100);
		}
	}
	return 1;
}

