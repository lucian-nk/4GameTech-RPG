CMD:editbiz(playerid, params[])
{
	new edits[24],amount;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 6) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[24]i", edits,amount)) { SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /editbiz <name> <amount>"); SendClientMessage(playerid, -1, "Available names: Level Price Fee ItemPrice Lock"); return 1; }
	{
		for(new i = 1; i <= businesses_count; i++)
		{
			if (IsPlayerInRangeOfPoint(playerid, 1.5, BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ]))
			{
				if(strmatch(edits,"level"))
				{
					BizzInfo[i][bLevel] = amount;
					bUpdate(i, bLevelx);
					BizzUpdate(i);
					format(gString,sizeof(gString),"Level of business %d changed to %d.",i,amount);
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"price"))
				{
					BizzInfo[i][bPrice] = amount;
					bUpdate(i, bPricex);
					BizzUpdate(i);
					format(gString,sizeof(gString),"Price of business %d changed to $%s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"fee"))
				{
					BizzInfo[i][bEntranceCost] = amount;
					bUpdate(i, bEntranceCostx);
					BizzUpdate(i);
					format(gString,sizeof(gString),"Fee of business %d changed to $%s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"itemprice"))
				{
					BizzInfo[i][bItemPrice] = amount;
					bUpdate(i, bItemPricex);
					BizzUpdate(i);
					format(gString,sizeof(gString),"Item price of business %d changed to $%s.",i,FormatNumber(amount));
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else if(strmatch(edits,"lock"))
				{
					BizzInfo[i][bLocked] = amount;
					bUpdate(i, bLockedx);
					BizzUpdate(i);
					format(gString,sizeof(gString),"Lock of business %d changed to %d.",i,amount);
					SendClientMessage(playerid, COLOR_GRAD3, gString);
				}
				else return 1;
				format(gString, sizeof(gString), "%s edited business %d, stat: %s, amount: %s.",GetName(playerid), i, edits, FormatNumber(amount));
				CMDRaport(gString, 6, 0);
			}
		}
	}
	return 1;
}

