CMD:givetax(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pLeader] != 3) return SendClientMessage(playerid, COLOR_ERROR, "You need to be mayor.");
	new players = 0, charitygive;
	foreach(new i : Player) if(PlayerInfo[i][pLevel] > 1 && PlayerInfo[i][pLevel] < 9) players ++;
	if(players < 5) return SendClientMessage(playerid, COLOR_ERROR, "Need to be minimum 5 player with level between 2 and 8.");
	if(CharityMoney < 500000) return SendClientMessage(playerid, COLOR_ERROR, "There are not enough funds in the Charity box.");
    charitygive = 100000 + random(400001);
	CharityMoney -= charitygive;
	charitygive /= players;
	
    foreach(new i : Player) if(PlayerInfo[i][pLevel] > 1 && PlayerInfo[i][pLevel] < 9) {
		GivePlayerCash(i, charitygive);
	}
	format(gString, sizeof(gString), "Mayor %s given to all players with level between 2 and 8 $%s.",GetName(playerid),FormatNumber(charitygive));
	SendClientMessageToAll(COLOR_LIGHTRED, gString);
	gQuery[0] = '\0';
	format(gQuery, sizeof(gQuery), "UPDATE `others` SET `Charity`=%d WHERE `ID`=1", CharityMoney);
	mysql_query(handle, gQuery);
	return 1;
}
