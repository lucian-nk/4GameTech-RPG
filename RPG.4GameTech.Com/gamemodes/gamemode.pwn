public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid, bodypart)
{
	if(weaponid == 49 || weaponid == 34|| weaponid == 50 || weaponid == 51 || weaponid == 53 || weaponid == 54 || GetPlayerState(issuerid) == PLAYER_STATE_DRIVER)
	{
	    if(weaponid != 51 && GetPlayerState(issuerid) == PLAYER_STATE_DRIVER && (IsPlayerInDynamicArea(playerid, safezone1) || IsPlayerInDynamicArea(issuerid, safezone1)))
		{
		    new Float:cx, Float:cy, Float:cz;
			GetPlayerPos(playerid, cx, cy, cz);
			SetPlayerPos(playerid, cx, cy, cz+0.5);
		    amount = 0;
		}
		
		new Float:HP, Float:AP, Float:remainHP;

		GetPlayerHealthEx(playerid,HP);
		GetPlayerArmourEx(playerid,AP);

		if ( AP >= amount )
		{
			SetPlayerArmourEx( playerid, AP - amount );
			SetPlayerHealthEx( playerid, HP );
		}
		else if ( AP <= 0 )
		{
			remainHP = HP - amount;
			SetPlayerHealthEx( playerid, remainHP );
			if ( remainHP <= 0 && JustDeath[playerid] == 0 )
			{
				OnPlayerDeath( playerid, issuerid, weaponid );
				JustDeath[playerid] = 1;
			}
		}
		else
		{
			remainHP = HP + ( AP - amount );
			SetPlayerArmourEx( playerid, 0.0 );
			SetPlayerHealthEx( playerid, remainHP );
			if ( remainHP <= 0 && JustDeath[playerid] == 0 )
			{
				OnPlayerDeath( playerid, issuerid, weaponid );
				JustDeath[playerid] = 1;
			}
		}
	}
	return 1;
}

public OnPlayerGiveDamage(playerid, damagedid, Float:amount, weaponid, bodypart)
{
	if ( damagedid == INVALID_PLAYER_ID ) return 1;
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) return 1;
	if(GetPlayerSurfingVehicleID(playerid) != INVALID_VEHICLE_ID) return 1;
	if(GetPlayerState(playerid) != PLAYER_STATE_PASSENGER && GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return 1;
	if(AFK[damagedid] == 1) return 1;
	
	if(BeingTested[playerid] >= gettime() && GetPlayerCameraMode(playerid) == 53)
	{
        new Float:X, Float:Y, Float:Z, Float:redzdist = 4.0, Float:A;
        GetPlayerPos(playerid, X, Y, Z);
        GetPlayerFacingAngle(playerid, A);
        GetTestAimPos(playerid, X, Y, redzdist);
        SetPlayerPos(damagedid, X, Y, Z);
	}

    new faction = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader];
    new factiond = PlayerInfo[damagedid][pMember] + PlayerInfo[damagedid][pLeader];
    if(IsInWar1(playerid) && IsInWar1(damagedid) && faction == factiond) amount = 0;
    if(IsInWar2(playerid) && IsInWar2(damagedid) && faction == factiond) amount = 0;
    if(IsInWar1(playerid) && IsInWar1(damagedid) && IsPlayerInAnyVehicle(playerid)) amount = 0;
    if(IsInWar2(playerid) && IsInWar2(damagedid) && IsPlayerInAnyVehicle(playerid)) amount = 0;
    
	if(IsPlayerInDynamicArea(playerid, safezone1) || IsPlayerInDynamicArea(damagedid, safezone1))
	{
	    if(IsACop(playerid) && !PlayerInfo[damagedid][pWanted])
	    {
		    if(weaponid != 0) SetPlayerArmedWeapon(playerid, 0);
		    amount = 0;
	    }
	    else if(faction == 7 && !strmatch(PlayerInfo[playerid][pContract], GetName(damagedid)))
	    {
	        if(weaponid != 0) SetPlayerArmedWeapon(playerid, 0);
		    amount = 0;
	    }
	    else if(!IsACop(playerid) && faction != 7)
	    {
	        if(weaponid != 0) SetPlayerArmedWeapon(playerid, 0);
		    amount = 0;
	    }
	}

	if(weaponid == 23 && IsACop(playerid) && TazerON[playerid] == 1)
	{
		if(Tazed[damagedid] != 0)
		{
			SendClientMessage(playerid, COLOR_ERROR, "That player is already tazed.");
			return 1;
		}

		if(IsACop(damagedid))
		{
			SendClientMessage(playerid, COLOR_ERROR, "You can't taze your teammates.");
			return 1;
		}

		if(IsPlayerInAnyVehicle(damagedid))
		{
			SendClientMessage(playerid, COLOR_ERROR, "That player is in a vehicle.");
			return 1;
		}

		new chance = random(10);
		new string2[128];

		switch(chance)
		{
			case 0,2,4,6,8:
			{
				format(string2, sizeof(string2), "You were Tazed by %s for 3 seconds.", GetName(playerid));
				SendClientMessage(damagedid, COLOR_SYN2, string2);
				format(string2, sizeof(string2), "You Tazed %s for 3 seconds.", GetName(damagedid));
				SendClientMessage(playerid, COLOR_SYN2, string2);
				format(string2, sizeof(string2), "* %s shoots with his Tazer at %s and tazed him.", GetName(playerid) ,GetName(damagedid));
				ProxDetector(30.0, playerid, string2, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				GameTextForPlayer(damagedid, "~r~Tazed", 2500, 3);
				TogglePlayerControllableEx(damagedid, 0);
				Tazed[damagedid] = 1;
				ApplyAnimation(damagedid,"PED","KO_skid_front",4.1,0,1,1,1,0);
				amount = 0;
				SetTimerEx("unfr", 3000, false, "i", damagedid);
			}
			case 1,3,5,7,9:
			{
				format(string2, sizeof(string2), "* %s shoots with his Tazer at %s, but missed.", GetName(playerid) ,GetName(damagedid));
				ProxDetector(30.0, playerid, string2, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				amount = 0;
			}
		}
	}
	
    if(bodypart == 9 && faction == 7 && strmatch(PlayerInfo[playerid][pContract], GetName(damagedid))) amount = 250;

	new Float:HP, Float:AP, Float:remainHP;

	GetPlayerHealthEx(damagedid,HP);
	GetPlayerArmourEx(damagedid,AP);

	if ( AP >= amount )
	{
		SetPlayerArmourEx( damagedid, AP - amount );
		SetPlayerHealthEx( damagedid, HP );
	}
	else if ( AP <= 0 )
	{
		remainHP = HP - amount;
		SetPlayerHealthEx( damagedid, remainHP );
		if ( remainHP <= 0 && JustDeath[damagedid] == 0 )
		{
			OnPlayerDeath( damagedid, playerid, weaponid );
			JustDeath[damagedid] = 1;
		}
	}
	else
	{
		remainHP = HP + ( AP - amount );
		SetPlayerArmourEx( damagedid, 0.0 );
		SetPlayerHealthEx( damagedid, remainHP );
		if ( remainHP <= 0 && JustDeath[damagedid] == 0 )
		{
			OnPlayerDeath( damagedid, playerid, weaponid );
			JustDeath[damagedid] = 1;
		}
	}
	return 1;
}

