function CheckFuel()
{
 	foreach(new i : Player)
	{
		if(GetPlayerState(i) != PLAYER_STATE_DRIVER) continue;
		if(IsAfk[i]) continue;
		new vehicle = GetPlayerVehicleID(i);
		if(Fuel[vehicle] > 0)
		{
		    new Speed = GetPlayerSpeed(i, 0);
			GetVehicleParamsEx(vehicle,engine,lights,alarm,doors,bonnet,boot,objective);
			if(engine == VEHICLE_PARAMS_ON && Speed > 10 && IsAfk[i] == 0)
			{
				if(GetVehicleType(vehicle) == 2 || GetVehicleType(vehicle) == 4 || GetVehicleType(vehicle) == 5 || GetVehicleType(vehicle) == 9 || IsADMVCar(vehicle) || IsAFarmerCar(vehicle) || IsAGarbageCar(vehicle) || IsAPizzaCar(vehicle) || IsAWCTCar(vehicle) || truckveh[vehicle] == 1)
				{
					Fuel[vehicle]++;
				}
				Fuel[vehicle]--;
				if(OwnedVeh(vehicle)) vInfo[OwnedVeh(vehicle)][vFuel] = Fuel[vehicle];
			}
		}
		else
		{
			SetVehicleParamsEx(vehicle,VEHICLE_PARAMS_OFF,lights,alarm,doors,bonnet,boot,objective);
		}
	}
	return 1;
}

