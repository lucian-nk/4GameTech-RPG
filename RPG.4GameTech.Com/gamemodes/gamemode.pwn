function StopWar(zone, war)
{
	if(war == 1)
	{
		GangZoneStopFlashForAll(Zone[zone]);
		if(ScoreAtt > ScoreDef)
		{
			format(gString, sizeof(gString), "%s won turf %d, Score: %s %.1f - %.1f %s",FactionName[Attackers],zone,FactionName[Attackers],ScoreAtt,ScoreDef,FactionName[Defenders]);
			SendFactionMessage(Defenders, COLOR_WAR, gString);
			SendFactionMessage(Attackers, COLOR_WAR, gString);

			ZoneInfo[zone][zTeam] = Attackers;
			zUpdate(zone,zTeamx);
			
			ZoneInfo[zone][zHours] = 12;
			zUpdate(zone, zHoursx);

			GangZoneHideForAll(Zone[zone]);
			GangZoneShowForAllEx(Zone[zone],GetZoneColor(ZoneInfo[zone][zTeam]));
			
			SafeInfo[Attackers][sMoney] += 50000;
			sUpdate(Attackers, sMoneyx);
			SafeInfo[Attackers][sMaterials] += 5000;
			sUpdate(Attackers, sMaterialsx);
		}
		else
		{
			format(gString, sizeof(gString), "%s won turf %d, Score: %s %.1f - %.1f %s",FactionName[Defenders],zone,FactionName[Defenders],ScoreDef,ScoreAtt,FactionName[Attackers]);
			SendFactionMessage(Defenders, COLOR_WAR, gString);
			SendFactionMessage(Attackers, COLOR_WAR, gString);

			ZoneInfo[zone][zTeam] = Defenders;
			zUpdate(zone,zTeamx);
			
			ZoneInfo[zone][zHours] = 12;
			zUpdate(zone, zHoursx);

			GangZoneHideForAll(Zone[zone]);
			GangZoneShowForAllEx(Zone[zone],GetZoneColor(ZoneInfo[zone][zTeam]));
			
			SafeInfo[Defenders][sMoney] += 50000;
			sUpdate(Defenders, sMoneyx);
			SafeInfo[Defenders][sMaterials] += 5000;
			sUpdate(Defenders, sMaterialsx);
		}

		new Best = 0;
		new Worst = 0;

		new BestMember[24] = "-";
		new WorstMember[24] = "-";

  		foreach(new i : Player)
		{
			if(IsInWar1(i))
			{
				if(PersonalKills[i] - PersonalDeaths[i] > Best)
				{
					GetPlayerName(i, BestMember, sizeof(BestMember));
					Best = PersonalKills[i] - PersonalDeaths[i];
				}
				else if(PersonalKills[i] - PersonalDeaths[i] < Worst)
				{
					GetPlayerName(i, WorstMember, sizeof(WorstMember));
					Worst = PersonalKills[i] - PersonalDeaths[i];
				}
			}
		}

		format(gString, sizeof(gString), "Best Gangster: %s (%d), Worst Gangster: %s (%d)",BestMember,Best,WorstMember,Worst);
		SendFactionMessage(Defenders, COLOR_WAR, gString);
		SendFactionMessage(Attackers, COLOR_WAR, gString);

  		foreach(new i : Player)
		{
			if(IsInWar1(i))
			{
				format(gString, sizeof(gString), "Your score was: Kills: %d Deaths: %d",PersonalKills[i],PersonalDeaths[i]);
				SendClientMessage(i, COLOR_WAR, gString);

				PersonalKills[i] = 0;
				PersonalDeaths[i] = 0;
				SetPlayerVirtualWorld(i, 0);
				for(new m = 0; m < 5; m++) SendDeathMessageToPlayer(i, 1001, 1001, 200);
			}
		}
		
		InWar[Attackers] = 0;
		InWar[Defenders] = 0;
		WarPause[Attackers] = gettime() + 300;

		Attackers = 99;
		Defenders = 99;

		ScoreAtt = 0.0;
		ScoreDef = 0.0;

		War = 0;
		TakeON[zone] = 0;
		WarTime[0] = 0;
		for(new i = barzini[0]; i <= corleone[7]; i++) if(GetVehicleVirtualWorld(i) == 10) SetVehicleToRespawn(i);
		DestroyWarPickups(1);
	}
 	else if(war == 2)
	{
		GangZoneStopFlashForAll(Zone[zone]);
		if(ScoreAtt2 > ScoreDef2)
		{
			format(gString, sizeof(gString), "%s won turf %d, Score: %s %.1f - %.1f %s",FactionName[Attackers2],zone,FactionName[Attackers2],ScoreAtt2,ScoreDef2,FactionName[Defenders2]);
			SendFactionMessage(Defenders2, COLOR_WAR, gString);
			SendFactionMessage(Attackers2, COLOR_WAR, gString);

			ZoneInfo[zone][zTeam] = Attackers2;
			zUpdate(zone,zTeamx);
			
			ZoneInfo[zone][zHours] = 12;
			zUpdate(zone, zHoursx);

			GangZoneHideForAll(Zone[zone]);
			GangZoneShowForAllEx(Zone[zone],GetZoneColor(ZoneInfo[zone][zTeam]));
			
			SafeInfo[Attackers2][sMoney] += 50000;
			sUpdate(Attackers2, sMoneyx);
			SafeInfo[Attackers2][sMaterials] += 5000;
			sUpdate(Attackers2, sMaterialsx);
		}
		else
		{
			format(gString, sizeof(gString), "%s won turf %d, Score: %s %.1f - %.1f %s",FactionName[Defenders2],zone,FactionName[Defenders2],ScoreDef2,ScoreAtt2,FactionName[Attackers2]);
			SendFactionMessage(Defenders2, COLOR_WAR, gString);
			SendFactionMessage(Attackers2, COLOR_WAR, gString);

			ZoneInfo[zone][zTeam] = Defenders2;
			zUpdate(zone,zTeamx);
			
			ZoneInfo[zone][zHours] = 12;
			zUpdate(zone, zHoursx);

			GangZoneHideForAll(Zone[zone]);
			GangZoneShowForAllEx(Zone[zone],GetZoneColor(ZoneInfo[zone][zTeam]));
			
			SafeInfo[Defenders2][sMoney] += 50000;
			sUpdate(Defenders2, sMoneyx);
			SafeInfo[Defenders2][sMaterials] += 5000;
			sUpdate(Defenders2, sMaterialsx);
		}

		new Best = 0;
		new Worst = 0;

		new BestMember[24] = "-";
		new WorstMember[24] = "-";

  		foreach(new i : Player)
		{
			if(IsInWar2(i))
			{
				if(PersonalKills[i] - PersonalDeaths[i] > Best)
				{
					GetPlayerName(i, BestMember, sizeof(BestMember));
					Best = PersonalKills[i] - PersonalDeaths[i];
				}
				else if(PersonalKills[i] - PersonalDeaths[i] < Worst)
				{
					GetPlayerName(i, WorstMember, sizeof(WorstMember));
					Worst = PersonalKills[i] - PersonalDeaths[i];
				}
			}
		}

		format(gString, sizeof(gString), "Best Gangster: %s (%d), Worst Gangster: %s (%d)",BestMember,Best,WorstMember,Worst);
		SendFactionMessage(Defenders2, COLOR_WAR, gString);
		SendFactionMessage(Attackers2, COLOR_WAR, gString);

  		foreach(new i : Player)
		{
			if(IsInWar2(i))
			{
				format(gString, sizeof(gString), "Your score was: Kills: %d Deaths: %d",PersonalKills[i],PersonalDeaths[i]);
				SendClientMessage(i, COLOR_WAR, gString);

				PersonalKills[i] = 0;
				PersonalDeaths[i] = 0;
				SetPlayerVirtualWorld(i, 0);
				for(new m = 0; m < 5; m++) SendDeathMessageToPlayer(i, 1001, 1001, 200);
			}
		}
		
		InWar[Attackers2] = 0;
		InWar[Defenders2] = 0;
		WarPause[Attackers2] = gettime() + 300;

		Attackers2 = 99;
		Defenders2 = 99;

		ScoreAtt2 = 0.0;
		ScoreDef2 = 0.0;

		War2 = 0;
		TakeON[zone] = 0;
		WarTime[1] = 0;
		for(new i = barzini[0]; i <= corleone[7]; i++) if(GetVehicleVirtualWorld(i) == 20) SetVehicleToRespawn(i);
