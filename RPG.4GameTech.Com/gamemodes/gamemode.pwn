		mysql_format(handle, gQuery, sizeof(gQuery), "SELECT `Name`, `CMember`, `CLeader`, `HouseKey`, `BizzKey` FROM `players` WHERE `Name` = '%e' OR `AName` = '%e' LIMIT 1", giveplayer, giveplayer);
		new Cache:result = mysql_query(handle, gQuery);
		new tmp[25], clanm, clanl, name[MAX_PLAYER_NAME], house, bizz;
		if(!cache_num_rows())
		{
		    cache_delete(result);
		    return SendClientMessage(playerid, COLOR_ERROR, "Invalid Player Name.");
		}
		cache_get_field_content(0, "CMember", tmp), clanm = strval(tmp);
		cache_get_field_content(0, "CLeader", tmp), clanl = strval(tmp);
		cache_get_field_content(0, "HouseKey", tmp), house = strval(tmp);
		cache_get_field_content(0, "BizzKey", tmp), bizz = strval(tmp);
		cache_get_field_content(0, "Name", tmp),  format(name, MAX_PLAYER_NAME, tmp);
		format(giveplayer, MAX_PLAYER_NAME, name);
		cache_delete(result);
		new clan = clanm + clanl;
		if(clan == 0) return SendClientMessage(playerid, COLOR_ERROR, "Player is not in a clan.");

        if(ClanInfo[clan][cTagType] == 0) strdel(name, 0, strlen(ClanInfo[clan][cTag]));
		else strdel(name, strlen(name) - strlen(ClanInfo[clan][cTag]), strlen(name));
		if(ExistName(name)) return SendClientMessage(playerid, COLOR_ERROR, "Name already exist.");

  		if(house > 0)
		{
			strmid(HouseInfo[house][hOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			hUpdate(house, hOwnerx);
			HouseUpdate(house);
		}

		if(bizz > 0)
		{
			strmid(BizzInfo[bizz][bOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			bUpdate(bizz, bOwnerx);
			BizzUpdate(bizz);
		}

  		for(new i = 1; i <= Total_Veh_Created; i++)
		{
			if(strmatch(vInfo[i][vOwner], giveplayer))
			{
				strmid(vInfo[i][vOwner], name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
				vUpdate(i, vOwnerx);
			}
		}
		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Name`='%e', `CMember` = '0', `CLeader` = '0', `CRank` = '0' WHERE `Name`='%e'", name, giveplayer);
		mysql_query(handle, gQuery);
		
		format(gString, sizeof(gString),"%s was uninvited from his clan.", name);
		SendClientMessage(playerid, COLOR_SYN2, gString);
	}
	return 1;
}

