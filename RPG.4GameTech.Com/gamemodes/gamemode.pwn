		TextDrawHideForPlayer(playerid, TD_Betsix[0]);
		TextDrawHideForPlayer(playerid, TD_Betsix[1]);
		Betting[playerid] = false;
	}
	
	PlayerInfo[playerid][pDeaths] ++;
    Update(playerid, pDeathsx);
	if(killerid != INVALID_PLAYER_ID)
	{
	    PlayerInfo[killerid][pKills] ++;
	    Update(killerid, pKillsx);
	    printf("[DEATH] %s was killed by %s, reason: %d", GetName(playerid), GetName(killerid), reason);
 	    if(JoinedMotoCross[playerid] > 0)
		{
		    JoinedMotoCross[playerid] = 0;
		    InscrisiMotoCross--;
		    CP[playerid] = 999;
		    DisablePlayerRaceCheckpoint(playerid);
		}
		if(JoinedDeathMatch[killerid] == 1 || JoinedDeathMatch[killerid] == 2)
		{
			return 1;
		}
		if(JoinedDeathMatch[playerid] > 0)
		{
			JoinedDeathMatch[playerid] = 0;
			SetPlayerSkin(playerid, PlayerInfo[playerid][pSkin]);
			ResetPlayerWeapons(playerid);
		}
		if(War == 1)
		{
			if(GetPlayerZone(playerid) != -1 && GetPlayerZone(killerid) != -1 && TakeON[GetPlayerZone(playerid)] == 1 && TakeON[GetPlayerZone(killerid)] == 1)
			{
				if(IsInWar1(killerid) && IsInWar1(playerid) && (IsAttacker(killerid) && IsDefender(playerid) || IsDefender(killerid) && IsAttacker(playerid)) && GetPlayerVirtualWorld(playerid) == 10 && GetPlayerVirtualWorld(killerid) == 10)
				{
					if(IsAttacker(killerid) && IsDefender(playerid))
					{
						ScoreAtt += 1.0;
					}
					else if(IsDefender(killerid) && IsAttacker(playerid))
					{
