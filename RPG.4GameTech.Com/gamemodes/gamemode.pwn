public OnGameModeExit()
{
	KillTimer(FlashTimer);
	for (new i = 0; i < MAX_VEHICLES; i++)
	{
		DestroyObject(obj[i]);
		DestroyObject(obj2[i]);
	}
	KillTimer(moneytimer);
	KillTimer(othertimer);
	KillTimer(checkcartimer);
	KillTimer(checkfueltimer);
	KillTimer(custompickups);
	KillTimer(paydaytimer);
	KillTimer(idletimer);
	KillTimer(areatimer);
	KillTimer(radartimer);

	TextDrawDestroy(Logo[0]);
	//TextDrawDestroy(Logo[1]);
	TextDrawDestroy(ClockTime);
	TextDrawDestroy(ClockDate);
	for(new playerid = 0; playerid < MAX_PLAYERS; playerid++)
	{
		TextDrawDestroy(Speedo[0][playerid]);
		TextDrawDestroy(Speedo[1][playerid]);
		TextDrawDestroy(Speedo[2][playerid]);
		TextDrawDestroy(Radio[playerid]);
		TextDrawDestroy(Destination[playerid]);
		TextDrawDestroy(DestinationKM[playerid]);
		TextDrawDestroy(JailTime[playerid]);
	}

	if(mysql_close(handle)) printf("[MySQL]: Successfully disconnected from database!");
	return 1;
}

