	lvlcost = PlayerInfo[playerid][pLevel] * levelcost;
	if(PlayerInfo[playerid][pLevel] == 1) lvlrespect = 2;
	else lvlrespect = PlayerInfo[playerid][pLevel] * levelrespect - 2;
	
	if(GetPlayerCash(playerid) < lvlcost) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money.");
	if(PlayerInfo[playerid][pRespect] < lvlrespect) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough respect points.");
	format(gString, sizeof(gString), "~g~LEVEL UP~n~~w~You Are Now Level %d", PlayerInfo[playerid][pLevel] + 1);
	GameTextForPlayer(playerid, gString, 5000, 1);
	GivePlayerCash(playerid, -lvlcost);
	if(PlayerInfo[playerid][pPremiumAccount] == 1)
	{
		PlayerInfo[playerid][pRespect] -= lvlrespect;
	}
	else
	{
		PlayerInfo[playerid][pRespect] = 0;
	}
	Update(playerid,pRespectx);
	
	PlayerInfo[playerid][pLevel]++;
	SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
	Update(playerid,pLevelx);
	TutVehUpdate(playerid);
	
	if(PlayerInfo[playerid][pReferral] > 0)
	{
		new bonusr =  lvlrespect * 25/100;
		new bonusm = lvlcost * 2;

		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `TReferralRespect` = `TReferralRespect` + %d, `TReferralMoney` = `TReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
		mysql_query(handle, gQuery);

		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `ReferralRespect` = `ReferralRespect` + %d, `ReferralMoney` = `ReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
		mysql_query(handle, gQuery);
	}
	if(PlayerInfo[playerid][pLevel] % 5 == 0)
	{
	    format(gString, sizeof(gString), "Reach level %d", PlayerInfo[playerid][pLevel]);
		GiveAchievement(playerid, 1, gString, 100);
	}
	else UpdateAchievementStatus(playerid, 1, 0);
	return 1;
}

