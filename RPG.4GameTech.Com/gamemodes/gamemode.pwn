	if ( GetPVarInt( playerid, "AccountLocked" ) == 1 )
	{
		if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		{
			SCM( playerid, COLOR_WHITE, "Your account is locked, use /unlockacc to unlock it." );
		}
		if(PlayerInfo[playerid][pLimba] == 2) // Romana
		{
			SCM( playerid, COLOR_WHITE, "Contul tau este blocat, deblocheaza-l folosind comanda /unlockacc." );
		}
	}
	new giveplayerid, splayer[25], reason[64];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[25]s[64]",splayer,reason)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /ban <Player ID/Name> <reason>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[giveplayerid][pAdmin] > PlayerInfo[playerid][pAdmin])  return SendClientMessage(playerid, COLOR_ERROR, "You can't do this.");
		AddBan(playerid,giveplayerid,0,reason,1);
	}
	return 1;
}

