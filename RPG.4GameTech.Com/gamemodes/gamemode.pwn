CMD:pay(playerid, params[])
{
	if ( GetPVarInt( playerid, "AccountLocked" ) == 1 )
	{
		if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		{
			SCM( playerid, COLOR_WHITE, "Your account is locked, use /unlockacc to unlock it." );
		}
		if(PlayerInfo[playerid][pLimba] == 2) // Romana
		{
			SCM( playerid, COLOR_WHITE, "Contul tau este blocat, deblocheaza-l folosind comanda /unlockacc." );
		}
	}
	new giveplayerid, splayer[25], money;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(sscanf(params,"s[25]i", splayer, money)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /pay <Player ID/Name> <amount>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[playerid][pLevel] < 2) return SendClientMessage(playerid, COLOR_ERROR, "You need level 2.");
		if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "You cannot pay yourself.");
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(PlayerInfo[playerid][pLevel] < 3) if(MoneyACheck(playerid, money, 1, 10000)) return 1;
		if(MoneyACheck(playerid, money, 1, 100000)) return 1;
		if(GetPlayerCash(playerid) < money) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money.");

		format(gString, sizeof(gString), "You have payed $%s to %s.", FormatNumber(money), GetName(giveplayerid));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "You have recieved $%s from %s.", FormatNumber(money), GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "* %s takes out some cash, and hands it to %s.", GetName(playerid) ,GetName(giveplayerid));
		ProxDetector(30.0, playerid, gString, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);

