// Request Event
CMD:event(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
    if(activeevent == 0) return SendClientMessage(playerid, COLOR_ERROR, "There is currently no active event running.");
    new string[256], name[MAX_PLAYER_NAME];
    foreach(new i : Player)
    {
        if(eventorganizer[i] == 1)
        {
            GetPlayerName(i, name, sizeof(name));
        }
    }
    SendClientMessage(playerid, 0xFFFFFFFF, "-------------------------------------------");
    format(string, sizeof(string), "{FFFF00}Title: {FFFFFF}%s{FFFF00} Location: {FFFFFF}%s{FFFF00} Time: {FFFFFF}%s{FFFF00}", eventtitle, eventlocation, eventtime);
    SendClientMessage(playerid, 0xFFFFFFFF, string);
    format(string, sizeof(string), "{FFFF00}Organizer: {FFFFFF}%s{FFFF00} Prize: {FFFFFF}$%s{FFFF00}", name, FormatNumber(eventprize));
    SendClientMessage(playerid, 0xFFFFFFFF, string);
    format(string, sizeof(string), "{FFFF00}Description: {FFFFFF}%s{FFFF00}", eventdescription);
    SendClientMessage(playerid, 0xFFFFFFFF, string);
    SendClientMessage(playerid, 0xFFFFFFFF, "-------------------------------------------");
    return 1;
}

CMD:e(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
    if(eventorganizer[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You're not the organizer of the event.");
    new text[250];
    if(sscanf(params, "s[250]", text)) return SendClientMessage(playerid, COLOR_SYN, "/e <text>");
    new string[256], name[MAX_PLAYER_NAME];
    GetPlayerName(playerid, name, sizeof(name));
    format(string, sizeof(string), "{7094DB}Event Organizer %s: %s", name, text);
    SendClientMessageToAll(0xFFFFFFFF, string);
    return 1;
}

CMD:cancelrequest(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
    if(requestingevent[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "You're not requesting an event.");
    requestingevent[playerid] = 0;
    SendClientMessage(playerid, 0xFFFFFFFF, "{FFFF00}Your event request has been cancelled.");
    return 1;
}

CMD:stopevent(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
    if(activeevent == 0 && PlayerInfo[playerid][pAdmin] > 0) return SendClientMessage(playerid, COLOR_ERROR, "There isn't any events to stop.");
    if(eventorganizer[playerid] == 0 && PlayerInfo[playerid][pAdmin] < 1) return SendClientMessage(playerid, COLOR_ERROR, "You're not the organizer of the event.");
    foreach(new i : Player)
    {
        eventorganizer[i] = 0;
    }
    SendClientMessageToAll(0xFFFFFFFF, "{7094DB}Event over.");
    activeevent = 0;
    return 1;
}

CMD:acceptevent(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
    if(PlayerInfo[playerid][pAdmin] < 1) return SendClientMessage(playerid, COLOR_ERROR, "You're not an admin.");
    if(activeevent == 1) return SendClientMessage(playerid, COLOR_ERROR, "There's already an active event.");
    new eventid;
    if(sscanf(params, "d", eventid)) return SendClientMessage(playerid, COLOR_SYN, "Syntax: /acceptevent <eventid>");
    if(requestingevent[eventid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Invalid event ID.");
    strmid(eventtitle, requestingtitle[eventid], 0, strlen(requestingtitle[eventid]));
    strmid(eventlocation, requestinglocation[eventid], 0, strlen(requestinglocation[eventid]));
    strmid(eventdescription, requestingdescription[eventid], 0, strlen(requestingdescription[eventid]));
    eventprize = requestingprize[eventid];
    strmid(eventtime, requestingtime[eventid], 0, strlen(requestingtime[eventid]));
    new string[256], name[MAX_PLAYER_NAME];
    GetPlayerName(eventid, name, sizeof(name));
    format(string, sizeof(string), "{FFFF00}Event: %d Title: %s Location: %s Time: %s", eventid, eventtitle, eventlocation, eventtime);
    SendClientMessageToAll(0xFFFFFFFF, string);
    format(string, sizeof(string), "{FFFF00}Organizer: %s Prize: $%s", name, FormatNumber(eventprize));
    SendClientMessageToAll(0xFFFFFFFF, string);
    format(string, sizeof(string), "{FFFF00}Description: %s", eventdescription);
    SendClientMessageToAll(0xFFFFFFFF, string);
    format(string, sizeof(string), "%s accepted %s' event request.", GetName(playerid), GetName(eventid));
    ABroadCast(COLOR_YELLOW, string, 1);
    format(string, sizeof(string), "Admin %s accepted your event request.", GetName(playerid));
    SendClientMessage(eventid, COLOR_YELLOW, string);
    eventorganizer[eventid] = 1;
    requestingevent[eventid] = 0;
    activeevent = 1;
    GiveAchievement(eventid, 8, "Create a event", 100);
    return 1;
}

CMD:requestevent(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
    if(activeevent == 1) return SendClientMessage(playerid, COLOR_ERROR, "There's already an active event.");
    if(requestingevent[playerid] == 1) return SendClientMessage(playerid, COLOR_ERROR, "You're already requesting an event, cancel it first.");
    if(PlayerInfo[playerid][pLevel] < 5) return SendClientMessage(playerid, COLOR_ERROR, "You need be level 5 to request a event.");
    new title[50], location[50], description[100], prize, time[50];
    if(sscanf(params, "p<|>s[50]s[50]s[50]s[100]d", title, location, time, description, prize)) return SendClientMessage(playerid, COLOR_SYN, "Syntax: /requestevent <Title|Location|Time|Description|Prize>");
    if(strlen(title) < 1 || strlen(title) > 50) return SendClientMessage(playerid, COLOR_ERROR, "Invalid amount of characters in title.");
    if(strlen(location) < 1 || strlen(location) > 50) return SendClientMessage(playerid, COLOR_ERROR, "Invalid amount of characters in location.");
    if(strlen(time) < 1 || strlen(time) > 50) return SendClientMessage(playerid, COLOR_ERROR, "Invalid amount of characters in time.");
    if(strlen(description) < 1 || strlen(description) > 100) return SendClientMessage(playerid, COLOR_ERROR, "Invalid amount of characters in description.");
    if(prize < 1 || prize > 500000) return SendClientMessage(playerid, COLOR_ERROR, "Invalid prize amount ( 1 - 500000 )");
    new string[256], name[MAX_PLAYER_NAME];
    GetPlayerName(playerid, name, sizeof(name));
    requestingevent[playerid] = 1;
    strmid(requestingtitle[playerid], title, 0, strlen(title));
    strmid(requestinglocation[playerid], location, 0, strlen(location));
    strmid(requestingdescription[playerid], description, 0, strlen(description));
    requestingprize[playerid] = prize;
    strmid(requestingtime[playerid], time, 0, strlen(time));

	format(string, sizeof(string), "{FFFF00}Event %d has been requested, details are below.", playerid);
	ABroadCast(0xFFFFFFFF,string,1);
	SendClientMessage(playerid, 0xFFFFFFFF, string);
	format(string, sizeof(string), "{FFFF00}Event: %d Title: %s Location: %s Time: %s", playerid, title, location, time);
	ABroadCast(0xFFFFFFFF,string,1);
	SendClientMessage(playerid, 0xFFFFFFFF, string);
	format(string, sizeof(string), "{FFFF00}Organizer: %s Prize: $%s", name, FormatNumber(prize));
	ABroadCast(0xFFFFFFFF,string,1);
	SendClientMessage(playerid, 0xFFFFFFFF, string);
	format(string, sizeof(string), "{FFFF00}Description: %s", description);
	ABroadCast(0xFFFFFFFF,string,1);
	SendClientMessage(playerid, 0xFFFFFFFF, string);
	format(string, sizeof(string), "{FFFF00}Use [/acceptevent %d] to accept this event.", playerid);
	ABroadCast(0xFFFFFFFF,string,1);
	
    SendClientMessage(playerid, 0xFFFFFFFF, "{FFFF00}Your event is pending approval from an admin, please wait.");
    return 1;
}

CMD:rejectevent(playerid, params[])
{
    if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
