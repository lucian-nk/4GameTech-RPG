CMD:transfer(playerid, params[])
{
	if ( GetPVarInt( playerid, "AccountLocked" ) == 1 )
	{
		if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		{
			SCM( playerid, COLOR_WHITE, "Your account is locked, use /unlockacc to unlock it." );
		}
		if(PlayerInfo[playerid][pLimba] == 2) // Romana
		{
			SCM( playerid, COLOR_WHITE, "Contul tau este blocat, deblocheaza-l folosind comanda /unlockacc." );
		}
	}
	new giveplayerid, splayer[25], money;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(BizzEntered[playerid] != 2) return SendClientMessage(playerid, COLOR_ERROR, "You need to be in Bank to transfer money.");
	if(sscanf(params,"s[25]i", splayer, money)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /transfer <Player ID/Name> <amount>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "You cannot transfer money yourself.");
		if(PlayerInfo[playerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_ERROR, "You need be level 3 to transfer money.");
		if(MoneyACheck(playerid, money, 1, 1000000000)) return 1;
		new tax = money/100;
		if(PlayerInfo[playerid][pAccount] < (money+tax)) return SendClientMessage(playerid, COLOR_ERROR, "You don't have enough money in bank.");
		
		format(gString, sizeof(gString), "You have transfered $%s to %s with a tax of $%s.", FormatNumber(money), GetName(giveplayerid), FormatNumber(tax));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "You have recieved $%s from %s.", FormatNumber(money), GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		PlayerInfo[giveplayerid][pAccount] += money;
		Update(giveplayerid,pAccountx);
		PlayerInfo[playerid][pAccount] -= (money + tax);
		Update(playerid,pAccountx);

		BizzInfo[2][bSafe] += tax;
		bUpdate(2, bSafex);
		BizzPayLog(playerid, 2, tax);
		
		format(gString, sizeof(gString), "%s transfered $%s to %s.", GetName(playerid), FormatNumber(money), GetName(giveplayerid));
		ABroadCast(COLOR_YELLOW, gString, 1);
		
		TransferLog(playerid, giveplayerid, money);
	}
	return 1;
}

