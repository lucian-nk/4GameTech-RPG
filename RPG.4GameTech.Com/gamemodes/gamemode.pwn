CMD:loadwood(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pJob] != 10) return SendClientMessage(playerid, COLOR_ERROR, "You don't have woodcutter job.");
	if(!IsAWCTCar(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, COLOR_ERROR, "You need be in a woodcutter vehicle.");

	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 2344.1970, -643.8792, 128.3895))
	{
	    UseFind[playerid] = 0;
	    IsTowing[playerid] = 0;
		SetPlayerCheckpoint(playerid, 2344.1970, -643.8792, 128.3895, 2.0);
		CP[playerid] = 1995;
		return 1;
	}
	if(PlayerInfo[playerid][pStockPile] < 1) return SendClientMessage(playerid, COLOR_ERROR, "You need have at least 1 square meter of wood in your stockpile.");
	if(Wood[GetPlayerVehicleID(playerid)] >= 50) return SendClientMessage(playerid, COLOR_ERROR, "This car is fully loaded with wood.");

	TogglePlayerControllableEx(playerid, 0);
	GameTextForPlayer(playerid, "Loading...", 2000, 4);
	SetTimerEx("unfr", 2000, false, "i", playerid);
	if(Wood[GetPlayerVehicleID(playerid)] >= 1)
	{
		new woodneed = 50 - Wood[GetPlayerVehicleID(playerid)];
		if(PlayerInfo[playerid][pStockPile] > woodneed)
		{
			PlayerInfo[playerid][pStockPile] -= woodneed;
			Update(playerid,pStockPilex);
			Wood[GetPlayerVehicleID(playerid)] += woodneed;
		}
		else
		{
			Wood[GetPlayerVehicleID(playerid)] += PlayerInfo[playerid][pStockPile];
			PlayerInfo[playerid][pStockPile] = 0;
			Update(playerid,pStockPilex);
		}
	}
	else
	{
		if(PlayerInfo[playerid][pStockPile] > 50)
		{
			PlayerInfo[playerid][pStockPile] -= 50;
			Update(playerid,pStockPilex);
			Wood[GetPlayerVehicleID(playerid)] = 50;
		}
		else
		{
			Wood[GetPlayerVehicleID(playerid)] = PlayerInfo[playerid][pStockPile];
			PlayerInfo[playerid][pStockPile] = 0;
			Update(playerid,pStockPilex);
		}
	}

	for(new i = 0; i < sizeof(woodcutter); i++)
	{
		if(GetPlayerVehicleID(playerid) == woodcutter[i])
		{
		    if(IsValidObject(wctobj[i])) DestroyObject(wctobj[i]);
			wctobj[i] = CreateObject(1463, 2348.31006, -641.61316, 128.24339,   0.00000, 0.00000, 91.00000);
			AttachObjectToVehicle(wctobj[i], GetPlayerVehicleID(playerid), 0.000000, -1.719998, 0.309999, 0.000000, 0.000000, 0.000000);
		}
	}
	UseFind[playerid] = 0;
	IsTowing[playerid] = 0;
	SetPlayerCheckpoint(playerid, 2210.6592, -2292.7727, 14.7632, 2.0);
	CP[playerid] = 1995;
	SendClientMessage(playerid, COLOR_SYN2, "Go to the checkpoint to sell the wood.");
	WoodWait[playerid] = gettime() + 90;
	return 1;
}

CMD:sellwood(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pJob] != 10) return SendClientMessage(playerid, COLOR_ERROR, "You don't have woodcutter job.");
	if(!IsAWCTCar(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, COLOR_ERROR, "You need be in a woodcutter vehicle.");
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_ERROR, "You need to be the driver.");
    
	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 2210.6592, -2292.7727, 14.7632))
	{
	    UseFind[playerid] = 0;
	    IsTowing[playerid] = 0;
		SetPlayerCheckpoint(playerid, 2210.6592, -2292.7727, 14.7632, 2.0);
		CP[playerid] = 1995;
		SendClientMessage(playerid, COLOR_SYN2, "Go to the checkpoint to sell the wood.");
		return 1;
	}
	
	if(WoodWait[playerid] > gettime())
	{
	    format(gString, sizeof(gString), "You need to wait %d seconds.", WoodWait[playerid]-gettime());
	    SendClientMessage(playerid, COLOR_ERROR, gString);
	    return 1;
	}
	
	if(Wood[GetPlayerVehicleID(playerid)] < 1) return SendClientMessage(playerid, COLOR_ERROR, "This car don't have wood loaded.");

	TogglePlayerControllableEx(playerid, 0);
	SetTimerEx("unfr", 2000, false, "i", playerid);
	new skill, remain, forup;
	GetWoodSkill(playerid, skill, remain, forup);
	new perwood = 58 + 10*skill;
	new money = Wood[GetPlayerVehicleID(playerid)]*perwood + random(1000);
    money += money*JobBoost/100;
    if(PlayerInfo[playerid][pMoneyBoost] > 0) money += money*20/100;
	GivePlayerCash(playerid, money);
	format(gString, sizeof(gString), "Unloading... +$%s", FormatNumber(money));
