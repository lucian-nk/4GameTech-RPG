function StopBeachParty()
{
    BeachPartyOn = 0;
    SendClientMessageToAll(COLOR_NEWS, "NR Bot: The beach party is over. Thanks for comming.");
    foreach(new i : Player) if(IsPlayerInDynamicArea(i, PartyArea, 0)) StopAudioStreamForPlayer(i);
    if(IsValidDynamicArea(PartyArea)) DestroyDynamicArea(PartyArea);
	for(new i = 0; i < sizeof(BeachPObject); i++) if(IsValidDynamicObject(BeachPObject[i])) DestroyDynamicObject(BeachPObject[i]);
	for(new i = 0; i < sizeof(PartyText); i++) if(IsValidDynamic3DTextLabel(PartyText[i])) DestroyDynamic3DTextLabel(PartyText[i]);
	for(new i = 0; i < sizeof(PartyPickup); i++) if(IsValidDynamicPickup(PartyPickup[i])) DestroyDynamicPickup(PartyPickup[i]);
	//DestroyDynamic3DTextLabel(GiftText);
	//DestroyDynamicPickup(GiftPickup);
}

function PayLog(playerid, giveplayerid, money)
{
	new entry[256];
	new y, m, d, h, mi,s; getdate(y,m,d); gettime(h,mi,s);
	format(entry, sizeof(entry), "(%02d/%02d/%d)[%02d:%02d:%02d] - %s[%s] payed $%s to %s[%s]\n",d,m,y,h,mi,s,GetName(playerid),GetIPEx(playerid),FormatNumber(money),GetName(giveplayerid),GetIPEx(giveplayerid));
	new File:hFile;
	hFile = fopen("logs/pay.log", io_append);
	fwrite(hFile, entry);
	fclose(hFile);
}

