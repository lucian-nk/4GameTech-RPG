///
	if(JustDeath[playerid] == 1) return 1;
    if(XDeaths[playerid] == 0)
    {
       LastDeath[playerid] = gettime();
    }
    XDeaths[playerid]++;
    if(XDeaths[playerid] > 3)
    {
        if((gettime() - LastDeath[playerid]) <= 2 && FKicked[playerid] == 0)
        {
            format(gString, sizeof(gString), "%s was kicked by AdmBot, reason: Fake Kill", GetName(playerid));
			SendClientMessageToAll(COLOR_LIGHTRED, gString);
			Kick(playerid);
			FKicked[playerid] = 1;
        }
		else if((gettime() - LastDeath[playerid]) > 2)
        {
            XDeaths[playerid] = 0;
        }
    }
    
	Update3DTextLabelText(label[playerid], 0x008080FF, "[Dead]");
	BizzEntered[playerid] = 0;
	HouseEntered[playerid] = 0;
	
	gPlayerUsingLoopingAnim[playerid] = 0;

	if(GunTest[playerid] == true)
	{
		for(new i = 0; i < sizeof(gobj); i++)
		{
			if(!IsValidDynamicObject(gobj[i])) continue;
			DestroyDynamicObject(gobj[i]);
		}
		GunTest[playerid] = false;
		GunScore[playerid] = 0;
		GunO = 0;
	}
	
