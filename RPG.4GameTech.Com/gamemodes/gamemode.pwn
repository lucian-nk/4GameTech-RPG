		if(rank < 1 || rank > 6) return SendClientMessage(playerid, COLOR_ERROR, "Rank must be between 1 and 6.");

		if(rank >= PlayerInfo[giveplayerid][pCRank])
		{
			format(gString, sizeof(gString), "You promoted %s to rank %d.",GetName(giveplayerid), rank);
			SendClientMessage(playerid, COLOR_SYN2, gString);
			format(gString, sizeof(gString), "Leader %s promoted you to rank %d.",GetName(playerid), rank);
			SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		}
		else
		{
			format(gString, sizeof(gString), "You demoted %s to rank %d.",GetName(giveplayerid), rank);
			SendClientMessage(playerid, COLOR_SYN2, gString);
			format(gString, sizeof(gString), "Leader %s demoted you to rank %d.",GetName(playerid), rank);
			SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		}

		PlayerInfo[giveplayerid][pCRank] = rank;
		Update(giveplayerid,pCRankx);
	}
	return 1;
}

