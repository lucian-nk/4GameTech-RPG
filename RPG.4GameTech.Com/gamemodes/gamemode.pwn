	if(bizz != 0) format(entry, sizeof(entry), "(%02d/%02d/%d)[%02d:%02d:%02d] - %s[%s] sold his business[ID:%d] to state for $%s\n",d,m,y,h,mi,s,GetName(playerid),GetIPEx(playerid),bizz,FormatNumber(money));
	if(vehicle != 0) format(entry, sizeof(entry), "(%02d/%02d/%d)[%02d:%02d:%02d] - %s[%s] sold his vehicle[Model:%s] to dealership for $%s\n",d,m,y,h,mi,s,GetName(playerid),GetIPEx(playerid),VehicleNames[vehicle],FormatNumber(money));
	new File:hFile;
	hFile = fopen("logs/sell.log", io_append);
	fwrite(hFile, entry);
	fclose(hFile);
}

