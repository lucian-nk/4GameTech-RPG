	if ( GetPVarInt( playerid, "AccountLocked" ) == 1 )
	{
		if(PlayerInfo[playerid][pLimba] == 1) // Engleza
		{
			SCM( playerid, COLOR_WHITE, "Your account is locked, use /unlockacc to unlock it." );
		}
		if(PlayerInfo[playerid][pLimba] == 2) // Romana
		{
			SCM( playerid, COLOR_WHITE, "Contul tau este blocat, deblocheaza-l folosind comanda /unlockacc." );
		}
	}
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	new tstr[32];
	format(tstr, sizeof(tstr), "{FFFFFF}SHOP (Tu ai {F7F416}%s GOLD{F7F416})", FormatNumber(PlayerInfo[playerid][pCoins]));
	ShowPlayerDialog(playerid, DIALOG_SHOP, DIALOG_STYLE_LIST, tstr, "1. Items\n2. Clan Items","SELECT", "EXIT");
	return 1;
}
CMD:charity(playerid, params[])
{
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "Nu esti logat!");
	if(IsPlayerConnected(playerid))
	{
		new moneycharity,string[100],sendername[30];
		if(sscanf(params, "i", moneycharity)) return SendClientMessage(playerid, 0xFFFFFFFF,"Tasteaza: /charity <Suma>");
		if(GetPlayerCash(playerid) < moneycharity) return SendClientMessage(playerid, 0xFFFFFFFF,"{FFB870}You don't have that Suma.");
		if(moneycharity < 1 || moneycharity > 1000000) return SendClientMessage(playerid,COLOR_WHITE,"{B8DBFF}Donation must be between $1 and $1.000.000.");
		GivePlayerCash(playerid, -moneycharity);
		CharityMoney +=	moneycharity;
		format(string, sizeof(string), "{52CC52}Thank you for your donation of %s$.", FormatNumber(moneycharity));
		SCM(playerid,COLOR_WHITE,string);
		GetPlayerName(playerid,sendername,sizeof(sendername));
		format(string, sizeof(string), "{52CC52}%s donated %s$ for players with level of between 2-8.", sendername,FormatNumber(moneycharity));
		SendFactionMessage(3, COLOR_YELLOW, string);
		new query[128];
		format(query,128,"UPDATE `charity` SET `CharityMoney`='%d' WHERE `ID`='1'",CharityMoney);
		mysql_query(SQL,query);
		Update(playerid, pCashx);
	}
	return 1;
}
CMD:settax(playerid, params[])
{
	new taxx;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pLeader] != 3)  return SendClientMessage(playerid, COLOR_ERROR, "You need to be mayor.");
	if(sscanf(params,"i", taxx)) return SendClientMessage(playerid, COLOR_SYN,"Synthax:{FFFFFF} /settax <amount>");
	{
	    if(MoneyACheck(playerid, taxx, 1, 1000)) return 1;
		format(gString, sizeof(gString), "Tax is now $%s.", FormatNumber(taxx));
		SendClientMessage(playerid,COLOR_SYN2,gString);
		Tax = taxx;
		gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery),"UPDATE `others` SET `Tax`=%d WHERE `ID`='1'", Tax);
		mysql_query(handle, gQuery);
	}
	return 1;
}

