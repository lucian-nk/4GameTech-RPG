				TogglePlayerControllableEx(playerid, 0);
			    SetPlayerPos(playerid,1663.3900,-2240.8093,6.9004);
			    SetPlayerFacingAngle(playerid, 177.2038);
			    SetPlayerCameraPos(playerid,1665.7689,-2246.3420,5.2622);
			 	SetPlayerCameraLookAt(playerid,1663.3900,-2240.8093,6.9004);
