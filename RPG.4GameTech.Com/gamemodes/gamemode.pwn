			PlayerInfo[giveplayerid][pRank] = 0;
			Update(giveplayerid,pRankx);
			PlayerInfo[giveplayerid][pSkin] = 60;
			Update(giveplayerid, pSkinx);
			PlayerInfo[giveplayerid][pFPunish] = 5;
			Update(giveplayerid, pFPunishx);
