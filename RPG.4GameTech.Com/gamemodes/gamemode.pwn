CMD:arrest(playerid, params[])
{
	new giveplayerid, splayer[25], rankname[64], money;
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "You're not part of Police Department / FBI.");
	if(OnDuty[playerid] == 0)return SendClientMessage(playerid, COLOR_ERROR, "You are not on duty.");
	if(!IsPlayerInRangeOfPoint(playerid, 5.0, 1560.2808, -1694.7703, 5.8970) && !IsPlayerInRangeOfPoint(playerid, 5.0, 1730.8948, -1590.3451, 601.0000)) return SendClientMessage(playerid, COLOR_ERROR, "You're not at arrest place.");
	if(sscanf(params,"s[25]",splayer)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /arrest <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[giveplayerid][pWanted] == 0) return SendClientMessage(playerid, COLOR_ERROR, "This player don't have wanted.");
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");

		if(PlayerInfo[playerid][pMember] == 1 || PlayerInfo[playerid][pLeader] == 1)
		{
			if(PlayerInfo[playerid][pRank] == 7) { rankname = "Chief"; }
			else if(PlayerInfo[playerid][pRank] == 6) { rankname = "Assistant Chief"; }
			else if(PlayerInfo[playerid][pRank] == 5) { rankname = "Captain"; }
			else if(PlayerInfo[playerid][pRank] == 4) { rankname = "Lieutenant"; }
			else if(PlayerInfo[playerid][pRank] == 3) { rankname = "Sergeant"; }
			else if(PlayerInfo[playerid][pRank] == 2) { rankname = "Detective"; }
			else if(PlayerInfo[playerid][pRank] == 1) { rankname = "Officer"; }
		}
		else if(PlayerInfo[playerid][pMember] == 2 || PlayerInfo[playerid][pLeader] == 2)
		{
			if(PlayerInfo[playerid][pRank] == 7) { rankname = "Director"; }
			else if(PlayerInfo[playerid][pRank] == 6) { rankname = "Assistant Director"; }
			else if(PlayerInfo[playerid][pRank] == 5) { rankname = "Section Chief"; }
			else if(PlayerInfo[playerid][pRank] == 4) { rankname = "Special Agent in Charge"; }
			else if(PlayerInfo[playerid][pRank] == 3) { rankname = "Supervisory Special Agent"; }
			else if(PlayerInfo[playerid][pRank] == 2) { rankname = "Special Agent"; }
			else if(PlayerInfo[playerid][pRank] == 1) { rankname = "Professional Staff"; }
		}

		if(PlayerInfo[giveplayerid][pWanted] == 1)
		{
			PlayerInfo[giveplayerid][pJailed] = 1;
			PlayerInfo[giveplayerid][pJailTime] = 150;
			PlayerInfo[giveplayerid][pBail] = 100000;
			money = 2000;
		}
		else if(PlayerInfo[giveplayerid][pWanted] == 2)
		{
			PlayerInfo[giveplayerid][pJailed] = 1;
			PlayerInfo[giveplayerid][pJailTime] = 300;
			PlayerInfo[giveplayerid][pBail] = 200000;
			money = 4000;
		}
		else if(PlayerInfo[giveplayerid][pWanted] == 3)
		{
			PlayerInfo[giveplayerid][pJailed] = 1;
			PlayerInfo[giveplayerid][pJailTime] = 450;
			PlayerInfo[giveplayerid][pBail] = 300000;
			money = 6000;
		}
		else if(PlayerInfo[giveplayerid][pWanted] == 4)
		{
			PlayerInfo[giveplayerid][pJailed] = 1;
			PlayerInfo[giveplayerid][pJailTime] = 600;
			PlayerInfo[giveplayerid][pBail] = 400000;
			money = 8000;
		}
		else if(PlayerInfo[giveplayerid][pWanted] == 5)
		{
			PlayerInfo[giveplayerid][pJailed] = 1;
			PlayerInfo[giveplayerid][pJailTime] = 750;
			PlayerInfo[giveplayerid][pBail] = 500000;
			money = 10000;
		}
		else if(PlayerInfo[giveplayerid][pWanted] == 6)
		{
			PlayerInfo[giveplayerid][pJailed] = 1;
			PlayerInfo[giveplayerid][pJailTime] = 1800;
			PlayerInfo[giveplayerid][pBail] = 600000;
			money = 12000;
		}

		Update(giveplayerid, pJailedx);
		Update(giveplayerid, pJailTimex);
		Update(giveplayerid, pBailx);

		GivePlayerCash(playerid,money);
		GivePlayerCash(giveplayerid,-money);

		SetPlayerInterior(giveplayerid, 1);
		new randp = random(sizeof(JailSpawn));
		SetPlayerPos(giveplayerid, JailSpawn[randp][0], JailSpawn[randp][1], JailSpawn[randp][2]);
		
		TogglePlayerControllableEx(giveplayerid, 0);
		ResetPlayerWeapons(giveplayerid);
		SetTimerEx("unfr", 1500, false, "i", giveplayerid);
		TextDrawShowForPlayer(giveplayerid, JailTime[giveplayerid]);

		for(new i;i<10;i++) RemovePlayerAttachedObject(giveplayerid,i);
		SetPlayerSpecialAction(giveplayerid,SPECIAL_ACTION_NONE);

		PlayerInfo[giveplayerid][pWanted] = 0;
		Update(giveplayerid, pWantedx);
		SetPlayerWantedLevel(giveplayerid,PlayerInfo[giveplayerid][pWanted]);

        PlayerInfo[giveplayerid][pArrests] ++;
	    Update(giveplayerid, pArrestsx);
	    ClearCrimes(giveplayerid);
		format(gString, sizeof(gString), "<< %s %s arrested suspect %s for %d seconds ($%s). Bail: $%s >>", rankname, GetName(playerid), GetName(giveplayerid), PlayerInfo[giveplayerid][pJailTime], FormatNumber(money), FormatNumber(PlayerInfo[giveplayerid][pBail]));
		SendClientMessageToAll(COLOR_LIGHTRED, gString);
		GiveAchievement(giveplayerid, 15, "Get arrested", 100);
		
		PlayerInfo[giveplayerid][pWantedLost] = 1800;
		Update(giveplayerid, pWantedLostx);
		TextDrawHideForPlayer(giveplayerid, WantedTime[giveplayerid]);
	}
	return 1;
}

