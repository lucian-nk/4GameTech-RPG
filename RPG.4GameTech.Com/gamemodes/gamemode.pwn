function DrawBetsixNumber() {
	new 
		number = 1 + random (49), str[3], bool: ok, win,
		string[144];

	Betsix_Current[Betsix_Draw] = number;

	if (Betsix_Draw > 0) {
		ok = HasBeenDrawn(number);
		while (ok) {
			number = 1 + random (49);
			Betsix_Current[Betsix_Draw] = number;
			ok = HasBeenDrawn(number);
		}
	}

	Betsix_Draw ++;

	format(str, sizeof str, "%d", number);
	TextDrawSetString(TD_Betsix[1], str);

	TextDrawColor(TD_Betsix[0], Betsix_Colors[random (sizeof Betsix_Colors)]);

	foreach (new i : Player) {
		if (!Betting[i])
			continue;
	
		TextDrawShowForPlayer(i, TD_Betsix[0]);
		TextDrawShowForPlayer(i, TD_Betsix[1]);

		if (BetsixWon[i] == 6)
			continue;

		for (new j = 0; j < 6; j++) {
			if (BetsixNumbers[i][j] == number) {
				BetsixNumbers[i][j] = 99;
				BetsixWon[i] ++;
				if (BetsixWon[i] == 6) {
					win = BetsixBet[i] * Betsix_Odds[Betsix_Draw - 1];
					GivePlayerCash(i, win);
					
					foreach (new k : Player) {
						if (!Betting[k])
							continue;
						
						format(string, sizeof(string), "[BetSix]: %s bet $%s and he won $%s.", GetName(i), FormatNumber(BetsixBet[i]), FormatNumber(win));
						SendClientMessage(k, 0x7CA3D7AA, string);
					}
					break;
				}
			}
		}  
	}

	if (Betsix_Draw < 35)
		SetTimer("DrawBetsixNumber", 5000, false);

	else if (Betsix_Draw == 35) {
		foreach (new i : Player) {
			if (!Betting[i])
				continue;
		
			TextDrawHideForPlayer(i, TD_Betsix[0]);
			TextDrawHideForPlayer(i, TD_Betsix[1]);

			if (BetsixNumbers[i][0] > 0)
				format(string, sizeof(string), "[BetSix]: Round #%d finished. You guessed %d numbers. The next round will start in 3 minutes.", Betsix_Round + 1, BetsixWon[i]);
			
			else
				format(string, sizeof(string), "[BetSix]: Round #%d finished. The next round will start in 3 minutes.", Betsix_Round + 1);
				
			SendClientMessage(i, 0xC596DDFF, string);
		
			for (new j = 0; j < 6; j++)
				BetsixNumbers[i][j] = 0;
			
			BetsixBet[i] = 0;
			BetsixWon[i] = 0;
		}
		Betsix_Draw = 0;
		Betsix_Time = 180;
		Betsix_Started = false;
		Betsix_Round ++;
	}
}
