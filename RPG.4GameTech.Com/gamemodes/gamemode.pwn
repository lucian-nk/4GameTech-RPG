			Kick(giveplayerid);
			return 1;
		}
		format(gString, sizeof(gString), "%s was warned by admin %s, reason: %s.", GetName(giveplayerid), GetName(playerid), reason);
		SendClientMessageToAll(COLOR_LIGHTRED, gString);
	}
	return 1;
}

CMD:offwarn(playerid, params[])
{
	new giveplayer[MAX_PLAYER_NAME], reason[64];
	if(gLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Nu esti autentificat");
	if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessage(playerid, COLOR_ERROR, "Your admin level is not high enough to use this command.");
	if(sscanf(params,"s[24]s[64]",giveplayer,reason)) return SendClientMessage(playerid, COLOR_SYN, "Synthax:{FFFFFF} /offwarn <Player Name> <reason>");
	{
	    if(GetPlayerID(giveplayer) != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_ERROR, "Player is online.");
        gQuery[0] = (EOS);
		mysql_format(handle, gQuery, sizeof(gQuery), "SELECT `AdminLevel`, `Warns` FROM `players` WHERE `Name` = '%e' OR `AName` = '%e' LIMIT 1", giveplayer, giveplayer);
		new Cache:result = mysql_query(handle, gQuery);
		new tmp[25], warns, admin;
		if(!cache_num_rows())
		{
		    cache_delete(result);
		    return SendClientMessage(playerid, COLOR_ERROR, "Invalid Player Name.");
		}
		cache_get_field_content(0, "AdminLevel", tmp), admin = strval(tmp);
		cache_get_field_content(0, "Warns", tmp), warns = strval(tmp);
		cache_delete(result);
		if(admin > PlayerInfo[playerid][pAdmin])  return SendClientMessage(playerid, COLOR_ERROR, "You can't do this.");

		warns ++;
		gQuery[0] = (EOS);
	    mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Warns` = %d WHERE `Name` = '%e' OR `AName` = '%e'", warns, giveplayer, giveplayer);
		mysql_query(handle, gQuery);
		if(warns >= 3)
		{
		    new year, month, day, hour, mins, sec, playerip[16];
   			getdate(year, month, day);
			gettime(hour, mins, sec);

			new nyear, nmonth, nday;
			new days = 3;

			CalcDate(nyear, nmonth, nday, days);
			//

			format(playerip, 64, "0.0.0.0");

			gQuery[0] = (EOS);
			mysql_format(handle, gQuery, sizeof(gQuery) ,"INSERT INTO `bans` (`Name`,`IP`,`Admin`,`Reason`,`BanY`,`BanM`,`BanD`,`BanH`,`UnbanY`,`UnbanM`,`UnbanD`,`UnbanH`,`Days`) VALUES ('%e','%e','%e','%e',%d,%d,%d,%d,%d,%d,%d,%d,%d)",
			giveplayer,
			playerip,
			"AdmBot",
			"3 warnings",
			year,
			month,
			day,
			hour,
			nyear,
			nmonth,
			nday,
			hour,
			days);
			mysql_query(handle ,gQuery);
			
			gQuery[0] = (EOS);
		    mysql_format(handle, gQuery, sizeof(gQuery), "UPDATE `players` SET `Warns` = '0' WHERE `Name` = '%e' OR `AName` = '%e'", giveplayer, giveplayer);
			mysql_query(handle, gQuery);

			format(gString, sizeof(gString), "%s was offline banned by AdmBot for %d days, reason: 3 warnings.",giveplayer, days);
			SendClientMessageToAll(COLOR_LIGHTRED, gString);
			return 1;
		}
		format(gString, sizeof(gString), "%s was offline warned by admin %s, reason: %s.", giveplayer, GetName(playerid), reason);
		SendClientMessageToAll(COLOR_LIGHTRED, gString);
	}
	return 1;
}

